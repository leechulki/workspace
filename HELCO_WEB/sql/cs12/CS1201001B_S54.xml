<?xml version="1.0" encoding="euc-kr"?>
<query>
	<type>select</type>
	<description><![CDATA[]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
<![CDATA[
SELECT CS128_RAT AS RAT
  FROM SAPHEE.ZCST128  
 WHERE MANDT = #G_MANDT# 
   AND CS128_GNO = #GNO# 
]]></statement>
	<input default-name="ds_cond_rate">
		<col name="G_MANDT" size="255" type="VARCHAR" description="" /> 
		<col name="GNO" size="255" type="VARCHAR" description="" /> 
	</input>
	<output default-name="ds_rate">
	</output>
</query>
