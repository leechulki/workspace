       SAPHEE.ZCST126
   SET
       CS126_RM2 = #RM2#
      ,CS126_CYN = #CYN#
      ,CS126_CTP = #CTP#
      ,CS126_CFG = #CFG#
      ,CS126_HEP = #HEP#
 WHERE
       MANDT = #G_MANDT#
   AND CS126_UPN = #UPN#
   AND CS126_CST = #CST#
   AND CS126_GNO = #GNO#
   AND CS126_DDT = ''
   AND CS126_PST = 'A6'	]]></statement>
