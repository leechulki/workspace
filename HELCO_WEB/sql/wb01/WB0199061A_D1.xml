<?xml version="1.0" encoding="euc-kr"?>
<query dynamic="true">
	<type>delete</type>
	<description><![CDATA[]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>0</result-count>
	<statement>
	DELETE FROM SAPHEE.ZCMFL002
	WHERE MANDT = #G_MANDT#
    AND GBN = #GBN#
    AND PSPID = #PSPID#
    AND POSID = #POSID#
	<isNotNull col="FNAME">
    AND FNAME = #FNAME#
	</isNotNull>
	</statement>
	<input default-name="ds_list2_tmp">
	</input>
	<output default-name="_none">
	</output>
</query>
