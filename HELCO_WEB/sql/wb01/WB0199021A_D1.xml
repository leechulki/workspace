<?xml version="1.0" encoding="euc-kr"?>
<query>
	<type>delete</type>
	<description><![CDATA[사용자 정보 삭제]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
<![CDATA[
DELETE FROM SAPHEE.ZUSERF
WHERE MANDT = #MANDT#
AND USERNUMB = #USERNUMB#	]]></statement>
	<input default-name="ds_list">
		<col name="USERNUMB" size="30" type="VARCHAR" description="" /> 
		<col name="MANDT" size="30" type="VARCHAR" description="" /> 
	</input>
	<output default-name="_none">
	</output>
</query>
