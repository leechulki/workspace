<?xml version="1.0" encoding="euc-kr"?>
<query dynamic="true">
	<type>select</type>
	<description><![CDATA[]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
		
SELECT
        GNUMB AS USERNUMB
  FROM SAPHEE.ZWBGRUP 
  WHERE 
        MANDT = #G_MANDT#
    AND GNUMB = #G_USER_ID#
    AND GGRUP = #GROUPNM# 

WITH UR
	</statement>
	<input default-name="none">
	</input>
	<output default-name="ds_list">
	</output>
</query>
