<?xml version="1.0" encoding="euc-kr"?>
<query>
	<type>delete</type>
	<description><![CDATA[프로그램 권한 정보 삭제]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
<![CDATA[
DELETE FROM SAPHEE.ZWBAUTH
WHERE
	MANDT = #MANDT#
	AND NUMB = #NUMB#
	AND PGCD = #PGCD#
	]]></statement>
	<input default-name="ds_pgmList">
	</input>
	<output default-name="_none">
	</output>
</query>
