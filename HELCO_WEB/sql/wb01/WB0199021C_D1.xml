<?xml version="1.0" encoding="euc-kr"?>
<query>
	<type>delete</type>
	<description><![CDATA[그룹정보 삭제]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
<![CDATA[
DELETE FROM SAPHEE.ZWBGRUP 
WHERE 
     MANDT = #MANDT#
AND GNUMB = #GNUMB#
AND GGRUP = #GGRUP#	]]></statement>
	<input default-name="ds_groupList">
		<col name="MANDT" size="255" type="VARCHAR" description="" /> 
		<col name="GNUMB" size="255" type="VARCHAR" description="" /> 
		<col name="GGRUP" size="255" type="VARCHAR" description="" /> 
	</input>
	<output default-name="_none">
	</output>
</query>
