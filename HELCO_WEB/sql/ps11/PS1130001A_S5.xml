<?xml version="1.0" encoding="euc-kr"?>
<query dynamic="true">
    <type>select</type>
    <description><![CDATA[중복체크 쿼리 사양]]></description>
    <reload>true</reload>
    <monitoring>true</monitoring>
    <result-count>0</result-count>
    <statement>
SELECT COUNT(SUVPRH) AS CNT
  FROM SAPHEE.ZPST1132
 WHERE MANDT = #G_MANDT#
   AND SUVPRH = #CODE1# 
   AND SUVPRD = #CODE2#
  
    </statement>
    <input default-name="ds_cntCond">
    </input>
    <output default-name="ds_cnt">
    </output>
</query>