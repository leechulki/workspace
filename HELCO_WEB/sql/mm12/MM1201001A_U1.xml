	<statement>
<![CDATA[
UPDATE 
 	SAPHEE.ZMMT997
SET 

     RATE = #RATE#
   , SPEC = #SPEC#
   , EMPLOYEE = #EMPLOYEE#
   , WORKER = #WORKER#  
   , EQUIPMENT = #EQUIPMENT#
   , CAPA_CURR = #CAPA_CURR#
   , CAPA_MAX = #CAPA_MAX#  
   , CAPA_UNIT = #CAPA_UNIT#
   , GRADE = #GRADE#
   , TRADE_YEAR = #TRADE_YEAR#
   , REGION = #REGION#  
   , DISTANCE = #DISTANCE#
   , JQPR_COUNT = #JQPR_COUNT#
   , JQPR_PROHI = #JQPR_PROHI#  
   , JQPR_COUNT_1 = #JQPR_COUNT_1#
   , JQPR_PROHI_1 = #JQPR_PROHI_1#
WHERE 
	 MANDT = #G_MANDT#        
	 AND ZCODE = #ZCODE#
	 AND ITEM_CODE = #ITEM_CODE#    
	 AND LIFNR = #LIFNR#           
	 	]]></statement>
