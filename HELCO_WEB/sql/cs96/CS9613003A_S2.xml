	<result-count>0</result-count>
	<statement>
<![CDATA[
	SELECT CODE2, 
	   	   CTEXT2 
	  FROM SAPHEE.ZLCODE 
	 WHERE MANDT = #G_MANDT#
	       AND CODE1 = 'CS200' 
	       AND LANG = #G_LANG#
	 ORDER BY CODE2
	  WITH UR
	]]></statement>
