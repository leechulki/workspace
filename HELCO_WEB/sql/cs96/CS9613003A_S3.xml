<?xml version="1.0" encoding="euc-kr"?>
<query>
	<type>select</type>
	<description><![CDATA[<!-- 지정연도의 행수 Retrieve -->]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>0</result-count>
	<statement>
<![CDATA[
	SELECT COUNT(*) AS C607
	  FROM SAPHEE.ZCST607
	 WHERE MANDT = #G_MANDT#
	   AND CS607_YEAR = #ZYEAR#
	   AND CS607_TYP = #TYPE#
	  WITH UR
	]]></statement>
	<input default-name="ds_cond">
	</input>
	<output default-name="ds_cnt">
	</output>
</query>
