<?xml version="1.0" encoding="euc-kr"?>
<query>
	<type>select</type>
	<description><![CDATA[<!-- 연도 정보 RETRIEVE -->]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>0</result-count>
	<statement>
<![CDATA[
	SELECT DISTINCT CS607_YEAR AS ZYEAR 
	  FROM SAPHEE.ZCST607
	 WHERE MANDT = #G_MANDT#   
	 ORDER BY CS607_YEAR DESC
	  WITH UR
	]]></statement>
	<input default-name="_none">
	</input>
	<output default-name="ds_year">
	</output>
</query>
