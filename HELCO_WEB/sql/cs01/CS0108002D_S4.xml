<?xml version="1.0" encoding="euc-kr"?>
<query dynamic="true">
	<type>select</type>
	<description><![CDATA[템플릿 마스터 맥스키 조회]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
		SELECT (VALUE(INT(MAX(NUM)),0) + 1) AS NUM FROM SAPHEE.ZCSTPLMS WHERE MANDT = #G_MANDT#
	</statement>
	<input default-name="_none">
	</input>
	<output default-name="ds_max_num">
	</output>
</query>
