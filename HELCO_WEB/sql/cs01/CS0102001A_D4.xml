<?xml version="1.0" encoding="euc-kr"?>
<query dynamic="true">
	<type>delete</type>
	<description><![CDATA[자재청구 취소시 MMPR정보 삭제]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>0</result-count>
	<statement>
DELETE FROM SAPHEE.ZQMT007
WHERE MANDT = #G_MANDT#
<isNotNull col="JQPRNUM">
   AND JQPRNUM  = #JQPRNUM#
</isNotNull>
<isNull col="JQPRNUM">
   AND JQPRNUM  = ( SELECT DISTINCT JQPRNUM  FROM SAPHEE.ZMMT031
                            WHERE MANDT= #G_MANDT#
                                AND REQNO = #REQNO#
                          )
</isNull>
   AND RECE_F = '0'	</statement>
	<input default-name="ds_mm031_1">
	</input>
	<output default-name="_none">
	</output>
</query>
