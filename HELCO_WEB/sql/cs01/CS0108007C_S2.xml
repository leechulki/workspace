<?xml version="1.0" encoding="euc-kr"?>
<query dynamic="true">
    <type>select</type>
    <description><![CDATA[연삭식 특성코드존재여부]]></description>
    <reload>true</reload>
    <monitoring>true</monitoring>
    <result-count>1</result-count>
    <statement>
        SELECT SPEC
        FROM   SAPHEE.ZCSTMATCHID
        WHERE  MANDT = #G_MANDT#
          AND  SPEC  = #SPEC#
    </statement>
    <input default-name="ds_spec_cnt">
    </input>
    <output default-name="ds_spec_cnt">
    </output>
</query>
