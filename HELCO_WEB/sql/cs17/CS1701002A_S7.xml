<query>
	<type>select</type>
	<description><![CDATA[]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
<![CDATA[
SELECT 
    COUNT(*) AS COUNT 
FROM 
    SAPHEE.ZCST131 
WHERE
    MANDT = #G_MANDT# 
    AND CS131_PJT = #PJT# 
    AND CS131_HNO = #HNO#
  WITH UR	]]></statement>
	<input default-name="ds_hno">
	</input>
	<output default-name="ds_cnt">
	</output>
</query>
