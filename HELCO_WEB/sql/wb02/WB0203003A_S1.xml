	            ELSE 0 END AS GISEONG
 FROM EVLADM.TBECSHF1
 LEFT OUTER JOIN EVLADM.TBERCKF1 ON CECSHPJT = CERCKPJT
 WHERE 
	 CECSHYYY = #YYY#
	 AND CECSHPJT LIKE #PJT# || '%'	]]></statement>
