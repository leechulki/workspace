	<statement>
<![CDATA[
INSERT INTO SAPHEE.ZMMT133 (
          MANDT
         ,POSID
         ,MATNR
         ,LIFNR
         ,QTNUM
         ,POST1
         ,ZEINR
         ,ZSPEC
         ,RQDAT
         ,REDAT
         ,CPDAT
         ,BIGO
         ,COMPL
         ,C_DZEINR
         ,SNAME
         ,SDATE
         ,STIME
         ,NAME1
         ,P_ZEINR
         ,ZKIND
         ,NETPR_IF

)        VALUES (
          #G_MANDT#
        , #POSID#
        , #MATNR#
        , #LIFNR#
        , NEXTVAL FOR SAPHEE.SQ_ZMMT133_SEQ
        , #POST1#
        , #ZEINR#
        , #ZSPEC#
        , #RQDAT#
        , #REDAT#
        , #CPDAT#
        , #BIGO#
        , '1'
        , #C_DZEINR#
        , #G_USER_ID#
        , HEX(CURRENT DATE)
        , HEX(CURRENT TIME)
        , #NAME1#
        , #P_ZEINR#
        , #ZKIND#
        , #NETPR_IF#

)	]]></statement>
