<?xml version="1.0" encoding="euc-kr"?>
<query>
	<type>delete</type>
	<description><![CDATA[영업사양 저장 삭제]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>0</result-count>
	<statement>
<![CDATA[
DELETE FROM SAPHEE.ZSDT0062
WHERE 
	 MANDT = #G_MANDT#
	 AND KUNNR = #KUNNR#
	 AND INELN = #INELN#
	 AND INSEQ = #INSEQ#
	 AND POSNR = #POSNR#
	 AND CHARAC = #CODE2#
	 AND ZGUBUN = #ZGUBUN#	]]></statement>
	<input default-name="ds_list">
		<col name="G_MANDT" size="255" type="VARCHAR" description="" /> 
		<col name="KUNNR" size="255" type="VARCHAR" description="" /> 
		<col name="INELN" size="255" type="VARCHAR" description="" /> 
		<col name="INSEQ" size="255" type="VARCHAR" description="" /> 
		<col name="POSNR" size="255" type="VARCHAR" description="" /> 
		<col name="CHARAC" size="255" type="VARCHAR" description="" /> 
		<col name="GBN" size="255" type="VARCHAR" description="" /> 
	</input>
	<output default-name="_none">
	</output>
</query>
