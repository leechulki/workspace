<?xml version="1.0" encoding="euc-kr"?>
<query>
	<type>delete</type>
	<description><![CDATA[견적의뢰 등록 DELETE]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>0</result-count>
	<statement>
<![CDATA[
DELETE FROM SAPHEE.ZSDT0061
WHERE 
	 MANDT = #G_MANDT#
	 AND KUNNR = #G_VEND_CODE#
	 AND INELN = #INELN#
	 AND INSEQ = #INSEQ#
	 AND POSNR = #POSNR#	]]></statement>
	<input default-name="ds_list">
		<col name="MANDT" size="9" type="VARCHAR" description="" /> 
		<col name="KUNNR" size="30" type="VARCHAR" description="" /> 
		<col name="INELN" size="33" type="VARCHAR" description="" /> 
		<col name="INSEQ" size="9" type="VARCHAR" description="" /> 
		<col name="POSNR" size="18" type="VARCHAR" description="" /> 
	</input>
	<output default-name="_none">
	</output>
</query>
