<?xml version="1.0" encoding="euc-kr"?>
<query>
	<type>select</type>
	<description><![CDATA[]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>
	<statement>
<![CDATA[
SELECT HEX(CURRENT DATE) AS CURR_DATE
      ,HEX(CURRENT TIME) AS CURR_TIME
FROM SYSIBM.SYSDUMMY1

FOR FETCH ONLY
WITH UR	]]></statement>
	<input default-name="_none">
	</input>
	<output default-name="ds_dates">
		<col name="CURR_DATE" size="8" type="CHAR" description="" /> 
		<col name="CURR_TIME" size="6" type="CHAR" description="" /> 
	</output>
</query>
