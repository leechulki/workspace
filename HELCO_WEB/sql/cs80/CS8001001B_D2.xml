<?xml version="1.0" encoding="euc-kr"?>
<query dynamic='true'>
	<type>delete</type>
	<description><![CDATA[견적 detail 삭제]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>0</result-count>
	<statement>

	DELETE FROM SAPHEE.ZCST002G
	      WHERE MANDT = #G_MANDT#
	        AND QTNUM = #QTNUM#
	        AND SEQ = #SEQ#
	        <isNotNull col="SEQNO">
			AND SEQNO = #SEQNO#
			</isNotNull>
	        
	</statement>
	<input default-name="ds_cond">
	</input>
	<output default-name="_none">
	</output>
</query>
