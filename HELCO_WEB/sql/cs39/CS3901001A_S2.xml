<?xml version="1.0" encoding="euc-kr"?>
<query dynamic="true">
	<type>select</type>
	<description><![CDATA[사업계획 편성연도 오픈여부]]></description>
	<reload>true</reload>
	<monitoring>true</monitoring>
	<result-count>1</result-count>	
	<statement>	
	
		SELECT ZYEAR AS DYEAR
		FROM SAPHEE.ZCST611
		WHERE ZYEAR = #DYEAR#
		AND VALUE(CS611_CLO, '') = 'Y'
		 
	</statement>	
	<input default-name="ds_year">
	</input>
	<output default-name="ds_year">	
	</output>
	</query>
