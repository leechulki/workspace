var fn_query_chk = false; //중복조회 방지용 flag

function fn_query() {

	var arr = ['견적작성기간', '12'];
	if(gfn_getDayCnt(ds_days, cal_qtdat_fm.Value, cal_qtdat_to.Value) > 365) {
		gfn_showAlert("CW10087", arr);
		cal_qtdat_fm.SetFocus();
		return false;
	}

	if(!fn_query_chk) {
		fn_query_chk = true;
	} else {
		gfn_showAlert("CW10025", "조회중입니다..");
		return;
	}

	ds_cond.DeleteAll();
	ds_cond.AddRow();

	ds_cond.setColumn(0, "MANDT"     , G_MANDT                 );
	ds_cond.SetColumn(0, "FR_QTDAT"  , cal_qtdat_fm.Value      );
	ds_cond.SetColumn(0, "TO_QTDAT"  , cal_qtdat_to.Value      );
	ds_cond.SetColumn(0, "FR_ZESTDAT", cal_soym_fm.Value       );
	ds_cond.SetColumn(0, "TO_ZESTDAT", cal_soym_to.Value       );
	ds_cond.SetColumn(0, "VKBUR"     , div_vbvg.cb_frvb.Value  );
	ds_cond.SetColumn(0, "VKGRP"     , div_vbvg.cb_frvg.Value  );
	ds_cond.SetColumn(0, "ZPRGFLG"   , cb_zpstat.Value         );
	ds_cond.SetColumn(0, "SMAN"      , div_sman.ed_sman_cd.Text);
	ds_cond.SetColumn(0, "QTNUM"     , ed_qtnum.value          );
	ds_cond.SetColumn(0, "GSNAM"     , ed_gsnam.value          );
	// 운송지역 조회조건 추가
	ds_cond.SetColumn(0, "REGION"	 , cb_zregn3.Value		   );
//	trace(ds_cond.SaveXml());
    // 승강기번호 추가
    ds_cond.SetColumn(0, "EL_DELVNO" , ed_el_delvno.value);
    
    // 최초견적일 기간 조회조건 추가
    ds_cond.SetColumn(0, "FR_FCDATE"     , cal_fcdate_fm.value);
    ds_cond.SetColumn(0, "TO_FCDATE"     , cal_fcdate_to.value);
    
