/********************************************************************************
* 기      능   : 견적승인접수
********************************************************************************/
function btn_receipt_OnClick(obj)
{
	// 그리드 저장 대상 로우
	if(ds_header.FindRow("CHECK", 1)==-1){
		gfn_showMsg('CW00046');
		return;
	}
	
	ds_header.Filter("CHECK==1");
	ds_header2.copyF(ds_header);
	ds_header.unFilter();

	tit_clearActionInfo();
	
	trace(ds_header2.SaveXML());
	
	// 서비스 호출 
	ds_error.ClearData(); 
	fn_createWait("",this);     // 처리중입니다. 메시지 처리
	nsfRequest("receipt"																	// srvId
				, "ses0080/receipt"															// url
				, "ds_list=ds_header2"														// inDsList
				, "ds_error=ds_error"												    	// outDsList
				, ""																		// strParam
				, "fn_afterReceipt"															// CallBackFunc
				);
	
}

/********************************************************************************
* 기      능   : 저장 Callback  
********************************************************************************/
function fn_afterReceipt(errCode, errMsg) {
	fn_destroyWait();

	ds_header2.ClearData(); 
	
	if ( errCode != 0 ) {
		gfn_showMsg("CE00001");
	} else {
		gfn_showMsg("CI00003");		
	}
	
	fn_query();
}

/********************************************************************************
* 기      능   : 견적승인접수취소
