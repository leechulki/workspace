/********************************************************************************
* 기      능   : 조회 1
********************************************************************************/
function fn_query() {

	ds_cond.DeleteAll();
	ds_cond.AddRow();

	ds_cond.SetColumn(0, "FR_QTDAT"  , cal_qtdat_fm.Value      );
	ds_cond.SetColumn(0, "TO_QTDAT"  , cal_qtdat_to.Value      );
	ds_cond.SetColumn(0, "FR_ZESTDAT", cal_soym_fm.Value       );
	ds_cond.SetColumn(0, "TO_ZESTDAT", cal_soym_to.Value       );
	ds_cond.SetColumn(0, "VKBUR"     , div_vbvg.cb_frvb.Value  );
	ds_cond.SetColumn(0, "VKGRP"     , div_vbvg.cb_frvg.Value  );
	ds_cond.SetColumn(0, "ZPRGFLG"   , cb_zpstat.Value         );
	ds_cond.SetColumn(0, "SMAN"      , div_sman.ed_sman_cd.Text);

	nsfRequest("header", "ses0080/header", "ds_cond=ds_cond ds_header=ds_header", "ds_header=ds_header ds_error=ds_error", "", "fn_afterQuery");
    fn_createWait("",this);     // 처리중입니다. 메시지 처리
}

/********************************************************************************
* 기      능   : 조회 2
********************************************************************************/
function fn_detail() {

	ds_cond.SetColumn(0, "QTNUM", grd_header.GetCellText("Body", grd_header.CurrentRow, 2));
	ds_cond.SetColumn(0, "QTSER", grd_header.GetCellText("Body", grd_header.CurrentRow, 3));

	nsfRequest("detail", "ses0080/detail", "ds_cond=ds_cond ds_detail=ds_detail", "ds_detail=ds_detail ds_error=ds_error", "", "fn_afterQuery");
}


