// Form에서 사용될 전역변수를 선언
// FORM에서 사용되는 전역변수는 대문자로 작성하며, F_XXX 의 형태를 갖도록 한다. 
//=========================================================================================

function fn_OnLoadCompleted(obj) {
	// Form 로드 이벤트 맨 첫줄에 기술 
	gfn_initForm(true);

	fn_init_form(); 	// 폼 초기화
	cal_qtdat_fm.Value = left(gfn_getCurrDate(),6)+"01";
	

}

/********************************************************************************
* 기      능   : form UnloadCompleted 
********************************************************************************/
function fn_OnUnloadCompleted(obj) {
	return gfn_isFormUnload(obj);
}


//=========================================================================================
// [ PART 3 ]
// Main Process Event 정의(조회,입력,삭제,저장,인쇄,form닫기)
//=========================================================================================
/********************************************************************************
* 기      능   : 조회
********************************************************************************/
var fn_query_chk = false; //중복조회 방지용 flag

function fn_query() {

	var arr = ['견적기간', '12'];
	if(gfn_getDayCnt(ds_days, cal_qtdat_fm.Value, cal_qtdat_to.Value) > 365) {
		gfn_showAlert("CW10087", arr);
		cal_qtdat_fm.SetFocus();
		return false;
	}

	if(!fn_query_chk) {
		fn_query_chk = true;
	} else {
		gfn_showAlert("CW10025", "조회중입니다..");
		return;
	}
	ds_cond.DeleteAll();
	ds_cond.AddRow();
	ds_cond.setColumn(0, "MANDT"     , G_MANDT                 );
	ds_cond.SetColumn(0, "FR_QTDAT"  , cal_qtdat_fm.Value      );
	ds_cond.SetColumn(0, "TO_QTDAT"  , cal_qtdat_to.Value      );
	ds_cond.SetColumn(0, "VKBUR"     , div_vbvg.cb_frvb.Value  );
	ds_cond.SetColumn(0, "VKGRP"     , div_vbvg.cb_frvg.Value  );
	ds_cond.SetColumn(0, "SMAN"      , div_manager.ed_sman_cd.Text);
	ds_cond.SetColumn(0, "QTNUM"     , ed_qtnum.Value          );

	fn_createWait("",this);     // 처리중입니다. 메시지 처리
	nsfRequest("header", "ses0560/header", "ds_cond=ds_cond ds_header=ds_header", "ds_header=ds_header ds_error=ds_error", "", "fn_afterQuery");
    //fn_createWait("",this);     // 처리중입니다. 메시지 처리
    trace(ds_header.savexml());
}

/********************************************************************************
* 기      능   : 조회 Callback  
********************************************************************************/
function fn_afterQuery(svcId, errCode, errMsg) {
	
	fn_query_chk = false; //중복조회 방지용 flag
	fn_destroyWait();
	
	if(ds_error.rowcount() > 0) {
		gfn_showMsg("CE00001");
		gfn_openWSErrorWin(grd_list, ds_header, ds_error, "FLAG");	// error 정보 표시 

	} else {
		if(errCode != 0) gfn_showMsg("CE00001");
		else             gfn_showMsg("CI00002", ds_header.RowCount() + "");
	}
	
	if(svcId == "save") {
		//gfn_showAlert("CI00003");
		fn_query();
		}
} 

/********************************************************************************
* 저장 
********************************************************************************/
function fn_save()
{
    var cnt;
    gfn_clearMsgBar();   

    //체크된 견적번호만 ds_header에 저장
    destroy('lds_header');
    create('dataset', 'lds_header');

    ds_header.fireevent=false;
    ds_header.Filter('CHK_QTNUM == "1"');
    cnt = ds_header.GetCount();
    
    lds_header.copyF(ds_header);
    ds_header.UnFilter();
    ds_header.fireevent=true;

    if(ds_ch_zkunnr.GetColumn(0,'CH_ZKUNNR') == null ||  cnt == 0 ) {

        gfn_showAlert("CW00005");    // 변경된 정보가 없습니다.

        return false;
    }
        

    if (gfn_showConfirm("CI10003") == false) return; // CI10003 저장하시겠습니까?
        //fn_createWait("",this);         // 처리중입니다. 메시지 처리



    ds_error.ClearData();           

    nsfRequest("save"                // srvId
                , "ses0560/save"    // url
                , "ds_cond=ds_cond ds_header=lds_header ds_ch_zkunnr=ds_ch_zkunnr"    // inDsList
                , ""    // outDsList
                , ""                // strParam
                , "fn_afterQuery"    // CallBackFunc
                );   
}


