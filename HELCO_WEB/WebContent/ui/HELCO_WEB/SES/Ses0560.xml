function ds_header_OnColumnChanged(obj,nRow,strColumnID,varOldValue,varNewValue,nPivotIndex)
{
	ds_header.fireevent = false;
	if(strColumnID == "CHK_QTNUM") {
		if(obj.getColumn(nRow, "CHK_QTNUM")==1){
			var chkSt = "QTNUM ==" + quote(obj.getColumn(nRow, "QTNUM")) + 
						" && QTSER ==" + quote(obj.getColumn(nRow, "QTSER"));
					
		
			var chkRow = ds_header.SearchRow(chkSt);
				ds_header.SetColumn(chkRow, "CHK_QTNUM", varNewValue);
				ds_header.SetColumn(chkRow, "FLAG", "U");
				obj.setColumn(nRow, "FLAG", "U");
		
		}else if(obj.getColumn(nRow, "CHK_QTNUM")==0){ 
			var chkSt = "QTNUM ==" + quote(obj.getColumn(nRow, "QTNUM")) + 
						" && QTSER ==" + quote(obj.getColumn(nRow, "QTSER"));
					
		
			var chkRow = ds_header.SearchRow(chkSt);
				ds_header.SetColumn(chkRow, "CHK_QTNUM", varNewValue);
				ds_header.SetColumn(chkRow, "FLAG", "U");
				obj.setColumn(nRow, "FLAG", "U");
		}	
	}

	ds_header.fireevent = true;
}
function grd_header_OnHeadClick(obj,nCell,nX,nY,nPivotIndex)
{
	var chk;
		if ( nCell == 0 ) {
		return;
	}else if(nCell == 1){
		for(var i=0; i<ds_header.GetRowCount(); i++){
			if(i==0){
				if(ds_header.GetColumn(i, "CHK_QTNUM")=="0"){
					chk = "1";
				}else{
					chk = "0";
				}
			}
			ds_header.SetColumn(i, "CHK_QTNUM", chk);
		}
	}else{
		// 그리드 정렬 처리 
		gfn_sortGrid(obj,nCell);
	}
	
}
