}

function fn_execute_print() 
{
	var qtnum = "";
	var qtser = "";
	var sqlstr = "";
	var loopCnt = 0;
	
	var nRowCnt = ds_list.CaseCountNF("CHK == '1' ");
				
	if(!fn_query_chk) {
		fn_query_chk = true;
	} else {
		gfn_showAlert("CW10025", "출력작업을 진행 중입니다.");
		return false;
	}
	
	//조회 조건 구성(견적번호, 견적차수)
	for(var i=0; i < ds_list.GetRowCount(); i++) {
		if(ds_list.GetColumn(i, "CHK") != "1") {
			continue;
		}
		
		loopCnt = loopCnt + 1;
				
		qtnum = ds_list.GetColumn(i, "QTNUM");
		qtser = ds_list.GetColumn(i, "QTSER");
		sqlstr = sqlstr + qtnum + qtser;
		
		if (nRowCnt > 1 && nRowCnt != loopCnt) {
			sqlstr = sqlstr + "," ;
		} 
	}
	
	// 서비스 호출
	nsfRequest("print"											// srvId
				, "ses0183/print"								// url
				, "ds_report=ds_report"							// inDsList
				, "ds_report=ds_report ds_error=ds_error"		// outDsList
				, "SQLSTR="+sqlstr		
				, "fn_afterQuery"								// CallBackFunc
				);
    fn_createWait("",this);     // 처리중입니다. 메시지 처리	

}
