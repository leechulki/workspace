var fn_query_chk = false; //중복조회 방지용 flag
var fn_query_chk2 = false; 

function fn_query() {
	
	if (fn_check_cond_vaild() == false) return;
	
	var lv_kunnr = div_buyr.ed_buyr_cd.Text;
	if (length(trim(lv_kunnr)) == 6 ) { 
		lv_kunnr = "0000"+lv_kunnr;
	}
	
	ds_cond.DeleteAll();
	ds_cond.AddRow();

	ds_cond.setColumn(0, "MANDT"     	, G_MANDT                 );
	ds_cond.SetColumn(0, "FR_BDDAT"  	, cal_bddat_fm.Value      );
	ds_cond.SetColumn(0, "TO_BDDAT"  	, cal_bddat_to.Value      );
	ds_cond.SetColumn(0, "FR_BDRSLTDT"	, cal_bdrsltdt_fm.Value   );
	ds_cond.SetColumn(0, "TO_BDRSLTDT"	, cal_bdrsltdt_to.Value   );
	ds_cond.SetColumn(0, "VKBUR"     	, div_vbvg.cb_frvb.Value  );
	ds_cond.SetColumn(0, "VKGRP"     	, div_vbvg.cb_frvg.Value  );
	ds_cond.SetColumn(0, "ZKUNNR"      	, div_sman.ed_sman_cd.Text);
	ds_cond.SetColumn(0, "KUNNR"   		, lv_kunnr);
	ds_cond.SetColumn(0, "QTNUM"     	, ed_qtnum.value          );
	ds_cond.SetColumn(0, "GUBUN"     	, cb_gubun.value          );
	ds_cond.SetColumn(0, "FR_QTDAT"  	, cal_qtdat_fm.Value      );
	ds_cond.SetColumn(0, "TO_QTDAT"  	, cal_qtdat_to.Value      );
	ds_cond.SetColumn(0, "AUART"  	    , cb_auart.Value          );
  
	// 서비스 호출
	nsfRequest("query"									// srvId
				, "ses0183/find"						// url
				, "ds_cond=ds_cond ds_list=ds_list"		// inDsList
				, "ds_list=ds_list ds_error=ds_error"	// outDsList
				, ""									// strParam
				, "fn_afterQuery"						// CallBackFunc
				);
    fn_createWait("",this);     // 처리중입니다. 메시지 처리

}


/********************************************************************************
* 기      능   : 폼 초기화
********************************************************************************/
function fn_init_form() {

	cal_bddat_fm.Text = left(gfn_getCurrDate(),6) + "01";
	cal_bddat_to.Text = gfn_getCurrDate();
	cal_bdrsltdt_fm.Text = left(gfn_getCurrDate(),6) + "01";
	cal_bdrsltdt_to.Text = gfn_getCurrDate();
	cal_qtdat_fm.Text = left(gfn_getCurrDate(),6) + "01";
	cal_qtdat_to.Text = gfn_getCurrDate();

	cb_gubun.Index = 0;
	cb_auart.Index = 0;
	
	var codeArr = ["BS403", "VKBUR", "VKGRP"]; 	//코드콤보 설정
	var dsObjArr = [ds_zbdtyp, ds_vkbur, ds_vkgrp];
	var addTypeArr = [G_CODE_ALL];
	gfn_searchCommCode(codeArr, dsObjArr, addTypeArr);
	
	// 실패사유 조회
	//gfn_queryDD07T('ZSDDZFRSN', ds_zfrsn, 'N');
			
	// 비고
	gfn_queryDD07T('ZSDDZRMKLIST', ds_zrmklist, 'N');		

	// 입찰사 조회 
	ds_sbdcm.DeleteAll();
	ds_sbdcm.AddRow();
	ds_sbdcm.SetColumn(0,"CODE","BS106");
	ds_sbdcm.SetColumn(0,"CODE_NAME","BS106");
	ds_sbdcm.SetColumn(0, "FILTER1","");

	nsfRequest("query"					// srvId
				, "combo/comboXml"		// url
				, "ds_sbdcm=ds_sbdcm"	// inDsList
				, "ds_sbdcm=ds_sbdcm"	// outDsList
				, ""					// strParam
				, "fn_afterQuery_sbdcm"	// CallBackFunc
				);
	
	// 광역지역 조회 
	ds_cond_atnam.ClearData();
	ds_cond_atnam.AddRow();
	ds_cond_atnam.SetColumn(0, "MANDT" , G_MANDT   );
	ds_cond_atnam.SetColumn(0, "KLART" , "300"     );
	ds_cond_atnam.SetColumn(0, "CLASS1", "ELV_01"  );
	ds_cond_atnam.SetColumn(0, "ATKLA" , "EL_BASIC");
	ds_cond_atnam.SetColumn(0, "ATNAM" , "EL_ALOCT");
	ds_cond_atnam.SetColumn(0, "WHERE" , ""        );
	nsfRequest("queryZregn2",
				"com0150/find",
				"ds_cond_atnam=ds_cond_atnam ds_list_atnam=ds_zlzone", 
				"ds_zlzone=ds_list_atnam", 
				"", 
				"fn_afterQuery");
				
	// 운송지역 조회 					
	ds_cond_atnam.ClearData();
	ds_cond_atnam.AddRow();
	ds_cond_atnam.SetColumn(0, "MANDT" , G_MANDT   );
	ds_cond_atnam.SetColumn(0, "KLART" , "300"     );
	ds_cond_atnam.SetColumn(0, "CLASS1", "ELV_01"  );
	ds_cond_atnam.SetColumn(0, "ATKLA" , "EL_BASIC");
	ds_cond_atnam.SetColumn(0, "ATNAM" , "EL_ADTA");
	ds_cond_atnam.SetColumn(0, "WHERE" , ""        );
	
	nsfRequest("queryZregn2",
				"com0150/find",
				"ds_cond_atnam=ds_cond_atnam ds_list_atnam=ds_region", 
				"ds_region=ds_list_atnam", 
				"", 
				"fn_afterQuery");
				
	ds_region.sort("ATWTB");

	// 오더유형 콤보목록 조회
	nsfRequest("query",
				"combo/comboAuart",
				"ds_auart_cd=ds_auart_cd", 
				"ds_auart_cd=ds_auart_cd ds_error=ds_error",
				"", 
				"fn_afterAuart");
				
	cb_gubun_OnChanged();
	
