		<Button Cursor="HAND" Height="18" Id="btn_posid" ImageID="btn_icon_search" Left="146" OnClick="btn_pjt_OnClick" TabOrder="6" Text="Button0" Top="28" Width="18"></Button>
		<Combo BindDataset="ds_cond" Border="Flat" CodeColumn="CODE" Column="YEARF" DataColumn="CODE_NAME" Height="20" Id="cb_yearf" InnerDataset="ds_yearf_cd" Left="86" Style="sty_ipt_search" TabOrder="8" Top="4" Width="75"></Combo>
		<Combo BindDataset="ds_cond" Border="Flat" CodeColumn="CODE" Column="MONTHF" DataColumn="CODE_NAME" Height="20" Id="cb_monthf" InnerDataset="ds_monthf_cd" Left="161" Style="sty_ipt_search" TabOrder="9" Top="4" Width="56"></Combo>
		<Static Align="Center" Border="None" Color="user7" Font="굴림,9" Height="20" Id="Static2" Left="218" TabOrder="7" Text="~" Top="5" VAlign="Middle" Width="18"></Static>
		<Combo BindDataset="ds_cond" Border="Flat" CodeColumn="CODE" Column="YEART" DataColumn="CODE_NAME" Height="20" Id="cb_yeart" InnerDataset="ds_yeart_cd" Left="236" Style="sty_ipt_search" TabOrder="10" Top="4" Width="75"></Combo>
		<Combo BindDataset="ds_cond" Border="Flat" CodeColumn="CODE" Column="MONTHT" DataColumn="CODE_NAME" Height="20" Id="cb_montht" InnerDataset="ds_montht_cd" Left="311" Style="sty_ipt_search" TabOrder="11" Top="4" Width="56"></Combo>
		<Combo BindDataset="ds_cond" Border="Flat" CodeColumn="CODE" Column="SUM" DataColumn="CODE_NAME" Height="20" Id="cb_sum" InnerDataset="ds_sum_cd" Left="248" Style="sty_ipt_search" TabOrder="12" Top="26" Width="100"></Combo>
		<Static Align="Right" Height="20" Id="st_gubun" Left="167" Style="sty_lb_search" TabOrder="13" Text="구분&#32;" Top="27" VAlign="Middle" Width="80"></Static>
		<Static Align="Right" Height="20" Id="st_ara" Left="369" Style="sty_lb_search" TabOrder="14" Text="지역&#32;" Top="5" VAlign="Middle" Width="80"></Static>
		<Combo BindDataset="ds_cond" Border="Flat" CodeColumn="CODE" Column="VKBUR" DataColumn="CODE_NAME" Height="20" Id="cb_ara" InnerDataset="ds_ara" Left="450" OnChanged="cb_ara_OnChanged" Style="sty_ipt_search" TabOrder="15" Top="4" Width="101"></Combo>
		<Combo BindDataset="ds_cond" Border="Flat" CodeColumn="CODE" Column="LIFNR" DataColumn="CODE_NAME" Height="20" Id="cb_gno" InnerDataset="ds_gno" Left="550" OnChanged="cb_gno_OnChanged" Style="sty_ipt_search" TabOrder="16" Top="4" Width="200"></Combo>
		<Combo BindDataset="ds_cond" Border="Flat" CodeColumn="CODE" Column="ZMAN" DataColumn="CODE_NAME" Height="20" Id="cb_user" InnerDataset="ds_user" Left="749" Style="sty_ipt_search" TabOrder="17" Top="4" Width="101"></Combo>
	</Form>
	<Script><![CDATA[/*
 ******************************************************************************************
 * 기      능   : 
 * 작  성  자   : 
 * 작성  일자   : 
 * ----------------------------------------------------------------------------------------
 * HISTORY    :
 ******************************************************************************************
*/
#include "LIB::commons.js";	// 공통 스크립트 
//=========================================================================================
// [ PART 1 ]
// Form에서 사용될 전역변수를 선언
// FORM에서 사용되는 전역변수는 대문자로 작성하며, F_XXX 의 형태를 갖도록 한다. 
//=========================================================================================

//=========================================================================================
// [ PART 2 ]
// Form Loading / Unloading 시 작업 정의
//=========================================================================================
function fn_OnLoadCompleted(obj) {
	gfn_initForm(true);

	var curYear = substr(gfn_getCurrDate(),0,4);
	fn_makeYear(curYear,-1,10);
	cb_monthf.index = parseInt(substr(gfn_getCurrDate(),4,2))-1;
	cb_montht.index = parseInt(substr(gfn_getCurrDate(),4,2))-1;
	cb_yearf.SetFocus();

	//코드콤보 설정 : 지역
	tit_clearActionInfo();
	tit_addSearchActionInfo("bs01:BS0106001A_S1");
	tit_addSearchActionInfo("bs01:BS0107001A_S1");
	tit_addSearchActionInfo("bs01:BS0106001A_S4");

	tit_callService(
        ""
        , ""
        , "ds_cond=ds_cond"
        , "ds_gno=ds_gno ds_user=ds_user ds_ara=ds_ara"
        , ""
        , "fn_afterGno"
        , true
        , true);
}

/********************************************************************************
* 기      능   : 공통코드가 조회된 후의 Callback 함수 
********************************************************************************/
function fn_completeInit(sCode, sDsObj, sCodeType) {
	ed_gno.value = G_VEND_NAME;
	gfn_showMsg("CI00007");
}

/********************************************************************************
* 기      능   : form UnloadCompleted 
********************************************************************************/
function fn_OnUnloadCompleted(obj) {
	return gfn_isFormUnload(obj);
}

/********************************************************************************
* 기      능   : 조회 Callback  
********************************************************************************/
function fn_afterGno(errCode, errMsg) {
	gfn_insertEmptyRow(ds_ara, G_CODE_ALL);
	cb_ara.value = "";
	gfn_insertEmptyRow(ds_gno, G_CODE_ALL);
	cb_gno.value = "";
	gfn_insertEmptyRow(ds_user, G_CODE_ALL);
	cb_user.value = "";

	var userGroup = gfn_getUserGroup();

	if(userGroup == "20") {
		ds_ara.Filter("CODE=="+quote(G_AREA_CODE_R));
		cb_ara.Index = 0;
		cb_ara.Enable = false;

		cb_ara_OnChanged(ds_ara,cb_ara.Value,"","","");
	} else if(userGroup == "10") {
		cb_ara.Index = 0;
		cb_ara.Enable = false;

		ds_gno.Filter("CODE=="+quote(G_VEND_CODE));
		cb_gno.Index = 0;
		cb_gno.Enable = false;

		cb_gno_OnChanged(ds_gno,cb_gno.Value,"","","");

		ds_user.Filter("CODE=="+quote(G_USER_ID));
		cb_user.Index = 0;

		if(ds_user.GetColumn(0, "DESC") == "REP") {
			cb_gno_OnChanged(ds_gno,cb_gno.Value,"","","");
		} else {
			cb_user.Enable = false;
		}
	}
}

/********************************************************************************
* 기      능   : 지급예정년 계산(현재년도,당해년도에서 뺄 년수, 당해년도에서 더할 년수
********************************************************************************/
function fn_makeYear(curYear,sNum,eNum) {
	var rowCnt = eNum-sNum;
	var row = 0;

	for(var i=rowCnt; i>-1; i--) {
		ds_yearf_cd.AddRow();
		ds_yearf_cd.SetColumn(row,"CODE",parseInt(curYear)+i+parseInt(sNum));
		ds_yearf_cd.SetColumn(row,"CODE_NAME",(parseInt(curYear)+i+parseInt(sNum))+"년");
		ds_yeart_cd.AddRow();
		ds_yeart_cd.SetColumn(row,"CODE",parseInt(curYear)+i+parseInt(sNum));
		ds_yeart_cd.SetColumn(row,"CODE_NAME",(parseInt(curYear)+i+parseInt(sNum))+"년");
		row++;
	}

	cb_yearf.index = eNum;
	cb_yeart.index = eNum;
}

//=========================================================================================
// [ PART 3 ]
// Main Process Event 정의(조회,입력,삭제,저장,인쇄,form닫기)
//=========================================================================================
/********************************************************************************
* 기      능   : 조회 1
********************************************************************************/
function fn_query() {
	gfn_clearMsgBar();

	var user_grp = gfn_getUserGroup();

	ds_cond.SetColumn(0, "SMON", cb_yearf.value + cb_monthf.Value + "");
	ds_cond.SetColumn(0, "EMON", cb_yeart.value + cb_montht.Value + "");
	ds_cond.SetColumn(0, "PROJ", ed_pjt.value);
	ds_cond.SetColumn(0, "LIFNR", cb_gno.value);
	ds_cond.SetColumn(0, "VKBUR", cb_ara.value);
	ds_cond.SetColumn(0, "SUM", cb_sum.value);
	ds_cond.SetColumn(0, "ZMAN", cb_user.value);
	
	fn_inputValidation();

	// sort mark 삭제 
	gfn_clearGridSortMark(grd_list);
	ds_list.row = -1;
	ds_list.ClearData();

	tit_callService(
        "BS0107001A_ACT"
        , "query"
        , "ds_cond=ds_cond ds_list=ds_list"
        , "ds_list=ds_list ds_error=ds_error"
        , ""
        , "fn_afterQuery");
}

/********************************************************************************
* 기      능   : 닫기  
********************************************************************************/
function fn_close() {
	close();
}

/********************************************************************************
* 기      능   : 액셀  
********************************************************************************/
function fn_excel() {
	// Excel Export 
	gfn_saveToExcel(grd_list, true);
}

//=========================================================================================
// [ PART 4]
// Callback 함수 정의 : callback 함수명은 메인 함수의 fn_xxxx() 에서 xxxx 부분은 fn_afterXXXX()
// 형태가 되도록 작성 
//=========================================================================================
/********************************************************************************
* 기      능   : 조회 Callback  
********************************************************************************/
function fn_afterQuery(errCode, errMsg) {
	if(ds_error.rowcount() > 0) {
		gfn_showMsg("CE00001");
		// error 정보 표시 
		gfn_openWSErrorWin(grd_list, ds_list, ds_error, "FLAG");
	} else {
		if(errCode != 0) {
			gfn_showMsg("CE00001");
		} else {
			gfn_showMsg("CI00002", ds_list.RowCount() + "");
		}
	}
}

function fn_afterCodeSearch(errCode, errMsg) {
	gfn_insertEmptyRow(ds_man_cd, G_CODE_ALL);
	cb_man.index = 0;
}

//=========================================================================================
// [ PART 6 ]
// component Event 정의 : 각 Component 별 이벤트 
//=========================================================================================
/********************************************************************************
* 기      능   : 그리드 헤더 클릭시 정렬 처리 
********************************************************************************/
function grd_list_OnHeadClick(obj,nCell,nX,nY,nPivotIndex) {
	if (nCell == 0) {

	} else {
		// 그리드 정렬 처리 
		gfn_sortGrid(obj, nCell);
	}
}

/********************************************************************************
* 기      능   : 지역콤보 변경시
********************************************************************************/
function cb_ara_OnChanged(obj,strCode,strText,nOldIndex,nNewIndex) {
	if(length(trim(strCode)) == 0) {
		ds_gno.Filter("LENGTH(ARA) == 0 || 1==1");
		ds_user.Filter("LENGTH(LIFNR) == 0 || 1==1");
		cb_gno.Index = 0;
		cb_user.Index = 0;
	} else {
		ds_gno.Filter("LENGTH(ARA) == 0 || ARA == "+quote(strCode));
		ds_user.Filter("LENGTH(LIFNR) == 0 || ARA == "+quote(strCode));
		cb_gno.Index = 0;
		cb_user.Index = 0;
	}	
}

/********************************************************************************
* 기      능   : 협력업체콤보 변경시
********************************************************************************/
function cb_gno_OnChanged(obj,strCode,strText,nOldIndex,nNewIndex) {
	if(length(trim(strCode)) == 0) {
		if(length(trim(cb_ara.Value)) > 0)
			ds_user.Filter("LENGTH(LIFNR) == 0 || ARA == "+quote(cb_ara.Value));
		else
		ds_user.Filter("LENGTH(LIFNR) == 0 || 1==1");

		cb_user.Index = 0;
	} else {
		ds_user.Filter("LENGTH(LIFNR) == 0 || LIFNR == "+quote(strCode));
		cb_user.Index = 0;
	}	
}

/********************************************************************************
* 기      능   : 원프로젝트 팝업
********************************************************************************/
function btn_pjt_OnClick(obj) {
	var arg = "pjt="+quote(ed_pjt.Text)+" gbn=A";
	gfn_openPopup("CS9601002A",true,arg);
}

/********************************************************************************
* 기      능   : 쿼리 파라미터 input validation
********************************************************************************/
function fn_inputValidation() {
	if (ds_cond.GetColumn(0, "LIFNR") == null) {
		ds_cond.SetColumn(0, "LIFNR", "");
	}
}]]></Script>
</Window>