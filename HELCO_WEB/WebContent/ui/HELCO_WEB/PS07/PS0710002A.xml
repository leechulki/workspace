// [ PART 3 ]
// Main Process Event 정의(조회,입력,삭제,저장,인쇄,form닫기)
//=========================================================================================
/********************************************************************************
* 기      능   : 조회 1
********************************************************************************/
function fn_query() {
	var sPspid = ds_cond.GetColumn(0, "PSPID");
	if(Trim(sPspid) == NULL || length(sPspid) == 0 || sPspid == "")
	{
		if ( !gfn_checkFromToObj(cal_fdate, cal_tdate, true, st_date.value, 0, true) ) {
			return;
		}
		
		// 조회 기간(일자)  1년 이내 범위로 제한. 
		gfn_getDayCnt(ds_dayCnt, cal_fdate.Value, cal_tdate.Value);
		nDays = ds_dayCnt.GetColumn(0, "DAYS");
		
		if(nDays > 365)
		{
			gfn_openMsgWin("조회 범위는 1년이내로 제한됩니다.");
			cal_fdate.Value = gfn_getAfterDate(gfn_getCurrDate(),-365);
			cal_tdate.Value = gfn_getCurrDate();
			return;
		}
	}
	
	
	
	gfn_clearGridSortMark(grd_list);

	ds_cond.SetColumn(0,"SO_GBN","");
	var qry = "";
	var args = "";

	if( userGroup == "40" && cb_actss.Value == '')
	{
		if(chk_so2.Value == '1' || chk_so5.Value == '1' || chk_so6.Value == '1') //&& (chk_so3.Value == '1' && chk_so4.Value == '0'))
		{
			qry += " AND (A.ZZACTSS IN (";
			if(chk_so2.Value == '1') qry+= "'01',";
			if(chk_so5.Value == '1') qry+= "'13',";
			if(chk_so6.Value == '1') qry+= "'14',";
			qry+= "'')) ";
			ds_cond.SetColumn(0,"SO_GBN","C");
		}	
	}
		
	args = " SQRY="+quote(qry);	



	tit_clearActionInfo();
	tit_addSearchActionInfo("ps07:PS0710002A_S1");
	tit_callService(
        ""
        , ""
        , "ds_cond=ds_cond"
        , "ds_list=ds_list"
        , args
        , "fn_afterQuery"
        , true
        , true);
}
