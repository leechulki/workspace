function fn_save() {
	// 그리드 저장 대상 로우
	if(ds_list.FindRow("CHECK", 1)==-1){
		gfn_showMsg('CW00046');
		return;
	}
	
	for(var i=0; i<ds_list.GetRowCount(); i++)
	{
		if(ds_list.GetColumn(i, "CHECK")==1)
		{					
			if(  ds_list.GetColumn(i, "KETF3_DATE") >'00000000' && 
			    ( ds_list.GetColumn(i, "KETF1_DATE") <= '00000000' || ds_list.GetColumn(i, "KETF2_DATE") <= '00000000' ) &&
			    ( length(ds_list.GetColumn(i, "KETF1_DATE")) <> 8 || length(ds_list.GetColumn(i, "KETF2_DATE")) <> 8 ))
			{
				var arr = [parseInt(i) + 1, "1차접수일 or 2차접수일 "];
				gfn_showMsg("CW00055", arr);
				return;
			} 		
					
			if(  ds_list.GetColumn(i, "KETF2_DATE") >'00000000' && ds_list.GetColumn(i, "KETF1_DATE") <= '00000000' && length(ds_list.GetColumn(i, "KETF1_DATE")) <> 8  )
			{
				var arr = [parseInt(i) + 1, "1차접수일"];
				gfn_showMsg("CW00055", arr);
				return;
			} 															
		}
	}

	ds_list.Filter("CHECK==1");
	ds_list2.copyF(ds_list);
	ds_list.unFilter();


	tit_clearActionInfo();
	tit_addSelectExActionInfo("ps07:PS0709101A_S2", "ps07:PS0709101A_I1", "ps07:PS0709101A_U1");
