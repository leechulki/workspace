// Form에서 사용될 전역변수를 선언
// FORM에서 사용되는 전역변수는 대문자로 작성하며, F_XXX 의 형태를 갖도록 한다. 
//=========================================================================================

//=========================================================================================
// [ PART 2 ]
// Form Loading / Unloading 시 작업 정의
//=========================================================================================
function fn_OnLoadCompleted(obj) {
	// Form 로드 이벤트 맨 첫줄에 기술 
	gfn_initForm();

	tit_clearActionInfo();
	tit_addSearchActionInfo("wb03:WB0301001A_S2");
	
	tit_callService(
        ""
        , ""
        , ""
        , "ds_dpt1_cd=ds_dpt1_cd"
        , ""
        , "fn_afterInit"
        , true);
        
}
/********************************************************************************
* 기      능   : 부서조회 후 콜백
********************************************************************************/
function fn_afterInit()
{
	ds_dpt1_cd.insertRow(0);
	ds_dpt1_cd.setColumn(0, "CODE", "");
	ds_dpt1_cd.setColumn(0, "CODE_NAME", "- 전 체 -");
	
	//장비구분
	cb_pcagbn.Index = 0;
	//사용여부
	cb_xdgbn.Index = 0;
	 //부서명
	cb_dpt1.Index = 0;
}

/********************************************************************************
* 기      능   : form UnloadCompleted 
********************************************************************************/
function fn_OnUnloadCompleted(obj) {
	return gfn_isFormUnload(obj);
}
//=========================================================================================
// [ PART 3 ]
// Main Process Event 정의(조회,입력,삭제,저장,인쇄,form닫기)
//=========================================================================================
/********************************************************************************
* 기      능   : 조회 1
********************************************************************************/
function fn_query() {	
    // Message 영역 초기화
	global.FRM_BOTTOM.img_msgType.imageId = "";
	global.FRM_BOTTOM.st_msg.value = "";
	// 사용자
	if (length(ed_pcbpme.value) > 0 && substr(ed_pcbpme.Value,0,1) != "H")
	{
		ds_cond.SetColumn(0,"PCBPME","H"+ed_pcbpme.Value);
	}	

	tit_clearActionInfo();
	tit_addSearchActionInfo("wb03:WB0301001A_S1");
	
	tit_callService(
        ""
        , ""
        , "ds_cond=ds_cond"
        , "ds_list=ds_list"
        , ""
        , "fn_afterQuery"
        , true);
}

/********************************************************************************
* 기      능   : 닫기  
********************************************************************************/
function fn_close() {
	close(null);
}
/********************************************************************************
* 기      능   : 액셀  
********************************************************************************/
function fn_excel() {
	// Excel Export 
	gfn_saveToExcel(grd_list, true );	
}
//=========================================================================================
// [ PART 4]
// Callback 함수 정의 : callback 함수명은 메인 함수의 fn_xxxx() 에서 xxxx 부분은 fn_afterXXXX()
// 형태가 되도록 작성 
//=========================================================================================
/********************************************************************************
* 기      능   : 조회 Callback  
********************************************************************************/
function fn_afterQuery(errCode, errMsg) {
	// 메시지 처리 
	gfn_showSearchMsg(grd_list);
}
//=========================================================================================
// [ PART 5 ]
// Detail Process Event 정의 ( User Define function )
// 사용자 정의 함수는 ufn_xxxx() 형태로 작성 
//=========================================================================================
/********************************************************************************
* 기      능   : 사용자 정의 함수  
********************************************************************************/
function ufn_closeWindow() {
	var arr = [
		  ds_list.GetColumn(ds_list.currow,"PCAPNO")
	];

	close(arr);
}

//=========================================================================================
// [ PART 6 ]
// component Event 정의 : 각 Component 별 이벤트 
//=========================================================================================
/********************************************************************************
* 기      능   : 그리드 헤더 클릭시 정렬 처리 
********************************************************************************/
function grd_list_OnHeadClick(obj,nCell,nX,nY,nPivotIndex)
{
	if ( nCell == 0 ) {
		// 그리드 표시할 컬럼 선택 
		gfn_selectGridCol(obj);
	} else {
		// 그리드 정렬 처리 
		gfn_sortGrid(obj,nCell);
	}
}


function btn_pme_OnClick(obj)
{
	var psno_arr = gfn_openPopup("WB0301001B",true,null);
	if (psno_arr != null)
	{
		ed_pcbpme.value = psno_arr[0];
		fn_query();
	}
}
