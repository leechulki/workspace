	gfn_sortGrid(obj,nCell);
}
function ufn_setYear(gbn)
{
	var curDate = substr(gfn_getCurrDate(),0,4);
	if(gbn=='1') return ParseInt(curDate)-5;
	else if(gbn=='2') return ParseInt(curDate)-4;
	else if(gbn=='3') return ParseInt(curDate)-3;
	else if(gbn=='4') return ParseInt(curDate)-2;
	else if(gbn=='5') return ParseInt(curDate)-1;
	else if(gbn=='6') return ParseInt(curDate);
