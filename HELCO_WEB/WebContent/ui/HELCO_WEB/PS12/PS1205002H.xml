#include "LIB::commons.js";	// 공통 스크립트
//=========================================================================================
// [ PART 1 ]
// Form에서 사용될 전역변수를 선언
// FORM에서 사용되는 전역변수는 대문자로 작성하며, F_XXX 의 형태를 갖도록 한다.
//=========================================================================================
var _F_FILE_DIR = "";	// 로컬에서 현재 선택한 파일 디렉토리
var _F_FILE_NAME = "";	// 로컬에서 현재 선택한 파일  명
var _F_FILE_SIZE = 0;	// 파일 사이즈
var _F_FILE_DELETE = false;
var _MAX_DAY = 7;
var _add_file = false;

//=========================================================================================
// [ PART 2 ]
// Form Loading / Unloading 시 작업 정의
//=========================================================================================
function fn_OnLoadCompleted(obj) {
	gfn_initFormNotAuth("PS1205002H", st_title.value, "");

	for (var i = 0; i < cnt; i++) {
		eval("divImage.div_date" + (i + 1)).Text = (i + 1) + '. ' + eval('cdate' + i);
		eval("divImage.div_date" + (i + 1)).Visible = true;

		var title = eval('fname' + i);
		if(length(title) > 12) title = left(title, 12) + "..." ;

		eval("divImage.div_pro_name" + (i + 1)).Text = title;
		eval("divImage.div_pro_name" + (i + 1)).Visible = true;

		eval('divImage.Image' + (i + 1)).ImageID = eval('fname_app' + i);
		eval('divImage.Image' + (i + 1)).UserData = eval('fname_app' + i);
		eval("divImage.Image" + (i + 1)).Visible = true;
	}

	divImage.ResizeScroll();
}

/********************************************************************************
* 기      능   : 이미지 큰팝업 호출
********************************************************************************/
function images_onClickOpenPopup(obj,nX,nY)
{
	var arg = "IMGURL=" + quote(obj.UserData);
	trace("userdata : " + obj.UserData);
	gfn_openPopupUrl("PS02::PS0209001C.xml", true, arg);
}

/********************************************************************************
* 기      능   : 창닫기
********************************************************************************/
function fn_close()
{
	close(null);
}

