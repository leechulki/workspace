		return;
	}
	
	fn_query();
}

/********************************************************************************
* 기      능   : 자재수령 버튼 클릭 시
********************************************************************************/
function btn_matnrRc_OnClick(obj)
{
	if(ds_head.GetRowCount() == 0)
	{
		gfn_openMsgWin("데이터 조회 후, 수령처리 가능합니다.");
		return;
	}
	
	if(!confirm("자재수령 하시겠습니까?")) return;	
	
	ds_head.fireevent = false;

	gfn_copyDs(ds_head, ds_head1, "CHK=1");

	if (ds_head1.rowcount < 1) {
		gfn_showAlert("CW00180","체크표시");
		ds_head.fireevent = true;
		return;
	}
	
	//alert(ds_head1.rowcount);	
	
	tit_callService(
        "PS1209002A_ACT"
        , "update_invoice"
        , "ds_head1=ds_head1"
        , ""
        , ""
        , "fn_afterSave"
        , true);
}

function fn_afterSave(errCode, errMsg)
{
	if ( errCode != 0 ) 
	{
		gfn_showAlert("CE00001");
	}
	else
	{
		gfn_openMsgWin("자재수령 처리를 완료했습니다.");
		fn_query();
	}
	
	ds_head.fireevent = true;	
}


