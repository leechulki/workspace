* 기      능   : 조회 
********************************************************************************/
function fn_query() {

	gfn_clearMsgBar();

	if(gfn_isNull(cal_from, true, Static0.value)) return;
	   
    // sort mark 삭제 
	gfn_clearGridSortMark(grd_list);
	ds_list.row = -1;
	ds_list.ClearData();

	tit_clearActionInfo();	
	if(rd_gbn.Index == 0){
    grd_list.Format = "Default";
	tit_addSearchActionInfo("ps12:PS1204007A_S1");	}
	else if(rd_gbn.Index == 1){
	grd_list.Format = "Default1"; 
    tit_addSearchActionInfo("ps12:PS1204007A_S2");  }
	else {
   grd_list.Format = "Default2"; 
	tit_addSearchActionInfo("ps12:PS1204007A_S3");  }
	tit_callService(
        ""
        , ""
        , "ds_cond=ds_cond"
        , "ds_list=ds_list"
        , ""
        , "fn_afterQuery"
        , true );    
   
     
}

