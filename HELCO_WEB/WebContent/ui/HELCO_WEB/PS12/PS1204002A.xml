		cal_f_to.Enable = false;
		cal_from.Enable = true;
		cal_to.Enable = true;
		
		
	}
	else
	{
		cb_finish.index = 0;
		cb_finish.Enable = true;
		
		ed_supman.text = '';
		ed_supman.Enable = true;
		
		
		cal_from.Value = '';
		cal_to.Value = '';	
	    cal_f_from.value = substr(gfn_getCurrDate(),0,6)+"01"; 
        cal_f_to.value	 = gfn_getCurrDate();
		cal_f_from.Enable = true;
		cal_f_to.Enable = true;
		cal_from.Enable = false;
		cal_to.Enable = false;
		
	}
		
		
}




/********************************************************************************
* 기      능   : 조회 
********************************************************************************/
function fn_query() {

	gfn_clearMsgBar();

	//if(gfn_isNull(cal_from, true, Static0.value)) return;
	
	// 필수 입력사항 및 일자 체크 	
	if(gfn_isNull(cb_gbn2, true, Static7.Text)) return;
	
	if( cb_gbn2.Index == 0 )  //요청서
	{
		if ( !gfn_checkFromToObj(cal_from, cal_to, true, Static0.value, 0, true)   ) 
		{	return; 	}
    }
	else //보고서
	{
		if ( !gfn_checkFromToObj(cal_f_from, cal_f_to, true, Static2.value, 0, true)   ) 
		{	return; 	}
    }
    
   
    // sort mark 삭제 
	gfn_clearGridSortMark(grd_list);
	ds_list.row = -1;
	ds_list.ClearData();

//===============================================> 여기서부터

	tit_clearActionInfo();	
	//tit_addSearchActionInfo("ps12:PS1204001A_S2");	
	if( cb_gbn2.Index == 0 )  //요청서
	{
		grd_list.Format = "Default";
		tit_addSearchActionInfo("ps12:PS1204002A_S2");	
	}
	else //보고서
	{
		grd_list.Format = "Default1";
		tit_addSearchActionInfo("ps12:PS1204002A_S3");	
	}
	tit_callService(
        ""
        , ""
        , "ds_cond=ds_cond"
        , "ds_list=ds_list"
        , ""
        , "fn_afterQuery"
        , true
        , true );    
        
        
        
}
 



