/****************************************************************
* Button 정보 loading 시키기
* @param  btnList 사용 가능한 버튼 목록 ( "1,2,3,4" 등의 숫자를 가지는 문자열  )
* @param  pageAuth	페이지에 대한 전체 권한 ( 1 ~ 9 중 )
* @param  rightPos - 필요시 버튼이 위치해야 하는 포지션
***************************************************************/
var F_BTN_GAP = 2;

function ufn_loadUseBtn(btnList, screenName, pageAuth, pgmUrl)
{
	var i1 = fa_pgmUrl.indexOf(":") + 2;
	var i2 = fa_pgmUrl.indexOf(".xml") - i1;

	st_title.value = screenName+"["+ pgmUrl.substr(i1,i2) +"]";

	var btnObj = null;
	var rPos = 976;
	var bArr = split( btnList, ",", true );
	var nRow = 0;
	var nArr = length(bArr)-1;

	// 닫기 버튼 추가
	nRow = ds_currBtnList.AddRow();
	ds_currBtnList.SetColumn(nRow, "NO", 900);
	ds_currBtnList.SetColumn(nRow, "BTN_ID", "close");
	// 버튼 배열을 역순으로 처리
	for (var i = nArr; i > -1; i--)
	{
		if ( ds_currBtnList.findRow("NO", toNumber(bArr[i]) ) != -1 )
		{
			continue;
		}

		nRow = ds_currBtnList.AddRow();
		ds_currBtnList.SetColumn(nRow, "NO", bArr[i]);
		ds_currBtnList.SetColumn(nRow, "BTN_ID", ds_btnList.getColumn(ds_btnList.findRow("NO", bArr[i]), "BTN_ID"));
	}

	for( var i = 0; i < ds_currBtnList.rowCount(); i++ )
	{
		if( length( ds_currBtnList.getColumn(i, "BTN_ID") ) == 0 )
		{
			continue;
		}

		btnObj = object("btn_" + ds_currBtnList.getColumn(i, "BTN_ID"));

		if ( isValid( btnObj ) )
		{
			if ( G_LANG == "en")
			{
				// 영문일 경우 이미지 교체
				btnObj.ImageId = substr(btnObj.imageId, 0, length(btnObj.imageId) -1 ) + "u";
				btnObj.width = ds_btnList.getColumn(ds_btnList.findRow("BTN_ID",ds_currBtnList.getColumn(i, "BTN_ID") ), "W");
			}
			btnObj.top = 11;
			btnObj.left = rPos - btnObj.width;
			rPos -= btnObj.width + F_BTN_GAP;
			btnObj.Visible = true;
		}
	}
}
