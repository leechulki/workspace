/****************************************************************
*  form KeyDown 이벤트 처리
***************************************************************/
function mainForm_OnKeyDown(obj,objSenderObj,nChar,bShift,bControl,bAlt,nLLParam,nHLParam)
{
	// Enter Key
	if (nChar == "13")
	{
		//  그리드(grd_list)일때 무시 - 2011.01.19 HSS 수정전 문의 바람.
		if ( ToUpperCase(objSenderObj.GetType()) == "GRID" )
		{
			return;
		}
		// 특정 그리드(grd_list_E)일때 무시
		if ( objSenderObj == "grd_list_E" )
		{
			return;
		}

		// Textarea 무시
		if ( ToUpperCase(objSenderObj.GetType()) == "TEXTAREA" )
		{
			return;
		}
		
        //=========반복적인 enter 눌림 방지를 위하여 적용=========
        if (objSenderObj = object("btn_query"))
        {
			return;
        }
        //========================================================

		var objObject = object("btn_query");

		if (IsValid (objObject) && objObject.Visible == true)
		{
			objObject.SetFocus();;
			eval( "div_main.fn_query()");
		}


	}
	// Ctrl + C
	else if (objSenderObj.getType() == "Grid" && (bControl) && (nChar == 67))
	{
		if(substr(ToString(eval("div_main.Tab00.tab"+(parseInt(div_main.Tab00.TabIndex)+1)+"."+objSenderObj.id)),11,8) == "grd_list")
		{
			grd_fn_ClipboardCopy_tab(eval("div_main.Tab00.tab"+(parseInt(div_main.Tab00.TabIndex)+1)+"."+objSenderObj.id), eval("div_main.Tab00.tab"+(parseInt(div_main.Tab00.TabIndex)+1)+"."+objSenderObj.BindDataset), "	");
		}
		else
		{
			grd_fn_ClipboardCopy(eval("div_main."+objSenderObj.id), eval("div_main."+objSenderObj.BindDataset), "	");
		}	
	}
	// Ctrl + V
	else if (objSenderObj.getType() == "Grid" && (bControl) && (nChar == 86))
	{
		grd_fn_ClipboardPaste(eval("div_main."+objSenderObj.id), eval("div_main."+objSenderObj.BindDataset), "	");
	}
	// Ctrl + Z
	else if (objSenderObj.getType() == "Grid" && (bControl) && (nChar == 90))
	{
		grd_fn_CellReset(eval("div_main."+objSenderObj.id), eval("div_main."+objSenderObj.BindDataset));
	}
}
