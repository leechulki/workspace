function fn_subsumGubun(vRowlevel)
{
	var vResult = "";

	switch(vRowlevel)	{
		case "1":
		case "2":
		case "3":	vResult = "이월";
					break;
		case "4":
		case "5":
		case "6":	vResult = "신규";
					break;
		case "7":
		case "8":
		case "9":	vResult = "계";
					break;
		default  :	break;
	}

	return vResult;
}

function fn_subsumAmtgubun(vRowlevel)
{
	var vResult = "";

	switch(vRowlevel)	{
		case "1":
		case "4":
		case "7":	vResult = "계획";
					break;
		case "2":
		case "5":
		case "8":	vResult = "집계";
					break;
		case "3":
		case "6":
		case "9":	vResult = "결과";
					break;
		default  :	break;
	}

	return vResult;
}

function fn_subsumexpr(vRowlevel, vCurrow, vValue)
{
	var vResult = 0;
	var vFilter = "GROUP==" + quote(ds_list.GetColumn(vCurrow - vRowlevel + 1, "GROUP"));
	switch(vRowlevel)	{
		case "1" :	vFilter += " && GUBUN==" + quote("A") + " && AMTGUBUN==" + quote("P");
					break;
		case "2" :	vFilter += " && GUBUN==" + quote("A") + " && AMTGUBUN==" + quote("R");
					break;
		case "3" :	vFilter += " && GUBUN==" + quote("A") + " && AMTGUBUN==" + quote("Z");
					break;
		case "4" :	vFilter += " && GUBUN==" + quote("N") + " && AMTGUBUN==" + quote("P");
					break;
		case "5" :	vFilter += " && GUBUN==" + quote("N") + " && AMTGUBUN==" + quote("R");
					break;
		case "6" :	vFilter += " && GUBUN==" + quote("N") + " && AMTGUBUN==" + quote("Z");
					break;
		case "7" :	vFilter += " && GUBUN==" + quote("T") + " && AMTGUBUN==" + quote("P");
					break;
		case "8" :	vFilter += " && GUBUN==" + quote("T") + " && AMTGUBUN==" + quote("R");
					break;
		case "9" :	vFilter += " && GUBUN==" + quote("T") + " && AMTGUBUN==" + quote("Z");
					break;
		default  :	break;
	}

	vResult = ds_list.CaseSum(vFilter, vValue);

	return vResult;
