		var tail_conts = substr(cur_conts, pos(cur_conts, "</summary>") + 10, length(cur_conts));

		grd_list.Contents = body_conts + tail_conts;
	
		Contents.Redraw = true;
	 }

	// HTTP SESSION TIMEOUT을 변경
	//http.TimeOut = 1800;

	tit_clearActionInfo();
