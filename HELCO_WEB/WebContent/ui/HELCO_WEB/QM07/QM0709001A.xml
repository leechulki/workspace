	}
	
	if (length(trim(cb_dept.Value)) == 0)	{
		 ds_list.GroupKey  = "GROUP,GROUP,GROUP,GROUP,GROUP,GROUP,GROUP,GROUP,GROUP";

		 grd_list.Contents = F_GRID_CONTS;
