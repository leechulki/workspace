	ds_list.ClearData();

	grd_d_list.SetCellProp("head", 0, "Text", "1");
	
	for(i=1; i < ds_excel.rowcount ; i++) {
		ds_list.AddRow();
		ds_list.SetColumn(i-1,"GBN", ds_excel.GetColumn(i,0));
		ds_list.SetColumn(i-1,"YEA", ds_excel.GetColumn(i,1));
		ds_list.SetColumn(i-1,"BUN", ds_excel.GetColumn(i,2));
		ds_list.SetColumn(i-1,"GNO", ds_excel.GetColumn(i,3));
		ds_list.SetColumn(i-1,"HNO", ds_excel.GetColumn(i,4));
		ds_list.SetColumn(i-1,"KUN", ds_excel.GetColumn(i,5));
		ds_list.SetColumn(i-1,"TEM", ds_excel.GetColumn(i,6));
		ds_list.SetColumn(i-1,"BSU", ds_excel.GetColumn(i,7));
		ds_list.SetColumn(i-1,"TOT", ds_excel.GetColumn(i,8));
		ds_list.SetColumn(i-1,"YID1", ds_excel.GetColumn(i,9));
		ds_list.SetColumn(i-1,"JG1", ds_excel.GetColumn(i,10));
		ds_list.SetColumn(i-1,"YID2", ds_excel.GetColumn(i,11));
		ds_list.SetColumn(i-1,"JG2", ds_excel.GetColumn(i,12));
		ds_list.SetColumn(i-1,"JID1", ds_excel.GetColumn(i,13));
