// 2018.10.15 J.H Han
function fn_calendar_changed(obj,strOldText,strNewText)
{
	var packinggb = '';
	if(cb_packingdt.Value == '0') { // 2018.10.15
		packinggb = 'PACKINGDT_RQ';
	} else if(cb_packingdt.Value == '1') {
		packinggb = 'PACKINGDT';
	}
	
	for(var i=0;i<ds_list.GetRowCount();i++) {
	
		if(ds_list.getColumn(i, "CHECK") == true) {
			ds_list.SetColumn(i, packinggb, strNewText);
			
			if(packinggb == 'PACKINGDT') {
				// 패킹완료 처리 
				ds_list.SetColumn(i, "PACKING_S", 3);
			}
		}
