#include "LIB::commons.js";	// 공통 스크립트 
#include "LIB::utility.js";	// 다중 선택 
//=========================================================================================
// [ PART 1 ]
// Form에서 사용될 전역변수를 선언
// FORM에서 사용되는 전역변수는 대문자로 작성하며, F_XXX 의 형태를 갖도록 한다. 
//=========================================================================================
var userGroup;
var gbn = "";
//=========================================================================================
// [ PART 2 ]
// Form Loading / Unloading 시 작업 정의
//=========================================================================================
function fn_OnLoadCompleted(obj) 
{

	gfn_initForm(true);

	userGroup = gfn_getUserGroup();

	tit_clearActionInfo();
	tit_addSearchActionInfo("pp01:PP0102002A_S1");
	tit_addSearchActionInfo("pp01:PP0102002A_S2");
	tit_callService(
		""
		, ""
		, "ds_cond_cd=ds_cond"
		, "ds_arbpl=ds_arbpl ds_lifnr=ds_lifnr"
		, ""
		, "fn_afterArbpl"
		, true,true);
}
/********************************************************************************
* 기      능   : 공통코드가 조회된 후의 Callback 함수 
********************************************************************************/
function fn_completeInit(sCode, sDsObj, sCodeType)
{
}
/********************************************************************************
* 기      능   : form UnloadCompleted 
********************************************************************************/
function fn_OnUnloadCompleted(obj)
{
	return gfn_isFormUnload(obj);
}
/********************************************************************************
* 기      능   : 조회 Callback  
********************************************************************************/
function fn_afterArbpl(errCode, errMsg) {
	//gfn_insertEmptyRow( ds_arbpl, G_CODE_ALL );
	cb_arbpl.Index = 0;
	cb_flag.Index = 0;
	cb_gubun.Index = 0;
}
//=========================================================================================
// [ PART 3 ]
// Main Process Event 정의(조회,입력,삭제,저장,인쇄,form닫기)
//=========================================================================================
/********************************************************************************
* 기      능   : 조회 
********************************************************************************/
function fn_query() 
{
	if ( !gfn_checkFromToObj(cal_fdt, cal_tdt, true, st_tdate.value, 0, false) )
	{
		return;
	}

	var aufnrCnt = 0;
	var posidCnt = 0;
	for(var i=0; i<ds_aufnr.RowCount(); i++)
	{
		if(length(ds_aufnr.GetColumn(i,"AUFNR")) > 0) aufnrcnt++;
	}
	for(var i=0; i<ds_posid.RowCount(); i++)
	{
		if(length(ds_posid.GetColumn(i,"POSID")) > 0) posidcnt++;
	}

	if(cal_fdt.Value == '' && ed_aufnr.Text == '' && posidcnt == 0 && ed_posid.Text == '' && aufnrcnt == 0)
	{
		gfn_showMsg("CW00207","출하예정일/오더번호/WBS호기");
		return;	
	}
	if(cb_arbpl.Value == '') ds_cond.SetColumn(0,"ARBPL","*"); 
	ds_cond.SetColumn(0,"RBT",rdo_rbt.Value);
	ds_cond.SetColumn(0,"PART",rdo_part.Value);
	if(length(ds_cond.GetColumn(0,"AUFNR")) == 7)
		ds_cond.SetColumn(0,"AUFNR",'00000'+ds_cond.GetColumn(0,"AUFNR"));
	
	if(ds_cond.GetColumn(0,"FLAG") != '3' && ds_cond.GetColumn(0,"RBT") == '1') grd_list.Format = "Default";
	else if(ds_cond.GetColumn(0,"FLAG") != '3' && ds_cond.GetColumn(0,"RBT") == '2') grd_list.Format = "Default1";
	else if(ds_cond.GetColumn(0,"FLAG") == '3' && ds_cond.GetColumn(0,"RBT") == '1') grd_list.Format = "Default2";
	else if(ds_cond.GetColumn(0,"FLAG") == '3' && ds_cond.GetColumn(0,"RBT") == '2') grd_list.Format = "Default3";
	else grd_list.Format = "Default";
	
	for(var i=0; i<ds_aufnr.RowCount(); i++)
	{
		if(length(ds_aufnr.GetColumn(i,"AUFNR")) == 7) ds_aufnr.SetColumn(i,"AUFNR",'00000'+ds_aufnr.GetColumn(i,"AUFNR"));
	}

	// sort mark 삭제 
	gfn_clearGridSortMark(grd_list);
	ds_list.row = -1;

	tit_clearActionInfo();
	tit_callService(
        "PP0102002A_ACT"
        , "query"
        , "ds_cond=ds_cond ds_posid=ds_posid ds_aufnr=ds_aufnr ds_item=ds_item"
        , "ds_list=ds_list"
        , ""
        , "fn_afterQuery"
        , false);
}
/********************************************************************************
* 기      능   : 저장
********************************************************************************/
function fn_save() {
	if ( ds_list.rowcount == 0 )
	{
		gfn_showMsg("CW00010");
		return;
	}
	if ( !gfn_isChangeDs(ds_list, true, "")){
		return;
	}

	for( var i=0; i<ds_list.rowcount; i++ )
	{
		if(ds_list.getColumn(i,"CHK") == '0') 
			ds_list.SetColumn(i,"CHK",'');
		else if(ds_list.getColumn(i,"CHK") == '1') 
			ds_list.SetColumn(i,"CHK",'X');
	}

	// 저장 처리
	tit_callService(
        "PP0102002A_ACT"
        , "save"
        , "ds_list=ds_list:A ds_cond=ds_cond"
        , "ds_list=ds_list"
        , "FLAG="+quote(cb_flag.Value) +" RBT="+quote(rdo_rbt.Value)
        , "fn_afterSave");
}
