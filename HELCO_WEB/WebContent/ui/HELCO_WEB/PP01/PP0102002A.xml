********************************************************************************/
function btn_copy_OnClick(obj)
{
	for(var i=0; i<ds_list.RowCount(); i++)
	{
		//MGVRG:계획량, LMNGA:기작업량, GMNGA:실작업량
		var gmnga = parseInt(ds_list.GetColumn(i,"MGVRG")) - parseInt(ds_list.GetColumn(i,"LMNGA"));
		if( gmnga > 0 ) ds_list.SetColumn(i,"GMNGA",gmnga);
	}
}
