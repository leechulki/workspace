/********************************************************************************
* 기      능   : 데이터셋 값 변경 시 - Flag 처리
********************************************************************************/
function ds_list_OnColumnChanged(obj,nRow,strColumnID,varOldValue,varNewValue,nPivotIndex)
{
	obj.fireevent = false;
	switch( strColumnID ) {
		case "BUDAT":
			ds_list.SetColumn(nRow,"CHK","1");
			break;
		case "LIFNR":
			ds_list.SetColumn(nRow,"CHK","1");
			break;
		case "PREIS":
			if(grd_list.Format == 'Default2')
			{
				ds_list.SetColumn(nRow,"AMUNT",parseInt(ds_list.GetColumn(nRow,"PREIS"))*parseInt(ds_list.GetColumn(nRow,"GMNGA")));
			}
			ds_list.SetColumn(nRow,"CHK","1");
			break;	
		case "GMNGA":
			if(parseInt(ds_list.GetColumn(nRow,"LMNGA")) + parseInt(ds_list.GetColumn(nRow,"GMNGA")) > parseInt(ds_list.GetColumn(nRow,"MGVRG")))
			{
				gfn_showMsg("CW00229");
				ds_list.SetColumn(nRow,"GMNGA",ds_list.GetOrgColumn(nRow,"GMNGA"));
				if(grd_list.Format == 'Default2')
				{
					ds_list.SetColumn(nRow,"AMUNT",parseInt(ds_list.GetColumn(nRow,"PREIS"))*parseInt(ds_list.GetColumn(nRow,"GMNGA")));
				}
				ds_list.SetColumn(nRow,"CHK","0");
				break;
			}
			if(grd_list.Format == 'Default2')
			{
				ds_list.SetColumn(nRow,"AMUNT",parseInt(ds_list.GetColumn(nRow,"PREIS"))*parseInt(ds_list.GetColumn(nRow,"GMNGA")));
			}
			ds_list.SetColumn(nRow,"CHK","1");
			break;
		case "REMAK":
			ds_list.SetColumn(nRow,"CHK","1");
			break;
	}

	obj.fireevent = true;	
}
