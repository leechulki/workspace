********************************************************************************/
function fn_save() 
{
    // 체크한 거래box를 새로운 dataset에 담기
	ds_list_0.ClearData();
	ds_list.fireevent = false;
	
	//trace(ds_list.SaveXML());
	for (var i = 0 ; i < ds_list.RowCount(); i++)
	{
		if (ds_list.GetColumn(i,"FLAG") == "I")
		{ 
			ds_list.SetColumn(i,"FODAT", replace(ds_list.GetColumn(i,"FODAT"),".",""));
	    }
	}
	
	// 저장할 데이터셋을 복사
	gfn_copyDs(ds_list, ds_list_0, "FLAG=I");
	//tit_showDs(ds_list_0,false);/// return;
	//trace( ds_list_0.savexml());
	
	ds_list.fireevent = true;

	if (ds_list_0.RowCount() < 1) {
		return;
	}
	
	if (!gfn_showConfirm("CI00008")) {
		ds_list_0.ClearData();
		return;
	}
	tit_clearActionInfo();

	// 저장 처리 
	tit_addMultiActionInfo("pp01:PP0102006A_I1");

    //   call 서비스에서 commit실행하여 commit 필요없음.
	tit_callService(
        ""
        , ""
        , "ds_list=ds_list_0:A"
        , ""
        , ""
        , "fn_afterSave");
	   
}

function fn_afterSave(errCode, errMsg){
	if ( errCode != 0 ) {
		gfn_showMsg("CE00001");
	} else {
		gfn_showMsg("CI00003");
		fn_query();
	}
}
