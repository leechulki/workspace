// Form Loading / Unloading 시 작업 정의
//=========================================================================================
function fn_OnLoadCompleted(obj) {
	// Form 로드 이벤트 맨 첫줄에 기술 
	gfn_initForm(true);
	// 현재 조회 일자 설정 
	cal_i_fdate.value = gfn_getCurrDate();
	cal_i_tdate.value = gfn_getAfterDate(cal_i_fdate.value, 7);
	

	
	gfn_showMsg("CI00007");
}


/********************************************************************************
* 기      능   : 공통코드가 조회된 후의 Callback 함수 
********************************************************************************/
function fn_completeInit(sCode, sDsObj, sCodeType) {
}
/********************************************************************************
* 기      능   : form UnloadCompleted 
********************************************************************************/
function fn_OnUnloadCompleted(obj) {
	return gfn_isFormUnload(obj);
}
//=========================================================================================
// [ PART 3 ]
// Main Process Event 정의(조회,입력,삭제,저장,인쇄,form닫기)
//=========================================================================================

/********************************************************************************
* 기      능   : 조회
********************************************************************************/
function fn_query() {
	// 필수 입력사항 및 일자 체크 
	if ( !gfn_checkFromToObj(cal_i_fdate, cal_i_tdate, true, st_date.value,,true) ) {
		return;
	}	

	if (length(ed_pspid.value) > 0 && length(ed_pspid.value) < 6){
		gfn_showAlert("CW00092");
		ed_pspid.SetFocus();
		return;
	}


	tit_clearActionInfo();
	
	tit_addSearchActionInfo("pp01:PP0102017A_S1");
	
	
	tit_callService(
        ""
        , ""
        , "ds_cond=ds_cond"
        , "ds_list=ds_list"
        , ""
        , "fn_afterQuery"
        , true);

}

/********************************************************************************
* 기      능   : 저장 
********************************************************************************/
function fn_save() {

	// 체크한 거래box를 새로운 dataset에 담기
	ds_list_0.ClearData();

	ds_list.fireevent = false;
	var exist_data = 0;
	
	//trace(ds_list.SaveXML());
	for (var i = 0 ; i < ds_list.RowCount(); i++)
	{
		 if (ds_list.GetColumn(i,"CHECK") != "1" && length(ds_list.GetColumn(i,"POSID")) >= 9)
		 {
			 ds_list.SetColumn(i,"CHECK", "1");
		 }
		

		for (var j = 2; j < grd_list.GetCellCount('head'); j++)
		{
			if (length(grd_list.GetCellValue(i, j)) > 0)
			{
				exist_data++;
			}
		}


		exist_data = 0;

	}

	// 저장할 데이터셋을 복사
	gfn_copyDs(ds_list, ds_list_0, "CHECK=1");
	//tit_showDs(ds_list_0,false);/// return;

	ds_list.fireevent = true;

	if (ds_list_0.RowCount() < 1) {
		return;
	}
	
	if (!gfn_showConfirm("CI00008")) {
		ds_list_0.ClearData();
		return;
	}

	tit_clearActionInfo();

	// 저장 처리 

	tit_addMultiActionInfo("pp01:PP0102017A_I1");

//   call 서비스에서 commit실행하여 commit 필요없음.

	tit_callService(
        ""
        , ""
        , "ds_list=ds_list_0:A"
        , ""
        , ""
        , "fn_afterSave");
}

