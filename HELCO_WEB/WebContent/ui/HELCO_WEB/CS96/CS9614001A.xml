function fn_OnLoadCompleted(obj) {
	// Form 로드 이벤트 맨 첫줄에 기술 
	gfn_initForm();
	gfn_initDs(ds_cond, true);
	
	fn_queryblock();
	ck_blk.Value = false;
	
	var codeArr = ["1"];
    var dsObjArr = [ds_gbn];
    var addTypeArr = [G_CODE_ALL];
    gfn_searchCommCode(codeArr, dsObjArr, addTypeArr);

	// Grid Format 지정 
    if (G_USER_GROUP_R == "")
    {
		grd_list.Format = "MM";
    }
    else
    {
		grd_list.Format = "Default";
    }
    
	/**************************/
	/* 호기 속도정보 설정 필요*/
	/**************************/
	
	// 호기 속도정보 : parent에서 팝업 호출시 argument 로 fa_hty 전송
	//                                        fa_hty  값이 '' 이면 이미지 모두 볼수 있도록 구성

	//if (isExistVar("fa_hty", true) ) {
	if (length(fa_hty)>0 ) {
		//일반중속(true)과 고속(false) 분리 처리
		var speed;
		
		if (fa_hty == "Y")
			speed = false;
		else 
			speed = true;
			
		if(speed) {
			//alert("일반중속");
			v_speed.Text='0';
			Tab00.tab1.Enable = false;
			Tab00.tab2.Enable = true;
			Tab00.tab3.Enable = true;
			Tab00.TabIndex = 1; 
		} else {
			//alert("고속");
			v_speed.Text='1';
			Tab00.tab1.Enable = true;
			Tab00.tab2.Enable = false;
			Tab00.tab3.Enable = true;
			Tab00.TabIndex = 0; 
		}
	
	} else {
		v_speed.Text = "";
		Tab00.TabIndex = 0; 
	}
	
	v_hogin.Text = fa_hogin;    
}
