function fn_query() {


    //2019.11.26검색조건 데이터 체므로직 추가
	var Str = replace(trim(ed_value.Text), "%", "");

	// 2자리로 조회값 제약조건 추가
	//if(length(trim(ed_value.Text)) < 2) {
	if(length(trim(Str)) < 2) {
		gfn_showAlert("최소2글자 이상의 조회값을 입력해주십시오.");
		return;
	}
	
	ds_list.ClearData();
	
	ds_cond.SetColumn(0, "VALS", "%"+ed_value.Text+"%");
	
	/**********************/
	/* 호기 정보 설정 필요*/
	/**********************/

	if (v_hogin.Text.length > 5) {
		ds_cond.SetColumn(0, "WOKNUM" , v_hogin.Text);
	}
	
	tit_clearActionInfo();
	tit_addSearchActionInfo("cs96:CS9614001A_S1", null, true);
	
	
	if (ck_blk.Value==1) {


		var q_string = null;
		
		//test block 값 가져오기
		
		var idx = idx_value.Value;
		
		
/*		
		for (i=1; i < 15 ; i++) {
			var blk = ds_block.GetColumn(idx, "CS612_BLK"+i);
			
			if(i==1 && length(blk)>0)
				q_string = " AND (A.BLOCK_NO LIKE '%"+blk+"%' ";
			else if (length(blk) > 0)
				q_string += " OR A.BLOCK_NO LIKE '%"+blk+"%' ";
			else {
				q_string += ") ";
				break;
			}
		}
*/		
		q_string = " AND (A.BLOCK_NO LIKE '%";
		
		for (i=1; i < 15 ; i++) {
			var blk = ds_block.GetColumn(idx, "CS612_BLK"+i);
			
			if(i==1 && length(blk)>0)
				q_string += blk+"%' ";
			else if (length(blk) > 0)
				q_string += " OR A.BLOCK_NO LIKE '%"+blk+"%' ";
			else {
				break;
			}
		}
		q_string += ") ";
		
		
		
		//alert(q_string);
		
	
		tit_callService(
			  ""
			, ""
			, "ds_cond=ds_cond"
			, "ds_list=ds_list"
			, "BLOCK=" + quote( q_string )
			, "fn_afterQuery"
			, false
			, true);
	} else {
	// 일반 조회
		tit_callService(
			  ""
			, ""
			, "ds_cond=ds_cond"
			, "ds_list=ds_list"
			, ""
			, "fn_afterQuery"
			, true);
		//ck_blk.Text = "";
	}
}

