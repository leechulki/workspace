{
	gfn_changeFlag( obj, strColumnID, nRow, "FLAG" );
}

function btn_calc_OnClick(obj) {
	if(length(trim(med_amt_m.Value)) == 0) {
		gfn_showAlert("공급가액을 입력하세요.");
		return;
	} else if(length(trim(med_rto.Value)) == 0) {
		gfn_showAlert("비율을 입력하세요.");
		return;
	} else {
		med_amt_g.Value = Round(ToNumber(med_amt_m.Value) * (ToNumber(med_rto.Value) / 100), 0);
	}
}
