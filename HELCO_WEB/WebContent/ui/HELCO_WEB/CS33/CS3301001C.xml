/********************************************************************************
* 기      능   : 복사 75
********************************************************************************/
function fn_copy() {
	gfn_clearMsgBar();

	var t_yymm_m = med_yymm_m.value;
	var t_yymm_g = med_yymm_g.value;
	var t_yymm_fm = cb_yymm_fm.value;
	var t_yymm_to = cb_yymm_to.value;

	if((length(t_yymm_m) > 0 && length(t_yymm_m) < 6) || (length(t_yymm_g) > 0 && length(t_yymm_g) < 6)) {
		gfn_showMsg("CW00009");
		return;
	}

	if(parseInt(substr(t_yymm_m,4,6)) > 12 || parseInt(substr(t_yymm_g,4,6)) > 12) {
		gfn_showMsg("CW00009");
		return;
	}

	var g_cnt = parent.ds_list.RowCount();

	for(var i=0; i<g_cnt; i++) {
		var t_jym = parent.ds_list.GetColumn(i, "JYM");
		var t_tis = parent.ds_list.GetColumn(i, "TIS");
		var t_pst = parent.ds_list.GetColumn(i, "PST");
		var t_yymm_s = t_yymm_fm;
		var t_yymm_e = t_yymm_to;

		//if(t_yymm_s <= t_jym && t_yymm_e >= t_jym && t_tis == '') {
		if(t_yymm_s <= t_jym && t_yymm_e >= t_jym) {
			if(length(med_yymm_m.value) == 6 && t_tis == '') {
				parent.ds_list.SetColumn(i, "YYM", med_yymm_m.value);
			}
			if(length(ed_ydt.value) == 2 && t_tis == '') {
				parent.ds_list.SetColumn(i, "YDT", ed_ydt.value);
			}
			if(med_amt_m.value > '' && t_tis == '') {
				parent.ds_list.SetColumn(i, "AMT", med_amt_m.value);
			}			
			if(med_hmt.value > '' && t_tis == '') {
				parent.ds_list.SetColumn(i, "HMT", med_hmt.value);
			}
			if(med_dmt.value > '' && t_tis == '') {
				parent.ds_list.SetColumn(i, "DMT", med_dmt.value);
			}
			if(med_acmt.value > '' && t_tis == '') {
				parent.ds_list.SetColumn(i, "ACMT", med_acmt.value);
			}			
			if(length(med_yymm_g.value) == 6 && t_pst <= 'A1') {
				parent.ds_list.SetColumn(i, "GYM", med_yymm_g.value);
			}
			if(med_amt_g.value > '' && t_pst <= 'A1') {
				parent.ds_list.SetColumn(i, "BAM", med_amt_g.value);
			}
			if(med_hmt.value > '' && t_pst <= 'A1') {
				parent.ds_list.SetColumn(i, "HAM", med_hmt.value);
			}
			if(med_dmt.value > '' && t_pst <= 'A1') {
				parent.ds_list.SetColumn(i, "DAM", med_dmt.value);
			}
			if(med_acam.value > '' && t_pst <= 'A1') {
				parent.ds_list.SetColumn(i, "ACAM", med_acam.value);
			}			
			if(med_amt_i1.value > '' && t_pst <= 'A1') {
				parent.ds_list.SetColumn(i, "IY1", med_amt_i1.value);
			}
			if(med_amt_i2.value > '' && t_pst <= 'A1') {
				parent.ds_list.SetColumn(i, "IY2", med_amt_i2.value);
			}
			if(med_amt_i3.value > '' && t_pst <= 'A1') {
				parent.ds_list.SetColumn(i, "IY3", med_amt_i3.value);
			}
			if(med_amt_i4.value > '' && t_pst <= 'A1') {
				parent.ds_list.SetColumn(i, "IY4", med_amt_i4.value);
			}
			if(med_amt_ret.value > '' && t_pst <= 'A1') {
				parent.ds_list.SetColumn(i, "RET", med_amt_ret.value);
			}
		}
	}

	fn_close();
}
