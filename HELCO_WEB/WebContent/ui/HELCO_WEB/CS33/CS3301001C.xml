	ds_code_cond.setColumn(0, "USD", substr(usd,0,6));
	ds_code_cond.setColumn(0, "UHJ", substr(uhj,0,6));

	tit_clearActionInfo();
	tit_addSearchActionInfo("cs33:CS3301001C_S1");

	tit_callService(
        ""
        , ""
        , "ds_cond=ds_code_cond"
