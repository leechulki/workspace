	<Script><![CDATA[/*********************************************************
*	Http Error 처리
*********************************************************/
function gEvent_OnHttpError(obj, url, errorcode, errormsg)
{
	switch (errorcode)
	{
		case -2085601261:
				Alert("HTTP 503: Service unavailable, 외부 서비스가 죽었거나 현재 멈춤 상태");
				G_LOGOUT="0";
				Exit();
				break;
		case -2085601262:
				Alert("HTTP 500: Internal server error, 내부서버 오류(잘못된 스크립트 실행시)");
				break;
		case -2085601263:
				Alert("HTTP405: Method not allowed, 리소스를 허용안함 ");
				break;
		case -2085601264:
				Alert("HTTP 404: Not found, 문서를 찾을 수 없음 ");
				break;
		case -2085605317:
				Alert("Network에 이상이 있는지,경로가 맞는지 확인해주시기 바랍니다."); //was다운됨
				G_LOGOUT="0";
				Exit();
				break;
	}
}

/*********************************************************
*	Domain 정보 Load 시킴.
*********************************************************/
function gfn_loadDomain(userLang)
{
	var url = G_SERVER_URL + "lang/domain";
	//var url = "./lang/domain"; 제우스사용시
	if ( length(userLang) == 0 )
	{
		userLang = G_LANG;
	}

	if ( userLang != null && userLang != "en")
	{
		url += "_" + userLang;
	}

	url += ".xml";
    
    global.loadDomain(url);
}


/********************************************************************************
* 기      능   :  자동 로그아웃 처리
********************************************************************************/
function win_close_OnClick(obj,nX,nY)
{
	// 서버에 조회 
	http.Sync = true;
	
	Transaction(
		  "logout"
		, G_SERVER_URL + "Login"
		, ""
		, "" 
		, "cmd='logout'"
		, "fn_logout");
		
	http.Sync = false;
}]]></Script>
