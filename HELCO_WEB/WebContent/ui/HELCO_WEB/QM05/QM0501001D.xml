function ds_list_OnColumnChanged(obj,nRow,strColumnID,varOldValue,varNewValue,nPivotIndex)
{
  	obj.fireevent = false;
  	
   	switch( strColumnID ) 
	{
	    case "DAESU" :
	       ds_list.SetColumn(nRow, "DAESU_T", ToFloat(ds_list.GetColumn(nRow, "DAESU")) +  ToFloat(ds_list.GetColumn(nRow, "DAESU_C")));
	       ds_list.SetColumn(nRow, "DAESU_J", ToFloat(ds_list.GetColumn(nRow, "DAESU_T")) - ToFloat(ds_list.GetColumn(nRow, "DAESU_H")));
	       break;
	    
	   case "DAESU_C" :
           ds_list.SetColumn(nRow, "DAESU_T", ToFloat(ds_list.GetColumn(nRow, "DAESU")) +  ToFloat(ds_list.GetColumn(nRow, "DAESU_C")));
	       ds_list.SetColumn(nRow, "DAESU_J", ToFloat(ds_list.GetColumn(nRow, "DAESU_T")) - ToFloat(ds_list.GetColumn(nRow, "DAESU_H")));
	       break;
	 }
	
	
	obj.fireevent = true;
}

