********************************************************************************/
function fn_save() {

 /* if(ds_list.FindRow("CHECK", 1)==-1){
		gfn_showMsg('CW00046');
		return;
   }*/
  for(var i=0; i<ds_list.GetRowCount(); i++)
    {		
       ds_list.SetColumn(i,"MDATE", ds_cond.GetColumn(0,"MDATE"));
	 	// 필수 입력 항목	
		if(ds_list.GetColumn(i, "DAESU") < 0 ){ 
			var arr = [parseInt(i) + 1, "검사가능대수"];
			gfn_showMsg("CW00224", arr);
			return;
		 }
		if(ds_list.GetColumn(i, "DAESU_C") < 0 ){
			var arr = [parseInt(i) + 1, "검사추가대수"];
			gfn_showMsg("CW00224", arr);
			return;
		 }		
		if(ds_list.GetColumn(i, "ZDATE") < 19990101 ){
			var arr = [parseInt(i) + 1, "마감일"];
			gfn_showMsg("CW00224", arr);
			return;
	    }
	    
	     if(length(ds_list.GetColumn(i,"TIME")) < 2)
         {// alert("a");
           ds_list.SetColumn(i,"MTIME",'0' + ds_list.GetColumn(i,"TIME") + '0000');
         }
          else
         { //alert("b");
           ds_list.SetColumn(i,"MTIME", ds_list.GetColumn(i,"TIME") + '0000');
         }
 	}
 	
   for(var i=0; i<ds_list2.GetRowCount(); i++)
    {	
       ds_list2.SetColumn(i,"MDATE", ds_cond.GetColumn(0,"MDATE"));
	}
  
	
  if(!gfn_showConfirm( "CI00014", "해당 데이터" )) return;	
 
  

    
   tit_clearActionInfo();
   tit_addSelectExActionInfo("qm05:QM0501001D_S3", "qm05:QM0501001D_I1",  "qm05:QM0501001D_U1");
   tit_callService(
		  ""	
		, ""
		, "ds_list=ds_list"// ds_list2=ds_list2"
		, ""
		, ""
		, ""//"fn_afterSave"
		, ""
		, ""
		, true  // ""
    );
    
    
      tit_clearActionInfo();
      tit_addSingleActionInfo("qm05:QM0501001D_D1"); // 
	  tit_addSingleActionInfo("qm05:QM0501001D_I2");  // 
	   tit_callService(
	    ""
	  , ""
	  , "ds_list2=ds_list2"
	  , ""
	  , ""
	  , "fn_afterSave"
	  , true);
 
}

