/********************************************************************************
* 기      능   : mail
********************************************************************************/
function btn_send_OnClick(obj)
{
	if(!gfn_confirm("문자를 발송하시겠습니까?")){
		return;
	}
	
	tit_clearActionInfo();
	tit_addSearchActionInfo("ps15:PS1505001G_S1");// 문자발송 일시 SELECT
	tit_callService(
          ""
        , ""
        , "ds_cond=ds_cond"
        , "ds_datetime=ds_datetime"
        , ""
        , ""
        , ""
        , ""
        , true);
        
	ds_sendMms.ClearData();
	
	for(var i=0; i < ds_send.GetRowCount(); i++)
	{
		// PM & 팀장 발송
		var nAddRow = ds_sendMms.AddRow();
		
		// 1.팀장
		ds_sendMms.setColumn(nAddRow,"TRAN_ID",G_USER_ID);
		ds_sendMms.setColumn(nAddRow,"TRAN_PHONE", Replace(ds_send.GetColumn(i,"CELLPT"),"-",""));
		//ds_sendMms.setColumn(nAddRow,"TRAN_PHONE", "01071601959");
		ds_sendMms.setColumn(nAddRow,"TRAN_CALLBACK", "1577-0603");
		ds_sendMms.setColumn(nAddRow,"TRAN_ETC1","WEB_APPROVAL");
		ds_sendMms.setColumn(nAddRow,"TRAN_ETC2", "");	
		ds_sendMms.setColumn(nAddRow,"SUBJECT", "");
		ds_sendMms.SetColumn(nAddRow, "TRAN_MSG", ds_longMsg.GetColumn(0, "TEXT0")+'-'+ds_send.GetColumn(i, "MSG_1")+"\n"+ds_longMsg.GetColumn(0, "TEXT1"));		
		ds_sendMms.SetColumn(nAddRow,"TRAN_TIME", ds_datetime.GetColumn(0, "DATETIME"));
		
		nAddRow = ds_sendMms.AddRow();
		
		// 2.PM
		ds_sendMms.setColumn(nAddRow,"TRAN_ID",G_USER_ID);
		ds_sendMms.setColumn(nAddRow,"TRAN_PHONE", Replace(ds_send.GetColumn(i,"PM_CELLP"),"-",""));
		//ds_sendMms.setColumn(nAddRow,"TRAN_PHONE", "01071601959");
		ds_sendMms.setColumn(nAddRow,"TRAN_CALLBACK", "1577-0603");
		ds_sendMms.setColumn(nAddRow,"TRAN_ETC1","WEB_APPROVAL");
		ds_sendMms.setColumn(nAddRow,"TRAN_ETC2", "");	
		ds_sendMms.setColumn(nAddRow,"SUBJECT", "");		
		ds_sendMms.SetColumn(nAddRow, "TRAN_MSG", ds_longMsg.GetColumn(0, "TEXT0")+'-'+ds_send.GetColumn(i, "MSG_1")+"\n"+ds_longMsg.GetColumn(0, "TEXT1"));		
		ds_sendMms.SetColumn(nAddRow,"TRAN_TIME", ds_datetime.GetColumn(0, "DATETIME"));
	}
			
	tit_clearActionInfo();
		
	//저장 처리 
	tit_addMultiActionInfo("ps15:PS1505001G_I1");
		
	tit_callService(
		"PDA_ACT"
		, ""
		, "ds_sms=ds_sendMms:A"
		, ""
		, ""
		, "fn_afterSend"
		, true
		, true
		, true);
   
        	
}

function fn_afterSend(errCode, errMsg)
{
	if ( errCode != 0 ) // 실패 
	{
		gfn_showMsg("CE00001");
	} 
	else // 성공
	{
		pop_meg = '문자가 발송되었습니다';		
			
		gfn_showInform("CW10025", pop_meg);		
	}
	
}

function btn_close_OnClick(obj)
{
	fn_close();
}
