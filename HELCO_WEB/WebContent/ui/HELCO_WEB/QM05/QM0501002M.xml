	// Form 로드 이벤트 맨 첫줄에 기술 
	gfn_initForm();

	//btn_send.Enable = false;
	ds_send.Copy(ds_mms);
	
	ds_longMsg.SetColumn(0, "TEXT0", "스케줄확정일자안내");
	ds_longMsg.SetColumn(0, "TEXT1", ds_send.GetColumn(0, "TEXT1"));
	
	
	for(var i=0;i<ds_send.GetRowCount();i++)
	{
		var sMsg1 = "";
		var sMsg2 = "";
		
		if(ds_send.GetColumn(i, "ART") == '8950')
		{
			sMsg1 = "설중-"+ds_send.GetColumn(i, "PARQDT")+"-"
			           +ds_send.GetColumn(i, "POST1")+"-"+ds_send.GetColumn(i, "HOGI")+"-"+ds_send.GetColumn(i, "SEQ")+"차"+"-"+ds_send.GetColumn(i, "TEMNONM");
		}
		else
		{	
			sMsg1 = "설완-"+ds_send.GetColumn(i, "PARQDT")+"-"
			           +ds_send.GetColumn(i, "POST1")+"-"+ds_send.GetColumn(i, "HOGI")+"-"+ds_send.GetColumn(i, "SEQ")+"차"+"-"+ds_send.GetColumn(i, "TEMNONM");
			
		}
		
		ds_send.SetColumn(i, "MSG_1", sMsg1);
		
	}

	
}
