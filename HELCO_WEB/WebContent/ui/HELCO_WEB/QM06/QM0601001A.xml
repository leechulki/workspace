function fn_OnLoadCompleted(obj) {
	// Form 로드 이벤트 맨 첫줄에 기술 
	gfn_initForm(true);

	var user_grp  = "";
	var user_grp1 = ""; //영업
	var user_grp2 = ""; //설치
	var user_grp3 = ""; //보수
	var user_grp4 = ""; //자재

    	
    user_grp1 = G_USER_GROUP_B; //영업
	user_grp2 = G_USER_GROUP_E; //설치
	user_grp3 = G_USER_GROUP_R; //보수
	user_grp4 = G_USER_GROUP_M; //자재

	user_grp = G_USER_GROUP;

	if(G_VEND_CODE == "0000009922") { //정보기술실
		user_grp = "Z40";
		bt_ipgo.Enable = true;
		btn_direct_into.Enable = true;
		btn_shipping_date.Enable = true;
		btn_arrival.Enable = true;	
	} else if(user_grp1 > "") { //영업
		btn_direct_into.Enable = false;
		btn_shipping_date.Enable = false;
		btn_arrival.Enable = false;
		user_grp = user_grp1;
	} else if(user_grp2 > "") { //설치
		btn_direct_into.Enable = false;
		btn_shipping_date.Enable = true;
		btn_arrival.Enable = true;	
		user_grp = user_grp2;
	} else if(user_grp3 > "") { //보수
		btn_direct_into.Enable = true;
		btn_shipping_date.Enable = false;
		btn_arrival.Enable = false;	
		user_grp = user_grp3;
	} else if(user_grp4 > "") { //자재
		bt_ipgo.Enable = true;
		btn_direct_into.Enable = true;
		btn_shipping_date.Enable = false;
		btn_arrival.Enable = false;	
		user_grp = user_grp4;
	} else {
		btn_direct_into.Enable = false;
		btn_shipping_date.Enable = false;
		btn_arrival.Enable = false;	
	}
    
	// 사용그룹 처리
	user_grp = gfn_getUserGroup();
	if(user_grp == '10') 
	{
		ds_cond.SetColumn(0,"GBN","10");
		ds_cond.SetColumn(0,"I_LIFNR",G_VEND_CODE_M);
		grd_list.SetColProp(42,"width",0);
		grd_list.SetColProp(43,"width",0);
		grd_list.SetColProp(44,"width",0);
	} 
	else if(user_grp == '30' || user_grp == '40') 
	{
		ds_cond.SetColumn(0,"GBN","");
	}
    
	// 공급 업체명 설정 
	// ed_lifnrName.value = G_VEND_NAME;
	// ds_cond.setColumn(0, "I_LIFNR", G_VEND_CODE_M);
	
	// cal_i_credt2.value = gfn_getCurrDate();
	// cal_i_credt1.value = gfn_getAfterDate(cal_i_credt2.value, -7);	

	// cal_i_appdt2.value = gfn_getCurrDate();
	// cal_i_appdt1.value = gfn_getAfterDate(cal_i_appdt2.value, -7);	

	gfn_showMsg("CI00007");
}


