/********************************************************************************
* 기      능   : 데이터셋 값 변경 시 - 자재 상세 조회입력,Flag 처리 
********************************************************************************/

function ds_list_a1_OnColumnChanged(obj,nRow,strColumnID,varOldValue,varNewValue,nPivotIndex)
{

  if ( strColumnID <> "CHECK"){
	
	ds_list.fireevent = false;
	var colid = strColumnID;
	
	
	if(colid == "POSID") {
		ds_cond2.SetColumn(0,"POSID",varNewValue);
		ds_cond2.SetColumn(0,"CNT",nRow);
		
		//현장명 조회
		tit_clearActionInfo();
		tit_addSearchActionInfo("mm08:MM0801001A_S2");
			
		tit_callService(
		""
		, ""
		, "ds_cond2=ds_cond2"
		, "ds_gubun=ds_gubun"
		, ""
		, "fn_refresh");
		
	}
	
	if(colid == "MATNR") {
	
		ds_cond3.SetColumn(0,"MATNR",varNewValue);
		ds_cond3.SetColumn(0,"CNT",nRow);
		
		//자재 정보조회
		tit_clearActionInfo();
		tit_addSearchActionInfo("mm08:MM0801001A_S3");
			
		tit_callService(
		""
		, ""
		, "ds_cond3=ds_cond3"
		, "ds_gubun2=ds_gubun2"
		, ""
		, "fn_refresh2");
	}
	
	if(colid == "LIFNR") {
	
		ds_cond4.SetColumn(0,"LIFNR",varNewValue);
		ds_cond4.SetColumn(0,"CNT",nRow);
		
		//업체 정보조회
		tit_clearActionInfo();
		tit_addSearchActionInfo("mm08:MM0801001A_S5");
			
		tit_callService(
		""
		, ""
		, "ds_cond4=ds_cond4"
		, "ds_gubun3=ds_gubun3"
		, ""
		, "fn_refresh3");
	}
	
	if ( Trim(obj.GetColumn(nRow,colid)) != Trim(ds_list2.GetColumn(nRow,colid)))
	{
		if(colid != "DIVDAT") {
		
			obj.SetColumn(nRow,"CHECK","1");
		}
	} else
	{
		obj.SetColumn(nRow,"CHECK","0");
	}
	
	ds_list.fireevent = true;
	
    }
	
}

/********************************************************************************
* 기      능   : 컬럼별 입력 속성
********************************************************************************/
var col = "";
function grd_list_OnExpandEdit(obj,nRow,nCell,strVal,nPivotIndex)
{
	//업체정보
	if(nCell = 11)
	{
		if (G_DPT1 == "D4" || G_DPT1 == "D5" || G_USER_ID == "2023091") 
		{
			var rtn2 = gfn_openPopup("WB0101001C",true);
		
			 if (rtn2 != null)
			 {
				ds_list_a1.SetColumn(nRow,"LIFNR",rtn2[0]);
			 }
		 }
	}
	//PDM상의 배포한 도면번호
	if(nCell = 10)
	{
		if (G_DPT1 == "D4" || G_DPT1 == "D5" || G_USER_ID == "2023091") 
		{
			var zzeinr = gfn_openPopup("MM0901007A", true);
			
			if (zzeinr != null && zzeinr != "0")
			{
				ds_list_a1.SetColumn(nRow,"P_ZEINR",zzeinr);
			}
		}
	}
	if(nCell = 12 || nCell = 14)
	{
		//견적요청일, 견적완료일 달력 선택
		if (G_DPT1 == "D4" || G_DPT1 == "D5" || G_USER_ID == "2023091")
		{
			var rect = obj.GetCellRect(nRow, ncell);
			cal_temp.top = rect[1] +60 ;
			cal_temp.Left = rect[0];
			cal_temp.Visible = true;
			
			
			switch ( nCell )
			{
				case 12 : 
					cal_temp.Value = ds_list_a1.GetColumn(nRow, "RQDAT");
					col = "RQDAT";
				break;
				
				case 14 :
					cal_temp.Value = ds_list_a1.GetColumn(nRow, "CPDAT");
					col = "CPDAT";
				break;
				
			}
		}
		
	}
	cal_temp.SetFocus();

}
