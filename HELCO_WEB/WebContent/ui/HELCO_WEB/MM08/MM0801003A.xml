/********************************************************************************
* 기      능   : 저장
********************************************************************************/
function fn_save() {

   if ( !gfn_isChangeDs(ds_list_a1, true) ) {
		return;
	}
	//  자료가 없으면 리턴
	if (ds_list_a1.rowcount < 1) {
		gfn_showAlert("CW00010");
		return;
	}
	// 저장여부 확인
	if (!gfn_showConfirm("CI00010")) {
		return;
	}
	
	ds_list_a1.fireevent = false;
	
	gfn_copyDs(ds_list_a1, ds_list_a2, "CHECK=1");
	
	var cnt = ds_list_a2.GetRowCount();
	
	if (cnt > 0)
	{
		for( var i = 0; i < ds_list_a2.GetRowCount(); i++ ){
		
			if ( length(ds_list_a2.GetColumn(i,"MATNR")) < 1 ) 
			{
				gfn_showAlert("CW00001","자재번호");
				ds_list_a1.fireevent = true;
				return;
			}
			if ( length(ds_list_a2.GetColumn(i,"LIFNR")) < 1 )
			{
				gfn_showAlert("CW00001","업체번호");
				ds_list_a1.fireevent = true;
				return;
			}
			if ( length(ds_list_a2.GetColumn(i,"P_ZEINR") != 8) )
			{
				gfn_showAlert("CW00011","8");
				ds_list_a1.fireevent = true;
				return;
			}
			
			if ( length(ds_list_a2.GetColumn(i,"RQDAT") != 8) )
			{
				gfn_showAlert("CW00011","8");
				ds_list_a1.fireevent = true;
				return;
			}
			if ( ds_list_a2.GetColumn(i,"ZKIND") == "2" && ds_list_a2.GetColumn(i,"NETPR_IF") == "" ) {
				gfn_showAlert("CW00002","종전단가","종전자재 선택");
				ds_list_a1.fireevent = true;
				return;
			}
		}
	}
	
	ds_list_a1.fireevent = true;
	
	tit_clearActionInfo();
	tit_addSelectExActionInfo(
		"mm08:MM0801001A_S4"
	   ,"mm08:MM0801001A_I1"
	   ,"mm08:MM0801001A_U1");
	   
	   
	
	//저장 처리 
	tit_callService(
		""
		, ""
		, "ds_list_a2=ds_list_a2:A"
		, ""
		, ""
		, "fn_afterSave");

}
