						<cell align="right" bkcolor2="user22" col="10" display="text" expr="NumFormat(round(SJ_MS/DAMT_T*100,&#32;0))+&apos;%&apos;"/>
						<cell align="right" bkcolor2="user22" col="11" display="number" expr="round(JMAMT_T+DAMT_T,0)"/>
