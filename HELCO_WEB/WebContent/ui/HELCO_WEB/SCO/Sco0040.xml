	<Script><![CDATA[/*
 ******************************************************************************************
 * 기      능   : 
 * 작  성  자   : 
 * 작성  일자   : 
 * ----------------------------------------------------------------------------------------
 * HISTORY    :
 ******************************************************************************************
 */
#include "LIB::commons.js";	// 공통 스크립트 
#include "LIB::nsfRequest.js";	// 공통 스크립트 
#include "LIB::common_hdel.js";	// 공통 스크립트  /* GNY 추가 */

//=========================================================================================
// [ PART 1 ]
// Form에서 사용될 전역변수를 선언
// FORM에서 사용되는 전역변수는 대문자로 작성하며, F_XXX 의 형태를 갖도록 한다. 
//=========================================================================================

//=========================================================================================
// [ PART 2 ]
// Form Loading / Unloading 시 작업 정의
//=========================================================================================
function fn_OnLoadCompleted(obj) {

	gfn_initForm(true);	// Form 로드 이벤트 맨 첫줄에 기술 
	gfn_translation(obj, G_LANG);
	fn_init_form(); 	// 폼 초기화
}

/********************************************************************************
* 기      능   : form UnloadCompleted 
********************************************************************************/
function fn_OnUnloadCompleted(obj) {
	return gfn_isFormUnload(obj);
}

//=========================================================================================
// [ PART 3 ]
// Main Process Event 정의(조회,입력,삭제,저장,인쇄,form닫기)
//=========================================================================================
/********************************************************************************
* 기      능   : 조회
********************************************************************************/
function fn_query() {

	if ( gfn_isNull(cal_ymd, true, st_ymd.Value, cal_ymd) ) {
		return false;
	}
	
	ds_cond.DeleteAll();
	ds_cond.AddRow();
	
	ds_cond.setColumn(0, "YMD"   , cal_ymd.Value);
	ds_cond.SetColumn(0, "REALT" , decode(rd_gubun.Value, 1, "X", ""));
	ds_cond.SetColumn(0, "DUED"  , decode(rd_gubun.Value, 2, "X", ""));
	
	nsfRequest("list", "Sco0040/find", "ds_cond=ds_cond", "ds_list65=ds_list65 ds_list66=ds_list66 ds_error=ds_error", "", "fn_afterQuery");
    fn_createWait("",this);     // 처리중입니다. 메시지 처리
}

/********************************************************************************
* 기      능   : 폼 초기화
********************************************************************************/
function fn_init_form() {

	ds_cond.ClearData();
	ds_list.ClearData();
	
	cal_ymd.Text = gfn_getCurrDate();
	
	// 그리드 헤더 설정 (당월, 전월)
	ufn_setGridHeader(cal_ymd.Value);
	
	// 팀 조회
	ds_frvg_cd.DeleteAll();
	ds_frvg_cd.AddRow();
	ds_frvg_cd.SetColumn(0,"CODE","E600"); // 리모델링 영업부
	nsfRequest("query", "combo/comboFrVg", "ds_frvg_cd=ds_frvg_cd", "ds_frvg_cd=ds_frvg_cd ds_error=ds_error", "", "fn_afterQuery_frvg");
}

/********************************************************************************
* 기      능   : 닫기  
********************************************************************************/
function fn_close() {
	close();
}

/********************************************************************************
* 기      능   : 액셀  
********************************************************************************/
function fn_excel() {

	// 조회 후 처리
	if ( ds_list.GetRowCount() == 0 )
	{
		// 조회 후 작업이 가능합니다.
		gfn_showAlert("CW00054");
		return false;
	}
	
	// Excel Export
	gfn_saveToExcel(grd_excel, true);
}

//=========================================================================================
// [ PART 4]
// Callback 함수 정의 : callback 함수명은 메인 함수의 fn_xxxx() 에서 xxxx 부분은 fn_afterXXXX()
// 형태가 되도록 작성 
//=========================================================================================
/********************************************************************************
* 기      능   : 조회 Callback  
********************************************************************************/
function fn_afterQuery(svcId, errCode, errMsg) {

	if(ds_error.rowcount() > 0) {
		gfn_showMsg("CE00001");
		gfn_openWSErrorWin(grd_list, ds_list, ds_error, "FLAG");	// error 정보 표시
		fn_destroyWait();
	} else {
		if(errCode != 0) {
			gfn_showMsg("CE00001");
			fn_destroyWait();
		} else {
			
			grd_list.Redraw = false;
			grd_excel.Redraw = false;
			
			// 그리드 헤더 설정 (당월, 전월)
			ufn_setGridHeader(ds_cond.GetColumn(0, "YMD"));
			
			// 목표, 실적 자료 통합
			ds_list.ClearData();
			for ( var i = 0 ; i < ds_list65.GetRowCount() ; i++ ) {
				// 목표(계획) 자료 추가
				var nRow = ds_list.AddRow();
				ds_list.CopyRow(nRow, ds_list65, i);
				
				var sGubun = ds_list65.GetColumn(i, "GUBUNA");
				var sVkGrp = ds_list65.GetColumn(i, "VKGRP");
				ds_list.SetColumn(nRow, "VKGRP2", iif( left(sVkGrp, 2) == "E6" && IsDigit(mid(sVkGrp, 2, 1)), "A", "B")); // 서울 지방 구분
				var sVkGrp2 = ds_list.GetColumn(i, "VKGRP2");
				
				// 명 설정 - 구분
				ds_list.SetColumn(i, "GUBUNA_NM", ds_gubuna_cd.GetColumn(ds_gubuna_cd.FindRow("CODE", sGubun), "CODE_NAME"));
				
				// 명 설정 - 팀
				var sVkGrpNm = ds_frvg_cd.GetColumn(ds_frvg_cd.FindRow("CODE", sVkGrp), "CODE_NAME");
				sVkGrpNm = replace(sVkGrpNm, "리모델링-", "");
				sVkGrpNm = replace(sVkGrpNm, "지사", "");
				ds_list.SetColumn(i, "VKGRP_NM", sVkGrpNm);
				
				// 명 설정 - 서울 지방
				ds_list.SetColumn(i, "VKGRP2_NM", ds_vkgrp2_cd.GetColumn(ds_vkgrp2_cd.FindRow("CODE", sVkGrp2), "CODE_NAME"));
				
				// 실적 자료 추가
				var nRow66 = ds_list66.SearchRow("VKGRP='" + sVkGrp + "'");
				if ( nRow66 > - 1 ) {
					ds_list.CopyRow(nRow, ds_list66, nRow66, "SJ_WMS=SJ_WMS,SJ_WNS=SJ_WNS"); // 원가
					switch (sGubun) { // 구분
						case "A" : // 수주
							ds_list.CopyRow(nRow, ds_list66, nRow66, "SJ_MS=SJ_MS,SJ_NS=SJ_NS");
							break;
						case "D": // 수금
							ds_list.CopyRow(nRow, ds_list66, nRow66, "SJ_MS=SG_MS,SJ_NS=SG_NS");
							break;
						case "G": // 매출
							ds_list.CopyRow(nRow, ds_list66, nRow66, "SJ_MS=MC_MS,SJ_NS=MC_NS");
							break;
						default :
							break;
					}
				}
			}
			ds_list.Sort("GUBUNA:A,VKGRP2:A,VKGRP:A");
			ds_list.GroupKey = "GUBUNA_NM,VKGRP2_NM";
			//trace(ds_list.SaveXML());
			//trace(ds_list65.SaveXML());
			
			grd_list.Redraw = true;
			grd_excel.Redraw = true;
			
			gfn_showMsg("CI00002", ds_list.rowCount + "");
			
			fn_destroyWait();
		}
	}
}
function fn_afterQuery_frvg(svcId, errCode, errMsg) {
	//trace(ds_frvg_cd.SaveXML());
}

//=========================================================================================
// [ PART 6 ]
// component Event 정의 : 각 Component 별 이벤트
//=========================================================================================
/********************************************************************************
* 기      능   : 그리드 헤더 설정 (당월, 전월)
********************************************************************************/
function ufn_setGridHeader(sYmd)
{
	if ( length(sYmd) == 8 ) {
		var sThisYear = mid(sYmd,0,4);
		var sPrevYear = toInteger(sThisYear)-1;
		var sThisMonth = mid(sYmd,4,2); // 당월
		var sPrevMonth = mid(addMonth(sYmd,-1),4,2); // 전월
		var sThisDay = mid(sYmd,6,2);
		
		var sNow = getDate();
		var sNowDate = mid(sNow,0,4)+"-"+mid(sNow,4,2)+"-"+mid(sNow,6,2);
		var sNowTime = mid(sNow,8,2)+":"+mid(sNow,10,2)+":"+mid(sNow,12,2);
		
		// grd_list 헤더 설정
		grd_list.SetCellProp("head", 2, "text", "전월누계 ("+sPrevMonth+"월)");
		grd_list.SetCellProp("head", 3, "text", sThisMonth+"월 실적");
		grd_list.SetCellProp("head", 7, "text", sPrevYear+"년\n"+sThisMonth+"월\n누계실적");
		
		// grd_excel 헤더 설정
		grd_excel.SetCellProp("head",  0, "text", sThisYear+"년도 리모델링 영업부 영업실적");
		grd_excel.SetCellProp("head",  1, "text", "("+sThisYear+"."+sThisMonth+"."+sThisDay+")");
		grd_excel.SetCellProp("head",  2, "text", "Date: "+sNowDate+"\nTime:"+sNowTime+"\n(단위: 백만원,%)");
		
		grd_excel.SetCellProp("head",  5, "text", "전월누계 ("+sPrevMonth+"월)");
		grd_excel.SetCellProp("head",  6, "text", sThisMonth+"월 실적");
		grd_excel.SetCellProp("head", 10, "text", sPrevYear+"년\n"+sThisMonth+"월\n누계실적");
	}
}
]]></Script>
