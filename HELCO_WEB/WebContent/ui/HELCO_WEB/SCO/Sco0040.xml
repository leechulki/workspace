						<cell align="right" bkcolor2="user22" col="17" colid="JYAMT_S" display="number" expr="round(JYAMT_S,0)"/>
						<cell align="right" bkcolor2="user22" col="18" display="text" expr="NumFormat(round((SJ_NS-JYAMT_S)/JYAMT_S*100,&#32;0))+&apos;%&apos;"/>
