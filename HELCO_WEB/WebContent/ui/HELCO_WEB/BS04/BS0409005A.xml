#include "LIB::commons.js";	// 공통 스크립트 
//=========================================================================================
// [ PART 1 ]
// Form에서 사용될 전역변수를 선언
// FORM에서 사용되는 전역변수는 대문자로 작성하며, F_XXX 의 형태를 갖도록 한다. 
//=========================================================================================
var userGroup = "";
//=========================================================================================
// [ PART 2 ]
// Form Loading / Unloading 시 작업 정의
//=========================================================================================
function fn_OnLoadCompleted(obj) {
	// Form 로드 이벤트 맨 첫줄에 기술 
	gfn_initForm();	
	
	userGroup = gfn_getUserGroup();

    //현재 년월
     cb_MAKFDT.Value = left(gfn_getCurrDate(), 6)+"01";
	 cb_MAKTDT.Value = addMonth(gfn_getCurrDate(), +1);
	//실적년월
     cb_MAKFDTD.Value = left(gfn_getCurrDate(), 6)+"01";
	 cb_MAKTDTD.Value = addMonth(gfn_getCurrDate(), +1);

	//기준일: 현재년월
	var curYear = substr(gfn_getCurrDate(),0,4);
	fn_makeYear(curYear,-10,1);
	cb_monthf.index = parseInt(substr(gfn_getCurrDate(),4,2))-1;
	
	cb_dpt1.Index = 0;
     
	ds_cond.SetColumn(0,"USER_GRUP",userGroup);
	if (userGroup == "10")
	{
		ds_cond.SetColumn(0,"USER_CODE",G_VEND_CODE_E);
	}
	else if (userGroup == "20")
	{
		ds_cond.SetColumn(0,"USER_CODE",G_USER_ID);
	}
	else if (userGroup == "30")
	{
		ds_cond.SetColumn(0,"USER_CODE",G_AREA_CODE_E);
	}
    
	fn_searchCode2();
 }


function fn_searchCode2() {
	tit_clearActionInfo();
	tit_addSearchActionInfo("bs04:BS0409002A_S2");
	tit_addSearchActionInfo("bs04:BS0409002A_S3");
	tit_callService(
		""
		, ""
		, ""
		, "ds_vkbur=ds_vkbur ds_vkgrp=ds_vkgrp"
		, ""
		, "fn_afterCode2Search"
		, true
		, true);
}


function fn_afterCode2Search(errCode, errMsg) 
{
	gfn_insertEmptyRow(ds_vkbur, G_CODE_ALL);
	gfn_insertEmptyRow(ds_vkgrp, G_CODE_ALL);
	cb_vkbur.Index = 0;
	cb_vkgrp.Index = 0;
}


/********************************************************************************
* 기      능   : 세금계산년 계산(현재년도,당해년도에서 뺄 년수, 당해년도에서 더할 년수
********************************************************************************/
function fn_makeYear(curYear,sNum,eNum)
{
	var rowCnt = eNum-sNum;
	var row = 0;

	for(var i=rowCnt; i>-1; i--)
	{
