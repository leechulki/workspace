/************************************************************************************
* 기      능   : Excel file upload
************************************************************************************/

function btn_upload_OnClick(obj)
{
	ds_txt.ClearData();
    
	Create("FileDialog", "_fileDialog_", "");
	var dialogObj = object("_fileDialog_");

	dialogObj.Type = "Open";
	dialogObj.FileName = "";
	dialogObj.Filter = "Text File(*.txt)|*.txt";
	dialogObj.open();

	if (length(dialogObj.FileName) == 0) {
		Destroy( "_fileDialog_" );
		return;
	}

	var sFileName = dialogObj.FilePath + "\\" + dialogObj.FileName;

	ext_ExcelImportByIndex(sFileName,null,"ds_txt",0,1);

	grd_list.BindDataset = "";

	if(substr(dialogObj.FileName,0,4) == "resu" or substr(dialogObj.FileName,0,4) == "recv" or substr(dialogObj.FileName,0,4) == "make" ) {
		fn_setDs2(substr(dialogObj.FileName,0,4));
	} else {
		fn_setDs(dialogObj.FileName);
	}
	
	grd_list.BindDataset = "ds_list";
	
	Destroy( "_fileDialog_" );  // 불러오기후 dialog 제거

}

/************************************************************************************/
// Excel data grid에 복사 작업
/************************************************************************************/
function fn_setDs(filename){

	ds_list.ClearData();
	ds_filename.ClearData();
	
	// 20150812 반송 처리 건 부터 바코드 내용에 s020 s028 추가
	for(i=0; i < ds_txt.rowcount ; i++) {
		
		var arr = Split(ds_txt.GetColumn(i,0),',');
		var barcode = arr[8] ;	
		var barcode2 = replace(barcode,"||","");
		
		/*wbs_gb 
		"N" : 수리부품 유상계약번호 11 자리
		"U" : 정기보수 유상계약번호 14자리
		""  : 전환안내 프로젝트 6자리
		*/
		var wbs_gb = substr(barcode2 ,9 ,1);
		var wbs      = "";
		var chk_hdel = "";
		var yyyymm   = "";
		var s020     = "";
		var s028     = "";
		
		var num_wbs  = 0;
		var num_s020 = 28 ;	// s020 28자리
		
		if (wbs_gb == "N") {
			num_wbs = 11;
		} else if(wbs_gb == "U") {
			num_wbs = 14;
		} else {
			num_wbs = 6;
		}	

		wbs = substr(barcode2 ,3 ,num_wbs) ;
		// s020의 시작이 HDEL 
		// HDEL 이 포함된 S020 과 S028이 포함된 바코드와 아닌 바코드가 혼재함 
		chk_hdel = substr(barcode2 ,3+num_wbs ,4);
		if(chk_hdel == "HDEL"){
			yyyymm = "";  // WBS 바로 뒤에 S020이 존재하는 경우 기준년월의 값이 없다.
			s020   = substr(barcode2 ,3+num_wbs ,num_s020);
			s028   = substr(barcode2 ,3+num_wbs+num_s020);
		} else {
		// 지로와 CMS의 경우 기준년월이 바코드에 포함됨
		// 지로 예시 1507
		// CMS 예시 2015.07
		// 기준년월 출력 포맷이 다르므로 분개하여 처리
			chk_hdel = substr(barcode2 ,3+num_wbs+4 ,1);
			//  기준년월 포맷 YYYY.MM
			if(chk_hdel == "."){ 
				yyyymm = substr(barcode2 ,3+num_wbs   ,7);
				s020   = substr(barcode2 ,3+num_wbs+7 ,num_s020);
				s028   = substr(barcode2 ,3+num_wbs+7+num_s020);
			} else {
			//  기준년월 포맷 YYMM  OR 기준년월 없음
				yyyymm = substr(barcode2 ,3+num_wbs   ,4);
				s020   = substr(barcode2 ,3+num_wbs+4 ,num_s020);
				s028   = substr(barcode2 ,3+num_wbs+4+num_s020);
			}
		}

		ds_list.AddRow();
		ds_list.SetColumn(i,"VKGRP"  , substr(barcode2, 0, 3));	
		ds_list.SetColumn(i,"PJTID"  , wbs );	
		ds_list.SetColumn(i,"YYMM"   , yyyymm);	
	
		if(length(s020) == 0 or length(s020) == 28 ){
			ds_list.SetColumn(i,"S020"   , s020);	
			ds_list.SetColumn(i,"S028"   , ParseInt(s028));
		} else {
			alert("해당파일에 문제가 있습니다. 정보기술부에 문의하세요.");
			return;
		}
		
		ds_list.SetColumn(i,"SEQNO"    , arr[0]);
		ds_list.SetColumn(i,"CDATE"    , arr[1]);
		ds_list.SetColumn(i,"CENTER"   , arr[2]);
		ds_list.SetColumn(i,"POSTGB"   , arr[3]);
		ds_list.SetColumn(i,"IMGNM"    , arr[4]);
		ds_list.SetColumn(i,"OTHER"    , arr[5]);
		ds_list.SetColumn(i,"RETURNCD" , arr[6]);
		ds_list.SetColumn(i,"SENDCD"   , arr[7]);
		ds_list.SetColumn(i,"BARCODE"  , arr[8]);
		
	}
//trace(ds_list.SaveXML());
	ds_filename.AddRow();
	ds_filename.SetColumn(0,"FILENAME"  , filename);
	
	
}
