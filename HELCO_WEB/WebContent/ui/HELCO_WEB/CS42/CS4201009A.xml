#include "LIB::commons.js";	// 공통 스크립트 
#include "LIB::utility.js";	// 공통 스크립트 
//=========================================================================================
// [ PART 1 ]
// Form에서 사용될 전역변수를 선언
// FORM에서 사용되는 전역변수는 대문자로 작성하며, F_XXX 의 형태를 갖도록 한다. 
//=========================================================================================
var gbu ;
//=========================================================================================
// [ PART 2 ]
// Form Loading / Unloading 시 작업 정의
//=========================================================================================
function fn_OnLoadCompleted(obj) {
	gfn_initForm();
	if(gbu == "pre") {
		fn_setInfo();
		fn_getMisu();
	} else {
		fn_query();
	}

}
/********************************************************************************
* 기      능   : 공통코드가 조회된 후의 Callback 함수 
********************************************************************************/
function fn_completeInit(sCode, sDsObj, sCodeType) {
	
}
/********************************************************************************
* 기      능   : form UnloadCompleted 
********************************************************************************/
function fn_OnUnloadCompleted(obj) {

}
function fn_setInfo() {
	
	st_vkgrp_nm.Text  = vkgrp_nm;
	st_vkgrp_tel.Text = vkgrp_tel;
	st_bankl.Text  = bankl;
	st_bankvn.Text = substr(bankvn,0,4)+"-"+substr(bankvn,4,4)+"-"+substr(bankvn,8,4)+"-"+substr(bankvn,12,2);
	st_nam01.Text = cs121_nam;
		
	st_s029.Text = s029;
	st_s030.Text = s030;
	st_s031.Text = s031;
	st_s032.Text = s032;

}
function fn_getMisu() {

	ds_cond.SetColumn(0,"UPN"  ,upn);
	ds_cond.SetColumn(0,"CST"  ,cst);
	ds_cond.SetColumn(0,"KUNNR",kunnr);
	ds_cond.SetColumn(0,"DAT2" ,dat2);

	tit_clearActionInfo();
//	tit_addSearchActionInfo("cs42:CS4208001B_S1");
	tit_addSearchActionInfo("cs42:CS4208001B_S2");

	tit_callService(
        ""
        , ""
        , "ds_cond=ds_cond"
        , "ds_misu=ds_list2"
        , ""
        , "fn_afterQuery"
        , true);
}
//=========================================================================================
// [ PART 3 ]
// Main Process Event 정의(조회,입력,삭제,저장,인쇄,form닫기)
//=========================================================================================
/********************************************************************************
* 기      능   : 조회
********************************************************************************/
function fn_query() {
	

	ds_cond.SetColumn(0, "CON_KEY", con_key);	

	tit_clearActionInfo();	
	tit_addSearchActionInfo("cs42:CS4201002A_S1"); 
	tit_callService(
		""
		, ""
		, "ds_cond=ds_cond"
		, "ds_list=ds_list"
		, ""
		, "fn_afterQuery"
		, true
		, true
        );
}
/********************************************************************************
* 기      능   : 조회 Callback  
********************************************************************************/
function fn_afterQuery(errCode, errMsg) {
//	trace(ds_misu.SaveXML());
	if(gbu == "pre") {
		st_upncst01.Text    = ds_misu.GetColumn(0, "UPN") + ds_misu.GetColumn(0, "CST") ;
		st_fkdat01.Text     = ds_misu.GetColumn(0, "FKDAT") ;
		st_misu_amt01.Text  = ds_misu.GetColumn(0, "TITSSMSGTA") ;
		st_text01.Text      = ds_misu.GetColumn(0, "TEXT") ;
	
		st_fkdat02.Text     = ds_misu.GetColumn(1, "FKDAT") ;
		st_misu_amt02.Text  = ds_misu.GetColumn(1, "TITSSMSGTA") ;
		st_text02.Text      = ds_misu.GetColumn(1, "TEXT") ;
		
		st_misu_amt_sum.Text = ds_misu.Sum("TITSSMSGTA") ;
	}
}



function fn_close() {
	close();
}
