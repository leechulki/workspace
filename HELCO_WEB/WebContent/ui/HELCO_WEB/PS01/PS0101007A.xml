	for( var i=0; i<ds_list.rowcount; i++ )
	{
		var sPosidDong = ds_list.GetColumn(i, "POSID_DONG");
		if ( cb_gbn.Index == 1 &&  Right(sPosidDong,1) <> '동')
		{
			//Trace(ds_list.GetColumn(i, "POSID"));
			lv_POSID_DONG = ds_list.GetColumn(i,"POSID_DONG") + ds_list.GetColumn(i,"DFCHAR");
			ds_list.SetColumn(i, "POSID_DONG", lv_POSID_DONG);
		}
	}
	
	// 설치장소 적용대상 추출
	fn_placeStandLogic();
	ds_list.fireevent = true;

	tit_clearActionInfo();
	tit_addSingleActionInfo("ps01:PS0101007A_U1");
	tit_addSelectExActionInfo("ps01:PS0101007A_S5", "ps01:PS0101007A_I2" , "ps01:PS0101007A_U3");   
	tit_callService(
		""
		, ""
		, "ds_list2=ds_listF"
		, ""
		, ""
		, "fn_afterSave"
		, ""
		, ""
		, true);
		

}	

/********************************************************************************
* 기      능   : 설치장소 생성 (기준문자열)
********************************************************************************/
function fn_placeStandLogic()
{
	ds_list.fireevent = false;
	ds_list.Sort("PSPID,POSID");
	// #1. 설치장소를 생성할 대상 추출 (변경된 동정보 호기 + 변경되지 않았더라도 프로젝트번호가 같은 모든 호기 추출)
	for(var i=0;i<ds_list.GetRowCount();i++)
	{
		var sChk = ds_list.GetColumn(i, "CHECK");
		var sPspid = ds_list.GetColumn(i, "PSPID");
		var sPosid = ds_list.GetColumn(i, "POSID");
		
		if(sChk == '1')
		{
			for(var j=0;j<ds_list.GetRowCount();j++)
			{
				var sChk2 = ds_list.GetColumn(j,"CHECK");
				var sPspid2 = ds_list.GetColumn(j,"PSPID");
				var sPosid2 = ds_list.GetColumn(j,"POSID");
				
				if(sChk2 != '1' && sPspid == sPspid2) // 체크된 내역 중 동일 프로젝트 내, 미체크된 내역이 있으면 모두 체크되도록 함.
				{
					ds_list.SetColumn(j, "CHECK", '1');
				}
				
			}
		}
	}
	ds_list.fireevent = true;
	
	ds_list.Filter("CHECK==1");
	ds_listF.copyF(ds_list);
	ds_list.unFilter();
	
	tit_callService(
          "PS0101007A_ACT"
        , "logic"
        , "ds_listF=ds_listF"
        , "ds_list_chg=ds_list"
        , ""
        , ""
        , ""
        , ""
        , true);
    

	
	
}


