 * HISTORY    :
 ******************************************************************************************
 */
#include "LIB::commons.js";	// 공통 스크립트 
//=========================================================================================
// [ PART 1 ]
// Form에서 사용될 전역변수를 선언
// FORM에서 사용되는 전역변수는 대문자로 작성하며, F_XXX 의 형태를 갖도록 한다. 
//=========================================================================================
var user_grp = "";
//=========================================================================================
// [ PART 2 ]
// Form Loading / Unloading 시 작업 정의
//=========================================================================================
function fn_OnLoadCompleted(obj) 
{
	// Form 로드 이벤트 맨 첫줄에 기술 
	gfn_initForm();

	user_grp = gfn_getUserGroup();

	ed_pjt.SetFocus();

	if ( isExistVar("posid", true) )
	{
		ed_pjt.Text = posid;
		fn_query();
	}
}
/********************************************************************************
* 기      능   : 공통코드가 조회된 후의 Callback 함수 
********************************************************************************/
function fn_completeInit(sCode, sDsObj, sCodeType) 
{
}
/********************************************************************************
* 기      능   : form UnloadCompleted 
********************************************************************************/
function fn_OnUnloadCompleted(obj) 
{
	return gfn_isFormUnload(obj);
}
//=========================================================================================
// [ PART 3 ]
// Main Process Event 정의(조회,입력,삭제,저장,인쇄,form닫기)
//=========================================================================================
/********************************************************************************
* 기      능   : 조회
********************************************************************************/
function fn_query() 
{
	//년필드 필수체크	
	if(gfn_isNull(ed_pjt, true, st_pjt.value))
		return;
	
	if(Length(Trim(ed_pjt.Text)) != 6)
	{
		gfn_openMsgWin("프로젝트 번호는 6자리만 입력 가능합니다.");
		return;
	}
	
	if(user_grp == '10') 
	{
		ds_cond.SetColumn(0,"GBN","10");
	} 
	else if(user_grp == '30' || user_grp == '40') 
	{
		ds_cond.SetColumn(0,"GBN","");
	}

	tit_clearActionInfo();
	
	//tit_addSearchActionInfo("ps01:PS0101001A_S1");  //==> CsvSaerch로 변경
	tit_addCsvSearchActionInfo("ps01:PS0101001A_S1"); 
	
	
	tit_callService(
          ""
        , ""
        , "ds_cond=ds_cond"
        , "ds_head=ds_head"
        , ""
        , "fn_afterQuery"
        , true);
}
/********************************************************************************
* 기      능   : 조회(특기사항)
********************************************************************************/
function fn_query1() 
{
	ds_cond1.DeleteAll();
	ds_cond1.AddRow();
	ds_cond1.SetColumn(0,"PSPID",ds_head.GetColumn(0,"POSID"));

	tit_callService(
          "PS0101001A_ACT"
        , "query"
        , "ds_cond=ds_cond1"
        , "ds_list1=ds_list"
        , ""
        , "fn_afterQuery1"
        , false);
}
/********************************************************************************
* 기      능   : 조회(호기정보)
********************************************************************************/
function fn_query2() 
{
	ds_cond2.DeleteAll();
	ds_cond2.AddRow();
	ds_cond2.SetColumn(0,"POSID",ds_head.GetColumn(0,"POSID"));

    //자재협력사,영업대리점은 모든업체 호기정보 조회하도록 수정 : 2008.10.27
	if(user_grp == '10' && trim(G_USER_GROUP_M) != 'M10' && trim(G_USER_GROUP_B) != 'B10')
	{
		ds_cond2.SetColumn(0,"GBN","10");
	}
	else if(user_grp == '10' || user_grp == '30' || user_grp == '40')
	{
		ds_cond2.SetColumn(0,"GBN","");
	}

	tit_clearActionInfo();
	
	tit_addSearchActionInfo("ps01:PS0101001A_S2");
	tit_addSearchActionInfo("ps01:PS0101001A_S3");
	tit_addSearchActionInfo("ps01:PS0101001A_S4");

	tit_callService(
          ""
        , ""
        , "ds_cond=ds_cond2"
        , "ds_list2=ds_list2 ds_list3=ds_list3 ds_list4=ds_list4"
        , ""
        , "fn_afterQuery2"
        , true);
}
/********************************************************************************
* 기      능   : 닫기 
********************************************************************************/
function fn_close() 
{
	close();
}
//=========================================================================================
// [ PART 4]
// Callback 함수 정의 : callback 함수명은 메인 함수의 fn_xxxx() 에서 xxxx 부분은 fn_afterXXXX()
// 형태가 되도록 작성 
//=========================================================================================
/********************************************************************************
* 기      능   : 조회 Callback 
********************************************************************************/
function fn_afterQuery(errCode, errMsg) 
{
	gfn_showMsg("CI00002",ds_head.RowCount()+"");
	
	if(ds_head.RowCount() > 0)
	{
		fn_query1();
		fn_query2();
	}	
}
/********************************************************************************
* 기      능   : 조회 Callback  
********************************************************************************/
