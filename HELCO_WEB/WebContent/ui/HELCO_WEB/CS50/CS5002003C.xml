function fn_OnLoadCompleted(obj) {
	gfn_initForm();
	ds_cond.ClearData();
	ds_cond.AddRow();

    var curYear = substr(gfn_getCurrDate(),0,4);
    var curMonth = substr(gfn_getCurrDate(),4,2);

	fn_makeYear(curYear,-3,0);
	if (curMonth == '01') {
		cb_year.Index = 1;
		cb_month.Index = 12;
	} else {
		cb_year.Index = 0;
		cb_month.Index = ToNumber(curMonth) - 1;
	}

	cb_type.Index = 0;
	cb_hst.Index = 0;
}
