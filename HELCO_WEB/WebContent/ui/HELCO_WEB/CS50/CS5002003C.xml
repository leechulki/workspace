function btn_pjt_OnClick(obj)
{
	var arg = "pjt="+quote(ed_pjt.value);
	arg += " gbn=C";
	var pjt= null;
	pjt = gfn_openPopup("CS9601002A",true,arg);
	
	if (pjt != null && obj == btn_pjt) {
		ed_spt.value = "";
		ed_pjt.value = pjt[0];
		ed_spt.value = pjt[1];
		ed_nam.value = pjt[2];
		ufn_getProject('',pjt[0]);
    }
}
