********************************************************************************/
function fn_query() {
	// 조회 처리 
	
	var sStrtDt = Substr(cal_mnth.Value,0,6)+'01';
	var sEndDt  = gfn_getLeapLastDay(sStrtDt);
	
	ds_cond.SetColumn(0, "FROM_DT", sStrtDt);
	ds_cond.SetColumn(0, "TO_DT", sEndDt);
	
	if(rd_date.Value == 'B') // 일자로 조회일 경우에만
	{
		var diffDays = gfn_getDayCnt(ds_days, cal_datef.Value, cal_datet.Value);
		
		if(diffDays > 31)
		{
			gfn_openMsgWin("조회일자를 31일 이내로 선택해 주십시오.");
			return;
		}
	}
	
	
	
	tit_clearActionInfo();
	
	if(rd_chk.Value == 1)	// 수통접수
	{
		tit_addSearchActionInfo("ps02:PS0201007A_S1");  
	}
	else
	{
		tit_addSearchActionInfo("ps02:PS0201007A_S2");  
	}
	
	
	tit_callService(
		""
		, ""
		, "ds_cond=ds_cond"
		, "ds_list=ds_list"		
		, ""
		, "fn_afterQuery"   
		, ""  //true
		, ""
		, true);

}

