		//return;
	//}
	//=======================================================================================

	// sort mark 삭제 
	gfn_clearGridSortMark(grd_list);
	ds_list.row = -1;

	// 조회 처리 
	tit_clearActionInfo();
	//tit_addSearchActionInfo("ps02:PS0201004A_S1");    //==> CsvSaerch로 변경
	tit_addCsvSearchActionInfo("ps02:PS0201004A_S1");  
	
	
	tit_callService(
		""
		, ""
		, "ds_cond=ds_cond"
		, "ds_list=ds_list"
		, ""
		, "fn_afterQuery"
		, true,true);
}

