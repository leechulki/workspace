function fn_query() {
	if(length(trim(ed_pjt.value)) > 0 && length(trim(ed_pjt.value)) < 6)
	{
		arr = [st_pjt.value, 6];
		gfn_showMsg("CW00174",arr);
		ed_pjt.SetFocus();
		return;
	}
	
	if(length(trim(ed_pjt.Text)) > 5)
	{
		ds_cond.SetColumn(0,"GUBUN","A");
		// 필수 입력사항 및 일자 체크 
		if ( !gfn_checkFromToObj(cal_cdatef, cal_cdatet, true, st_date.value,8,false) )
			return;
	}
	else 
	{
		ds_cond.SetColumn(0,"GUBUN","B");
		// 필수 입력사항 및 일자 체크 
		if ( !gfn_checkFromToObj(cal_cdatef, cal_cdatet, true, st_date.value,8,true) )
