* 기      능   : form UnloadCompleted 
********************************************************************************/
function fn_OnUnloadCompleted(obj) 
{
	return gfn_isFormUnload(obj);
}
//=========================================================================================
// [ PART 3 ]
// Main Process Event 정의(조회,입력,삭제,저장,인쇄,form닫기)
//=========================================================================================
/********************************************************************************
* 기      능   : 조회
********************************************************************************/
function fn_query() 
{
/*
	//년필드 필수체크	
	if(gfn_isNull(ed_pjt, true, st_pjt.value))
		return;

	if(user_grp == '10') 
	{
		ds_cond.SetColumn(0,"GBN","10");
	} 
	else if(user_grp == '30' || user_grp == '40') 
	{
		ds_cond.SetColumn(0,"GBN","");
	}
*/
	tit_clearActionInfo();
	
	//tit_addSearchActionInfo("ps01:PS0101001A_S1");  //==> CsvSaerch로 변경
	//tit_addCsvSearchActionInfo("ps01:PS0101001A_S1"); 
	tit_addCsvSearchActionInfo("ps02:PS0204008B_S1"); 
	tit_addCsvSearchActionInfo("ps01:PS0101001A_S8");
	

	tit_callService(
          ""
        , ""
        , "ds_cond=ds_cond"
        , "ds_head=ds_head ds_head2=ds_list10"  //ds_head2=ds_list10"
        , ""
        , "fn_afterQuery"
        , true);
              
        
}

//=========================================================================================
// [ PART 4]
// Callback 함수 정의 : callback 함수명은 메인 함수의 fn_xxxx() 에서 xxxx 부분은 fn_afterXXXX()
// 형태가 되도록 작성 
//=========================================================================================
/********************************************************************************
* 기      능   : 조회 Callback 
********************************************************************************/
function fn_afterQuery(errCode, errMsg) 
{
	//gfn_showMsg("CI00002",ds_head.RowCount()+"");
	
	if(ds_head.RowCount() > 0)
	{
		fn_query1();		
	}	
}

/********************************************************************************
* 기      능   : 조회(특기사항)
********************************************************************************/
function fn_query1() 
{
	ds_cond1.DeleteAll();
	ds_cond1.AddRow();
	ds_cond1.SetColumn(0,"PSPID",ds_head.GetColumn(0,"PSPID"));

	tit_callService(
          "PS0101001A_ACT"
        , "query"
        , "ds_cond=ds_cond1"
        , "ds_list1=ds_list"
        , ""
        , ""  //"fn_afterQuery1"
        , false);
    
    PUID = "";
    PUSER = "";
	
	// 생산관리담당자 추가
	tit_clearActionInfo();
	tit_callService(
	  "PS0101001A_ACT"
	, "getPrdUser"
	, "ds_cond=ds_cond"
	, ""
	, ""
	, "fn_afterQueryPrd"
	, true);
	
}

function fn_afterQueryPrd()
{
	
	ds_cond_p.ClearData();
	ds_cond_p.AddRow();
	ds_cond_p.SetColumn(0, "PUID", PUID);
	
	// 생관담당자 전화번호 SELECT
	tit_clearActionInfo();
	tit_addCsvSearchActionInfo("ps02:PS0204008B_S2"); 
	
	tit_callService(
          ""
        , ""
        , "ds_cond=ds_cond_p"
        , "ds_tel=ds_tel"  
        , ""
		, ""
		, ""
		, ""
		, true);

	
	// 생관담당자
	ds_head.SetColumn(0, "P_MAN", PUSER);
	
    if(ds_tel.GetRowCount() > 0)
    {
		ds_head.SetColumn(0, "P_TEL", ds_tel.GetColumn(0, "P_TEL"));
    }
	
	
}
/********************************************************************************
* 기      능   : 닫기 
********************************************************************************/
function fn_close() 
{
	close();
}



