function fn_OnLoadCompleted(obj) {
	// Form 로드 이벤트 맨 첫줄에 기술 
	gfn_initForm();	

	user_grp = gfn_getUserGroup();

	if ( parent.grd_list.Format == 'Mbefore' || parent.grd_list.Format == 'Default')
	{
		ds_cond_p.SetColumn(0, "PSPID", ds_list.GetColumn(ds_list.row,"PSPID")); 	
		ds_cond_p.SetColumn(0, "POSID", ds_list.GetColumn(ds_list.row,"POSID"));  		
	}
	else if ( parent.grd_list.Format == 'Mafter')
	{
		ds_cond_p.SetColumn(0, "PSPID", ds_list2.GetColumn(ds_list2.row,"PSPID")); 	
		ds_cond_p.SetColumn(0, "POSID", ds_list2.GetColumn(ds_list2.row,"POSID"));  	
	}
	else if ( parent.grd_list.Format == 'Mall')
	{
		ds_cond_p.SetColumn(0, "PSPID", ds_list3.GetColumn(ds_list3.row,"PSPID")); 	
		ds_cond_p.SetColumn(0, "POSID", ds_list3.GetColumn(ds_list3.row,"POSID"));  	
	}

	fn_query();




	
}
