function fn_query() {

    // Message 영역 초기화
	global.FRM_BOTTOM.img_msgType.imageId = "";
	global.FRM_BOTTOM.st_msg.value = "";

	if( gfn_isNull(ed_posid, false) && gfn_getDayCnt(ds_days,cal_date_fm.value, cal_date_to.value) > 183)
	{
		gfn_showAlert("CW00078","6");
		cal_reqdate_fm.SetFocus();
		return;
	}

	ds_cond.SetColumn(0, "GBN", ufn_getGbn());
	ds_cond.SetColumn(0, "CB1", div_code.cb_big.value);
	ds_cond.SetColumn(0, "CB2", div_code.cb_mid.value);
	ds_cond.SetColumn(0, "CB3", div_code.cb_small.value);

	
   // sort mark 삭제 
	gfn_clearGridSortMark(grd_list);
	ds_list.row = -1;
	ds_list.ClearData();
	for (k = 1; k < 7; k++)
	{
		grd_list.SetCellProp("Summary",k,"Text", '');
	}
	
	tit_clearActionInfo();
	tit_addSearchActionInfo("cs01:CS0101006B_S1");
	tit_callService(
        ""
        , ""
        , "ds_cond=ds_cond"
        , "ds_list=ds_list"
        , ""
        , "fn_afterQuery"
        , false
        , false 
        , false);
}

