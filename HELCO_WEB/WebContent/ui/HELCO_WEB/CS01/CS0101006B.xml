********************************************************************************/
function fn_afterQuery(errCode, errMsg) {
	// 디테일 정보 필터링
	if (ds_list.rowcount > 0) {
	    colarr = ["SHEAVE","ROPE","ROLLER","CHAIN","HANDRAIL","WHEEL"];
		sumcol = Array(6);
	    for (i = 0; i < ds_list.rowcount; i++)
	    {
			for (j = 0; j < 6 ; j++)
			{
				if(length(ds_list.GetColumn(i,colarr[j])) > 0)
				{
					sumcol[j]++; 
				}
			}
			
	    }

	    for (k = 1; k < 7; k++)
	    {
			grd_list.SetCellProp("Summary",k,"Text", sumcol[k-1]+'');
	    }
		ufn_enableBtn("excel", true);
		gfn_showMsg("CI00002", ds_list.rowCount() + "");
	} else {
		ufn_enableBtn("excel", false);
		gfn_showMsg("CW00010");
	}
}


