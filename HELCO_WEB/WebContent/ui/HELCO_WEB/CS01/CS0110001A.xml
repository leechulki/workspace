/********************************************************************************
* 기      능   : 출력건수 저장
********************************************************************************/
function fn_TempSave(gbn) {
	var t_sql = "";

	gfn_clearMsgBar();

	if(ds_list.FindRow("CHK", 1) == -1) {
		gfn_showMsg("CW00013");
		return;
	}

	if(gbn == "A") {
		t_sql = "cs01:CS0110001A_U1";
		g_gbn = gbn;
	} else if(gbn == "B") {
		t_sql = "cs01:CS0110001A_U2";
		g_gbn = gbn;
	} else {
		return false;
	}

	ds_list2.ClearData();

	var j = 0;
	for(var i=0; i<ds_list.RowCount(); i++) {
		if(ds_list.GetColumn(i, "CHK") == true) {
			ds_list2.AddRow();
			ds_list2.CopyRow(j, "ds_list", i);

			j++;
		}
	}

	tit_clearActionInfo();
	tit_addMultiActionInfo(t_sql);
	tit_addSearchActionInfo("cs01:CS0110001A_S3");
	tit_callService(
        ""
        , ""
        , "ds_list2=ds_list2"
        , "ds_list3=ds_list3"
        , ""
        , "fn_afterPrt");
}

/********************************************************************************
* 기      능   : 출력 팝업
********************************************************************************/
function fn_afterPrt() {
	for(var i=0; i<ds_list2.RowCount(); i++) {
		ds_list2.SetColumn(i, "USR", ds_list3.GetColumn(0, "USR"));
		ds_list2.SetColumn(i, "ADR", ds_list3.GetColumn(0, "ADR"));
		ds_list2.SetColumn(i, "TEM_NM", ds_list3.GetColumn(0, "TEM_NM"));
		ds_list2.SetColumn(i, "DTEXT2", ds_list3.GetColumn(0, "DTEXT2"));
		ds_list2.SetColumn(i, "DTEXT3", ds_list3.GetColumn(0, "DTEXT3"));
		ds_list2.SetColumn(i, "DTEXT4", ds_list3.GetColumn(0, "DTEXT4"));
		ds_list2.SetColumn(i, "ZIP2", ds_list3.GetColumn(0, "ZIP2"));
		ds_list2.SetColumn(i, "SEQ", i+1);
		ds_list2.SetColumn(i, "NUM", ds_list3.GetColumn(0, "NUM"));
	}

	if(g_gbn == "A") {
		Dialog("CS01::CS0110001B.xml", "");
	} else if(g_gbn == "B") {
		Dialog("CS01::CS0110001C.xml", "");
	} else {
		return false;
	}

	fn_query();
}
