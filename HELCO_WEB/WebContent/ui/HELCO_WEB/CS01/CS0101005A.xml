********************************************************************************/
function fn_query() {

    // Message 영역 초기화
	global.FRM_BOTTOM.img_msgType.imageId = "";
	global.FRM_BOTTOM.st_msg.value = "";

	ds_cond.SetColumn(0,"USER_GROUP",gfn_getUserGroup());

	//필수입력 check
	if( gfn_isNull(ed_upn, false) || gfn_isNull(cal_year, false) || gfn_isNull(cb_itn, false))
	{ 
		gfn_showAlert("CW00002");
		return;
	}
	ds_cond.SetColumn(0, "YEAR", substr(cal_year.value,0,4));
	
	var hyear = parseInt(substr(cal_year.value,0,4));
	
	for (i = 0; i < 10 ; i++)
	{
	    grd_list.SetCellProp("HEAD",5+i,"text", (hyear-i)+"년");
	}
  // sort mark 삭제 
	gfn_clearGridSortMark(grd_list);
	ds_list.row = -1;
	ds_list.ClearData();
	
	tit_clearActionInfo();
	tit_addSearchActionInfo("cs01:CS0101005A_S1");
	tit_callService(
        ""
        , ""
        , "ds_cond=ds_cond"
        , "ds_list=ds_list"
        , ""
        , "fn_afterQuery1"
        ,""
        , true
        , true);
}

// host 내용 조회
function fn_query2() {

	
	
	//필수입력 check
	if( gfn_isNull(ed_upn, false) || gfn_isNull(cal_year, false) || gfn_isNull(cb_itn, false))
	{
		gfn_showAlert("CW00002");
		return;
	}
	
	gfn_copyDs(ds_cond,ds_cond1);
	
	var upn = ds_cond1.GetColumn(0,"UPN");
	if (substr(upn,0,1) == "0")
	{
		ds_cond1.SetColumn(0,"UPN", substr(upn,1));
	} else if (substr(upn,1,1) == "0" ) //(substr(upn,0,1) == "M" || substr(upn,0,1) == "N" || substr(upn,0,1) == "J" || substr(upn,0,1) == "G")
	{
		ds_cond1.SetColumn(0,"UPN", substr(upn,0,1) + substr(upn,2));
	}

	tit_clearActionInfo();
	tit_addSearchActionInfo("cs01:CS0101005A_S2");
	tit_callService(
        "HOST_ACT"
        , ""
        , "ds_cond1=ds_cond1"
        , "ds_list1=ds_list1"
        , ""
        , "fn_afterQuery2"
        ,""
        , true
        , true);


}
