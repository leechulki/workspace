function fn_afterQuery1(errCode, errMsg) {
	// 디테일 정보 필터링
	if (ds_list.rowcount > 0)
	{
	    if ( ds_type.value != "J" && parseInt(ds_cond.GetColumn(0,"YEAR")) < 2018 && (substr(ds_cond.GetColumn(0,"UPN"),0,1) == "0" || substr(ds_cond.GetColumn(0,"UPN"),1,1) == "0"))
	    {
			fn_query2();
		}
		else
		{
			ufn_enableBtn("excel", true);
			gfn_showMsg("CI00002", ds_list.rowCount() + "");
		}
	}else {
		ufn_enableBtn("excel", false);
		gfn_showMsg("CW00010");
	}
}

function fn_afterQuery2(errCode, errMsg) {
	// 디테일 정보 필터링

	if (ds_list1.rowcount > 0)
	{
	  for (i = 0; i < ds_list.rowcount; i++)
	  {
		for (j  = 0; j < ds_list1.rowcount; j++)
		{ 
		   if ( ((substr(ds_list.GetColumn(i,"PJT"),0,1) == "0" && substr(ds_list.GetColumn(i,"PJT"),1) == ds_list1.GetColumn(j,"PJT")) ||
		         (substr(ds_list.GetColumn(i,"PJT"),1,1) == "0" && substr(ds_list.GetColumn(i,"PJT"),0,1) + substr(ds_list.GetColumn(i,"PJT"),2) == ds_list1.GetColumn(j,"PJT")) )
		     && ds_list.GetColumn(i,"HNO") == ds_list1.GetColumn(j,"HNO")) //substr(ds_list.GetColumn(i,"PJT"),1) == ds_list1.GetColumn(j,"PJT") &&  
		   {
			  for(x = 0; x < 10 ; x++)
			  {
				if (length(ds_list.GetColumn(i, 17+x)) = 0 && length(ds_list1.GetColumn(j, 17+x)) > 0) 
				{
					ds_list.SetColumn(i,2+x, ds_list1.GetColumn(j, 2+x));
					ds_list.SetColumn(i,17+x, ds_list1.GetColumn(j, 17+x));
				}
			  }
		   }
		}
	  }
		ufn_enableBtn("excel", true);
		gfn_showMsg("CI00002", ds_list.rowCount() + "");
	} else if (ds_list.rowcount > 0) {
		ufn_enableBtn("excel", true);
		gfn_showMsg("CI00002", ds_list.rowCount() + "");
	} else {
		ufn_enableBtn("excel", false);
		gfn_showMsg("CW00010");
	}
}

