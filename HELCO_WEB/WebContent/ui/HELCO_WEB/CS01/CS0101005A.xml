function grd_list_OnHeadClick(obj,nCell,nX,nY,nPivotIndex)
{
	if ( nCell == 0 ) {
		// 그리드 표시할 컬럼 선택 
		gfn_selectGridCol(obj);
	} else {
		// 그리드 정렬 처리 
		gfn_sortGrid(obj,nCell);
	}
}
/********************************************************************************
* 기      능   : 통합프로젝트 검색 팝업
********************************************************************************/
function btn_popProj_OnClick(obj)
{
	var arg = "upn="+quote(ed_upn.Text);
	var arr = gfn_openPopup("CS9601001A", true, arg);
	
	if (arr != null)
	{
		ed_upn.value = arr[0];
		ed_cst.value = arr[1];
	}
}


/********************************************************************************
* 기      능   : BLOCK변경시 자재변경
********************************************************************************/

function fn_blockChanged(obj)
{
	ds_itn.Filter(" DTEXT2=" +quote(obj.value));
	cb_itn.index = 0;
	if(substr(obj.value,0,1) == "5")
	{
		cb_atype.index = 4;
	}
}

function grd_list_OnCellDBLClick(obj,nRow,nCell,nX,nY,nPivotIndex)
{
	if (nCell > 4){
		// 품목표 조회 팝업
		var arg = "FA_PJT=" + quote(SubStr(obj.GetCellValue(nRow,1),0,9));
		arg += " FA_YEAR=" + quote(SubStr(grd_list.GetCellProp("HEAD",nCell,"text"),0,4));
		arg += " FA_GUBN=" + quote(ds_list.GetColumn(nRow,nCell-4));
		gfn_openPopup("CS0101005B", true, arg, 420, 529);
	}
}

