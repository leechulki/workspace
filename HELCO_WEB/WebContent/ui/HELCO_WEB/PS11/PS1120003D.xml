#include "LIB::commons.js";	// 공통 스크립트 
#include "LIB::nsfRequest.js";	// 공통 스크립트 
#include "LIB::common_hdel.js";	// 공통 스크립트  /* GNY 추가 */
//=========================================================================================
// [ PART 1 ]
// Form에서 사용될 전역변수를 선언
// FORM에서 사용되는 전역변수는 대문자로 작성하며, F_XXX 의 형태를 갖도록 한다. 
//=========================================================================================
//var F_TITLE = "기계실 출입문 위치";

//=========================================================================================
// [ PART 2 ]
// Form Loading / Unloading 시 작업 정의
//=========================================================================================
function fn_OnLoadCompleted(obj) {
    // 팝업 버튼 적용
	div_btn.fn_loadUseBtn("20,900", pr_SUVPRHNAME, "", "");
	img.Text = pr_SUVPRHNAME+"\n\n\n\n\n";
	gfn_initForm();
	fn_init_form() ;
}

/********************************************************************************
* 기      능   : form UnloadCompleted 
********************************************************************************/
function fn_OnUnloadCompleted(obj) {
	return gfn_isFormUnload(obj);
}

/********************************************************************************
* 기      능   : 폼 초기화
********************************************************************************/
function fn_init_form() {
	fn_query_code("ELR_M_DOORP"); // 콤보값 조회
	
	var i = 0; 

	while(i < ds_CODE.GetRowCount()) {
		var col = ds_CODE.GetColumn(i, "CHK2");
		var val = ds_CODE.GetColumn(i, "SUVPRD");
		if(pr_SUVPRD == val) {
			object(col).value = 1;
		}else {
			object(col).value = 0;
		}
		i++;
	}
} 

// 실측사양 코드값 조회
function fn_query_code(code) {
	var arr = split(code, ",");
	for(var i = 0; i < arr.length(); i++) {
		ds_cond.ClearData();
		ds_cond.AppendRow();
		ds_cond.SetColumn(0, "SUVPRH", arr[i]);
		
		tit_clearActionInfo();
		tit_addSearchActionInfo("ps11:PS1120002A_S3"); // 헤더 조회
		
		tit_callService(""
		    			,""
						,"ds_cond=ds_cond"
						,"ds_dummy=ds_dummy"
						,""
						,"fn_afterQuery_code"
						,""
						,""
						,true);
	}
}

/********************************************************************************
* 기      능   : 조회 Callback  
********************************************************************************/
function fn_afterQuery_code(errCode, errMsg) {
	ds_CODE.copy(ds_dummy);
	
	for(var i = 0; i < ds_CODE.GetRowCount(); i++) {
		var col  = ds_CODE.GetColumn(i, "CHK2");
		var name = ds_CODE.GetColumn(i, "SUVPRDNAME");
		
		object(col).Text = name;
	}
}

//=========================================================================================
// [ PART 3 ]
// Main Process Event 정의(조회,입력,삭제,저장,인쇄,form닫기)
//=========================================================================================
function fn_OnClick_btn(obj)
{
	var fncName = "fn_" + substr(obj.id, 4); 
	eval( fncName + "()");
}

/********************************************************************************
* 기      능   : 조회
********************************************************************************/
function fn_query() {
}

/********************************************************************************
* 기      능   : 액셀  
********************************************************************************/
function fn_excel() {
    fn_createWait("",this);
}

/********************************************************************************
* 기      능   : 확인  
********************************************************************************/
function fn_confirm() {
    fn_createWait("",this);
}

/********************************************************************************
* 기      능   : 입력  
********************************************************************************/
function fn_input() {
	var value = "";
	var i = 0; 

	while(i < ds_CODE.GetRowCount()) {
		var col = ds_CODE.GetColumn(i, "CHK2");
		if(object(col).value == 1) {
			value = ds_CODE.GetColumn(i, "SUVPRD");
		}
		i++;
	}
	
	if(value == "") {
		gfn_openMsgWin(F_TITLE+"를 선택 해 주십시오.", "W");
	}else {
		this.close(value);
	}
}

/********************************************************************************
* 기      능   : 닫기  
********************************************************************************/
function fn_close() {
	this.close("");
}

/********************************************************************************
* 기      능   : 저장
********************************************************************************/
function fn_save() {
} 

/********************************************************************************
* 기      능   : 저장 입력값 체크
********************************************************************************/
function fn_save_validate() {
    var isValidate = true;
    return isValidate;
}

//=========================================================================================
// [ PART 4]
// Callback 함수 정의 : callback 함수명은 메인 함수의 fn_xxxx() 에서 xxxx 부분은 fn_afterXXXX()
// 형태가 되도록 작성 
//=========================================================================================
/********************************************************************************
* 기      능   : fn_init_form Callback  
********************************************************************************/
function fn_afterInitForm(svcId, errCode, errMsg) {
    fn_destroyWait();
	if(ds_error.rowcount > 0)	{
		gfn_showAlert(ds_error.GetColumn(0,"ERRMSG"));
		return;
	}
}

/********************************************************************************
* 기      능   : 조회 Callback  
********************************************************************************/
function fn_afterQuery(svcId, errCode, errMsg) {
    fn_destroyWait();
	if(ds_error.rowcount > 0)	{
		gfn_showAlert(ds_error.GetColumn(0,"ERRMSG"));
		return;
	}
}

/********************************************************************************
* 기      능   : 저장 Callback  
********************************************************************************/
function fn_afterSave(svcId, errCode, errMsg) {
    fn_destroyWait();
	if(ds_error.rowcount > 0)	{
		gfn_showAlert(ds_error.GetColumn(0,"ERRMSG"));
		return;
	} else {
	}
}

//=========================================================================================
// [ PART 5]
// 화면 UI 함수
//=========================================================================================

// 체크박스 클릭
function chk_OnClick(obj,strValue) {
	if(strValue == 0) return;

	var i = 0; 
	while(i < ds_CODE.GetRowCount()) {
		var col = ds_CODE.GetColumn(i, "CHK2");
		if(obj.id != col) {
			object(col).value = 0;
		}
		i++;
	}
}

function fn_OnKeyDown(obj,objSenderObj,nChar,bShift,bControl,bAlt,nLLParam,nHLParam) {
	if(bShift == "0" && bControl == "1" && indexOf("83", nChar) > -1) {
		fn_input();
		
	}else if(indexOf("27", nChar) > -1) {
		fn_close();
	}
}
