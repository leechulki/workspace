function fn_query() {
// Message 영역 초기화
	global.FRM_BOTTOM.img_msgType.imageId = "";
	global.FRM_BOTTOM.st_msg.value = "";
	
	// 필수 입력 항목 체크 
	if ( gfn_isNull(cal_date_fm, true) ) {
		gfn_showMsg("CW00001","출하요청일");
		return;
	}
	if ( gfn_isNull(cal_date_to, true) ) {
		gfn_showMsg("CW00001","출하요청일");
		return;
	}

	if ( !gfn_checkDate( cal_date_fm.value, cal_date_to.value, true, cal_date_to) ) {
		return;
	}
	
	tit_clearActionInfo();
	tit_addSearchActionInfo("mm05:MM0506001A_S1");
	
	tit_callService(
        ""
        , ""
        , "ds_cond=ds_cond"
        , "ds_list=ds_list"
        , ""
        , "fn_afterQuery"
        , true);
	  

}

