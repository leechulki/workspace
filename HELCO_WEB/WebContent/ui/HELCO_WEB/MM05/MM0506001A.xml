function grd_list_OnExpandEdit(obj,nRow,nCell,strVal,nPivotIndex)
{
	//호기정보
	if(nCell = 2)
	{
		var rtn = gfn_openPopup("MM0901004A", true);
		
		if(rtn != null)
		{
			ds_list.SetColumn(nRow,"POSID",rtn[4]);
			ds_list.SetColumn(nRow,"POST1",rtn[2]);
		}
		
	}
	//자재정보
	if(nCell = 8)
	{
		var rtn2 = gfn_openPopup("MM0901002B",true);
		
		if (rtn2 != null)
		{
			ds_list.SetColumn(nRow,"MATNR",rtn2[0]);
			ds_list.SetColumn(nRow,"MATNR_NM",rtn2[2]);
			ds_list.SetColumn(nRow,"SPEC",rtn2[4]);
		}
	}
	//반납일 및 출하일 입력
	if(nCell = 5 || nCell = 6)
	{
		var rect = obj.GetCellRect(nRow, ncell);
		
		cal_temp.top = rect[1] +60 ;
		cal_temp.Left = rect[0];
		cal_temp.Visible = true;
		
		switch ( nCell )
			{
				case 5 : 
					cal_temp.Value = ds_list.GetColumn(nRow, "RTNDAT");
					col = "RTNDAT";
				break;
				
				case 6 :
					cal_temp.Value = ds_list.GetColumn(nRow, "SHIPDAT");
					col = "SHIPDAT";
				break;
				
			}
		cal_temp.SetFocus();	
	}
}
