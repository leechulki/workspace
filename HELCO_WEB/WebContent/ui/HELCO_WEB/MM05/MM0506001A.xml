}

/********************************************************************************
* 기      능   : 저장
********************************************************************************/
function fn_save() {
	
    if ( !gfn_isChangeDs(ds_list, true) ) 
    {
		return;
	}
		//  자료가 없으면 리턴
	if (ds_list.rowcount < 1) {
		gfn_showAlert("CW00010");
		return;
	}
	// 저장여부 확인
	if (!gfn_showConfirm("CI00010")) {
		return;
	}
	
	for(var i=0; i < ds_list.GetRowCount(); i++)
	{
		if(ds_list.GetColumn(i,"FLAG") == "I" || ds_list.GetColumn(i,"FLAG") == "U")
		{
			var a = ToFloat(ds_list.GetColumn(i,"ZCOST1"));
			var b = ToFloat(ds_list.GetColumn(i,"ZCOST2"));
			
			if(a-b < 0){
				gfn_showAlert("CW00114","판매금액이 장부금액보다 큰 값은");
				return;
			}
		}
	}
	
	ds_list.fireevent = false;
	
	tit_clearActionInfo();
	tit_addSaveActionInfo(
		"mm05:MM0506001A_I1"
	  , "mm05:MM0506001A_U1"
 	  , "mm05:MM0506001A_U2"
	  , "FLAG"
	  , ""
	  , 0
	  , "");
	  
	 // 저장 처리 
		tit_callService(
		""
		, ""
		, "ds_list=ds_list"
		, ""
		, ""
		, "fn_afterSave"        
		); 
	
}

/********************************************************************************
* 기      능   : 삭제
********************************************************************************/
function fn_delete() {
	
	ds_list.fireevent = false;
	
	if(ds_list.rowcount() == 0) 
	{
		gfn_showMsg("CW00013");
		return;
	}
	if(ds_list.GetColumn(ds_list.currow,"FLAG") == "I")
	{
		ds_list.DeleteRow(ds_list.currow);
	
	} else {
		ds_list.SetColumn(ds_list.currow,"FLAG","D");
	}
	
	ds_list.fireevent = true;
	
}
//=========================================================================================
// [ PART 4]
// Callback 함수 정의 : callback 함수명은 메인 함수의 fn_xxxx() 에서 xxxx 부분은 fn_afterXXXX()
// 형태가 되도록 작성 
//=========================================================================================
/********************************************************************************
* 기      능   : 조회 Callback  
********************************************************************************/
function fn_afterQuery(errCode, errMsg) {

   	if ( ds_list.rowCount() > 0 ) {
		gfn_showMsg("CI00002", ds_list.rowCount() + "");
		
	} else {
		gfn_showMsg("CW00010");
	}

}

/********************************************************************************
* 기      능   : 저장 Callback  
********************************************************************************/
function fn_afterSave(errCode, errMsg) {
	
	if ( errCode!= 0 ) {
		gfn_showMsg("CE00001");
	} else {
		gfn_showMsg("CI00003");
		ds_list.fireevent = true;
	}
	
	ds_list.fireevent = true;
	
	fn_query();
}

/********************************************************************************
* 기      능   : 삭제 Callback  
********************************************************************************/
function fn_afterDelete(errCode, errMsg) {
	
	if ( errCode != 0 )
	{
		gfn_showMsg("CE00001");
	} else {
		gfn_showMsg("CI00003");
	}
	
	ds_list.fireevent = true;
	
	fn_query();
}
