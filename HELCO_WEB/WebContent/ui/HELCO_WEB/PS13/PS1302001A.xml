function grd_list_OnHeadClick(obj,nCell,nX,nY,nPivotIndex)
{
	if ( nCell == 0 ) {
		// 그리드 표시할 컬럼 선택 
		gfn_selectAllGrid2(obj);
	} else {
		// 그리드 정렬 처리 
		gfn_sortGrid(obj,nCell);
	}
}


function ds_list_OnColumnChanged(obj,nRow,strColumnID,varOldValue,varNewValue,nPivotIndex)
{
	ds_list.fireevent = false;
	if(strColumnID!="CHK")
	{
		ds_list.SetColumn(nRow, "CHK", 1);
	}
	ds_list.fireevent = true;
}

/********************************************************************************
* 기      능   : 그리드에서 날짜 입력시
********************************************************************************/
function grd_list_OnExpandEdit(obj,nRow,nCell,strVal,nPivotIndex)
{
	var rect = obj.GetCellRect(nRow, ncell);

	cal_temp.top = rect[1] + 100;
	if (cal_temp.top > 700)
	{
		cal_temp.top = 700;
	}
	cal_temp.Left = rect[0] + 20;
	if (cal_temp.Left > 790)
	{
		cal_temp.Left = 790;
	}
	cal_temp.Visible = true;

	if(nCell == 5)
		cal_temp.value = ds_list.getColumn(nRow, "INPUT_DATE");
	else if(nCell == 6)
		cal_temp.value = ds_list.getColumn(nRow, "CLOSE_DATE");

	cal_temp.SetFocus();
}
/********************************************************************************
* 기      능   : 달력에서 일자 선택했을 경우
********************************************************************************/
function cal_temp_OnDayClick(obj,strText)
{

	if(grd_list.GetClickedCell() == 5)
		ds_list.SetColumn(ds_list.row, "INPUT_DATE", strText);
	else if(grd_list.GetClickedCell() == 6)
		ds_list.SetColumn(ds_list.row, "CLOSE_DATE", strText);


	obj.Visible = false;
	global.FRM_BOTTOM.st_msg.value = "";
	global.FRM_BOTTOM.img_msgType.imageId = "";
}


/********************************************************************************
* 기      능   : 달력에서 포커스 잃었을 경우
********************************************************************************/
function cal_temp_OnKillFocus(obj)
{
	obj.Visible = false;
