/********************************************************************************
* 기      능   : 저장 
********************************************************************************/
function fn_save() {
	Trace(ds_list.saveXML());
	if(ds_list.FindRow("CHK", 1)==-1){
		gfn_showMsg('CW00046');
		return; 
	  }
	  
	for(var i=0;i<ds_list.GetRowCount();i++)
	{
		if(ds_list.GetColumn(i, "CHK") == '1' && Length(Trim(ds_list.GetColumn(i, "LIFNR"))) == 0)
		{
			gfn_openMsgWin("공급업체가 누락되었습니다.\n["+(i+1)+"행]");
			return;
		}
		
		if(ds_list.GetColumn(i, "CHK") == '1' && Length(Trim(ds_list.GetColumn(i, "GBN"))) == 0)
		{
			gfn_openMsgWin("업체구분이 누락되었습니다.\n["+(i+1)+"행]");
			return;
		}
		
		if(ds_list.GetColumn(i, "CHK") == '1' && Length(Trim(ds_list.GetColumn(i, "LIFNR"))) != 10)
		{
			gfn_openMsgWin("공급업체가는 10자리로 입력해주세요.\n["+(i+1)+"행]");
			return;
		}
		
		if(ds_list.GetColumn(i, "CHK") == '1' && ds_list.GetRowType(i) == 'insert')
		{
			var sLifnr = ds_list.GetColumn(i, "LIFNR");
			var sGbn   = ds_list.GetColumn(i, "GBN");
			
			var nFindRow = ds_list_all.SearchRow("LIFNR == '"+sLifnr+"' && GBN == '"+sGbn+"'");
			
			if(nFindRow >= 0)
			{
				gfn_openMsgWin("중복된 업체코드/업체구분이 존재합니다.\n["+(i+1)+"행]");
				return;
			}
		}
		
		
	}
  
    ds_list.Filter("CHK==1");
    ds_list2.ClearData();
	ds_list2.copyF(ds_list);
	ds_list.unFilter();	
	
	
    tit_clearActionInfo();				
    	
	tit_addSelectExActionInfo("ps13:PS1302001A_S2", "ps13:PS1302001A_I1", "ps13:PS1302001A_U1");
 	tit_callService(
		""
		, ""
		, "ds_list=ds_list2"
		, ""
		, ""
		, "fn_afterSave"
		, ""
		, ""
		, true  // ""
		
		);
}
