	// 인계일자
	if ( !gfn_checkFromToObj(cal_datef, cal_datet, true, st_date.value, 0, false) ) {
		return;
	}

	tit_clearActionInfo();
	tit_addSearchActionInfo("ps13:PS1301002A_S1");
		
	tit_callService(
        ""
        , ""
        , "ds_cond=ds_cond"
        , "ds_list=ds_list"
        , ""
        , "fn_afterQuery"
        , true);
}
/********************************************************************************
* 기      능   : 닫기  
********************************************************************************/
function fn_close() {
	close();
}

/********************************************************************************
* 기      능   : 저장 
********************************************************************************/
function fn_save() {
	if ( !gfn_isChangeDs(ds_list, true, "FLAG")){
		return;
	}

	tit_clearActionInfo();
	tit_addSaveActionInfo(
          ""
        , "ps13:PS1301002A_U2"
        , ""
        , "FLAG"
        , ""
        , -1
        , "");
	tit_callService(
        ""
        , ""
        , "ds_list=ds_list:A"
        , ""
        , ""
        , "fn_afterSave");
}
