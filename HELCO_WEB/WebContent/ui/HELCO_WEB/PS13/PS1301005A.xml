	  ds_cond.SetColumn(0,"STATS","Y");
	}
	//협력업체:저장,반송조회, 설치부:반송조회(윤리경영부추가)
	if(userGroup = '10'   || G_USER_ID = '2018900'|| G_USER_ID = '2022702'|| G_USER_ID = '2020912' || G_USER_ID = '2010500' || G_USER_ID = '2015129' || G_USER_ID = '2016427' || G_USER_ID ='5004977' ) 
