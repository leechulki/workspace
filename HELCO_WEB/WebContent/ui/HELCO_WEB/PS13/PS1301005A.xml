function fn_query() {
	if ( !gfn_checkFromToObj(cal_datef, cal_datet, true, st_date.value, 0, false) ) {
		return;
	}
	//반송된 정보 조회(윤리경영부추가)
	if(userGroup = '10' || G_USER_ID = '3018202' || G_USER_ID = '2017245' || G_USER_ID = '2018900' || G_USER_ID = '2022702' || G_USER_ID = '2020912' || G_USER_ID = '2010500' || G_USER_ID = '2015129' || G_USER_ID = '2016427' || G_USER_ID ='5004977' //  || G_USER_ID = '3018903' || 
	   ) 
