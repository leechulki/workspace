function grd_list_OnHeadClick(obj,nCell,nX,nY,nPivotIndex)
{
	if ( nCell == 0 ) {
		// 그리드 표시할 컬럼 선택 
		gfn_selectAllGrid(obj,nCell,nCell,true);
	} else {
		// 그리드 정렬 처리 
		gfn_sortGrid(obj,nCell);
	}
}
/********************************************************************************
* 기      능   : 리스트내용 변경시
********************************************************************************/
function ds_list_OnColumnChanged(obj,nRow,strColumnID,varOldValue,varNewValue,nPivotIndex)
{
	obj.fireevent = false;
	switch( strColumnID ) {
		case "CHK":
			var stats = ds_list.GetColumn(nRow,"STATS");
			//업체:상태가 저장일경우, 설치부:상태가 반송일경우 삭제가능
			if(userGroup == '10' && stats == 'A' && varNewValue == "1") ds_list.SetColumn(nRow,"FLAG","U");
			else if(userGroup == '40' && stats == 'C' && varNewValue == "1") ds_list.SetColumn(nRow,"FLAG","U");
			else
			{
				ds_list.SetColumn(nRow,"CHK","");
				ds_list.SetColumn(nRow,"FLAG","");
			}	
			break;
	}
	obj.fireevent = true;
}
/********************************************************************************
* 기      능   : 사업부 선택시 ds_lifnr 데이터셋 값 filtering
********************************************************************************/
function chk_so2_OnClick(obj,strValue)
{
var so = ' ';
	var so2 = ' ';
	var so3 = ' ';
	var so4 = ' ';
	var so5 = ' ';
	var so7 = ' ';
	
	if(chk_so.Value == '1')  so =  'X';
	if(chk_so2.Value == '1') so2 = 'X';
	if(chk_so3.Value == '1') so3 = 'X';
	if(chk_so4.Value == '1') so4 = 'X';
	if(chk_so5.Value == '1') so5 = 'X';
	if(chk_so7.Value == '1') so7 = 'X';
	

	if(chk_so.Value+chk_so2.Value+chk_so3.Value+chk_so4.Value+chk_so5.Value+chk_so7.Value == '000000')
	{
		ds_lifnr.Filter("SO5=='' || CODE==''");
	}	
	else
	{
	  ds_lifnr.Filter("CODE=='' || SO=="+quote(so) +" || SO2=="+quote(so2) +" || SO3=="+quote(so3) +" || SO4=="+quote(so4) +" || SO5=="+quote(so5) +" || SO7=="+quote(so7));
	}
	
	cb_lifnr.Index = 0;	
}
/********************************************************************************
* 기      능   : 입사:인원상세정보, 퇴사:퇴사이력정보 팝업호출
********************************************************************************/
function grd_list_OnCellDblClick(obj,nRow,nCell,nX,nY,nPivotIndex)
{
	if(nCell != 0 && ds_list.GetColumn(nRow,"RETIF") == '등록')
	{
		if(userGroup == '10') 
		{
//ALERT("A");		
			gfn_openPopup("PS1301005D",true,"arg=''");
		}
		else 
		{
//ALERT("B");		
			gfn_openPopup("PS1301005B",true,"arg=''");
		}
		fn_query();
	}	
	else if(nCell != 0 && ds_list.GetColumn(nRow,"RETIF") == '삭제') 
	{
		if(userGroup == '10') 
		{
			gfn_openPopup("PS1301005E",true,"arg=''");
		}
		else 
		{
			gfn_openPopup("PS1301005C",true,"arg=''");
			fn_query();
		}	
	}	
}
