function fn_query() {
	// 호기번호 검증
	if (length(HOGIN) > 9)
	{
		HOGIN = HOGIN.substring(0,9);
	}

	ds_list.ClearData();

	ds_cond.setColumn(0, "HOGIN", HOGIN);
	ds_cond.setColumn(0, "MATNR", MATNR);

	tit_clearActionInfo();
	tit_addSearchActionInfo("mm01:MM0102001B_S1", null, true);

	tit_callService(
        ""
        , ""
        , "ds_cond=ds_cond"
        , "ds_list=ds_list"
        , ""
        , "fn_afterQuery"
        , true);
}

function fn_query2() {
