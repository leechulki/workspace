/********************************************************************************
* 기      능   : 조회 
********************************************************************************/
function fn_query() {

	ds_cond.SetColumn(0, "VALS", "%"+ed_value.Text+"%");
	
	if (v_hogin.Text.length > 5) {
		//ds_cond.SetColumn(0, "WOKNUM" , v_hogin.Text);
		ds_cond.SetColumn(0,"WOKNUM",substr(fa_hogin,0,7));
	}
		
	tit_clearActionInfo();
	tit_addSearchActionInfo("mm01:MM0103004A_S1", null, true);
	
	if (ck_blk.Value==1) {

		var q_string = null;
		
		//test block 값 가져오기
		
		var idx = idx_value.Value;
		
		q_string = " AND (A.MATNR LIKE '";
		
		for (i=1; i < 15 ; i++) {
			var blk = ds_block.GetColumn(idx, "CS612_BLK"+i);
			
			if(i==1 && length(blk)>0)
				q_string += blk+"%'";
			else if (length(blk) > 0)
				q_string += " OR A.MATNR LIKE '"+blk+"%' ";
			else {
				break;
			}
		}
		q_string += ") ";
		
		tit_callService(
			  ""
			, ""
			, "ds_cond=ds_cond"
			, "ds_list1=ds_list1"
			, "BLOCK=" + quote( q_string )
			, "fn_afterQuery"
			, false
			, true);
	} else {
	// 일반 조회
		tit_callService(
			  ""
			, ""
			, "ds_cond=ds_cond"
			, "ds_list1=ds_list1"
			, ""
			, "fn_afterQuery"
			, true);
		//ck_blk.Text = "";
	}
}

