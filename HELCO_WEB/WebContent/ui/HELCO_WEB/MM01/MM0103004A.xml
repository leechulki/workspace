function fn_afterQuery(errCode, errMsg) {

	// 이미지가 있는 경우만 이미지 아이콘을 보여준다.
	 ds_list1.fireevent = false;

	for (var i =0 ; i < ds_list1.rowcount; i++)
	{

		for(var j=0; j < ds_list_temp.GetRowCount(); j++)
		{
			if(ds_list1.GetColumn(i,"MATNR") == ds_list_temp.GetColumn(j,"MATNR"))
			{
				ds_list1.SetColumn(i,"CHECK","O");
			}
		}
	}
	ds_list1.fireevent = true;
	
	//메시지 처리 
	gfn_showSearchMsg(grd_list1);
	
	
}

