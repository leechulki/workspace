function fn_OnLoadCompleted(obj) {
	// Form 로드 이벤트 맨 첫줄에 기술 
	gfn_initForm();
	gfn_initDs(ds_cond, true);

	fn_queryblock();
	ck_blk.Value = false;
		
	var codeArr = ["1"];
    var dsObjArr = [ds_gbn];
    var addTypeArr = [G_CODE_ALL];
    gfn_searchCommCode(codeArr, dsObjArr, addTypeArr);
    
    // Grid Format 지정 
    if (G_USER_GROUP_R == "")
    {
		grd_list1.Format = "MM";
    }
    else
    {
		grd_list1.Format = "Default";
    }
	
// ES, 무빙워크 기준
	
	if(substr(fa_hogin,6,1) == "S")
	{
		Tab00.tab1.Enable = true; 
		Tab00.tab2.Enable = false;
		Tab00.TabIndex = 0;
	} else {
		Tab00.tab1.Enable = false;
		Tab00.tab2.Enable = true;
		Tab00.TabIndex = 1;
	}
	v_hogin.Text = fa_hogin;
	
	ds_cond_temp.SetColumn(0,"WOKNUM",substr(fa_hogin,0,7));
	
	tit_clearActionInfo();
	tit_addSearchActionInfo("mm01:MM0103004A_S2");
	
	tit_callService(
        ""
        , ""
        , "ds_cond_temp=ds_cond_temp"
        , "ds_list_temp=ds_list_temp"
        , ""
        , ""
        , true);
}
