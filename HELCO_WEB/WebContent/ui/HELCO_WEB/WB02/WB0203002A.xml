/********************************************************************************
* 기      능   : 조회
********************************************************************************/
function fn_query() {
	// sort mark 삭제 
	gfn_clearGridSortMark(grd_list);
	ds_list.row = -1;

	// if (length(ed_act.value) < 3){
	    // var arr = ["계정코드","3"];
		// gfn_showAlert("CW00174",arr);
