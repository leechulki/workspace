********************************************************************************/
function grd_list_OnHeadClick(obj,nCell,nX,nY,nPivotIndex)
{
	if ( nCell == 0 ) {
		gfn_selectGridCol(obj);
	} else {
		gfn_sortGrid(obj,nCell);
	}
}


function ds_sum_OnCellPosChanged(obj,nOldRow,nRow)
{
	if (ds_sum.rowcount < 1) return;
	
	var kind = "";
	var acnt;
		
	if (ds_cond.GetColumn("MRK") == "1")
	{
		acnt = ds_sum.GetColumn(nRow,"CEACDAC1");
	}
	else
	{
		acnt = ds_sum.GetColumn(nRow,"CEACDAC2");
	}
	
	ds_cond.SetColumn(0,"ACT", ds_sum.GetColumn(nRow,"CEACDACT"));
	
	if (acnt == "EAKB03M" || acnt == "EAKB05M" ||
	    acnt == "EAKB07M" || acnt == "EAKB08M" ||
	    acnt == "EAKB14M" || acnt == "EAKB26M")
	{
		kind = "A";
	}
	else if (acnt == "EAKB09M" )
	{
		kind = "B";
	}
	else if (acnt == "EAKB10M" || acnt == "EAKB22M")
	{
		kind = "C";
	}
	else if (acnt == "EAKB11M" )
	{
		kind = "D";
	}
	else if (acnt == "EAKB12M" )
	{
		kind = "E";
	}
	else if (acnt == "EAKB13M" )
	{
		kind = "F";
	}
	else if (acnt == "EAKB15M" )
	{
		kind = "G";
	}
	else if (acnt == "EAKB20M" )
	{
		kind = "H";
	}
	else if (acnt == "EAKB21M" )
	{
		kind = "I";
	}
	else if (acnt == "EAKB24M" )
	{
		kind = "J";
	}	
	else if (acnt == "EAKB25M" )
	{
		kind = "K";
	}
	else 
	{
		return;
	}
	getDetail(kind);
	
}

function getDetail(desc)
{
	ds_cond.setColumn(0,"KIND",desc);
	tit_clearActionInfo();
	
	switch(desc)
	{
		case 'A':
				tit_addSearchActionInfo("wb02:WB0203002A_S2");
				break; 
		case 'B':
				tit_addSearchActionInfo("wb02:WB0203002A_S3");
				break; 
		case 'C':
				tit_addSearchActionInfo("wb02:WB0203002A_S4");
