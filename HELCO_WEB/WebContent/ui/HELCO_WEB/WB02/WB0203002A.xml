				break; 
		case 'H':
				tit_addSearchActionInfo("wb02:WB0203002A_S9");
				break; 
		case 'I':
				tit_addSearchActionInfo("wb02:WB0203002A_S10");
				break; 
		case 'J':
				tit_addSearchActionInfo("wb02:WB0203002A_S11");
				break; 
		case 'K':
				tit_addSearchActionInfo("wb02:WB0203002A_S12");
				break; 
	}

	tit_callService(
		"HOST_ACT"
		, ""
		, "ds_cond=ds_cond"
		, "ds_list=ds_list"
		, ""
		, "fn_afterDetailQuery"
		, true);

}

/********************************************************************************
* 기      능   : 상세조회 Callback  
********************************************************************************/
function fn_afterDetailQuery(errCode, errMsg) 
{
   // 조회 결과 > 0 
	if ( ds_list.rowCount() > 0 ) 
	{
		if (ds_cond.GetColumn(0,"KIND") == "A")
		{
			grd_list.Format  = "F1";
		}
		else if (ds_cond.GetColumn(0,"KIND") == "B")
		{
			grd_list.Format  = "F2";
		}
		else if (ds_cond.GetColumn(0,"KIND") == "C")
		{
			grd_list.Format  = "F3";
		}
		else if (ds_cond.GetColumn(0,"KIND") == "D")
		{
			grd_list.Format  = "F4";
		}
		else if (ds_cond.GetColumn(0,"KIND") == "E")
		{
			grd_list.Format = "F5";
		}
		else if (ds_cond.GetColumn(0,"KIND") == "F")
		{
			grd_list.Format  = "F6";
		}
		else if (ds_cond.GetColumn(0,"KIND") == "G")
		{
			grd_list.Format  = "F7";
		}
		else if (ds_cond.GetColumn(0,"KIND") == "H")
		{
			grd_list.Format  = "F8";
		}
		else if (ds_cond.GetColumn(0,"KIND") == "I")
		{
			grd_list.Format  = "F9";
		}
		else if (ds_cond.GetColumn(0,"KIND") == "J")
		{
			grd_list.Format  = "F10";
		}
		else if (ds_cond.GetColumn(0,"KIND") == "K")
