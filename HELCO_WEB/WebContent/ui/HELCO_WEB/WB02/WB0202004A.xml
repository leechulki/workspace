********************************************************************************/
function fn_query() {
	// 필수 입력사항 및 일자 체크
	if(gfn_isNull(cal_date, true, st_date.value)) return false;

	// sort mark 삭제 
	gfn_clearGridSortMark(grd_list);
	ds_list.row = -1;

	tit_clearActionInfo();
	tit_addSearchActionInfo("wb02:WB0202004A_S1");
	tit_callService(
        ""
        , ""
        , "ds_cond=ds_cond"
        , "ds_list=ds_list"
        , ""
        , "fn_afterQuery"
        , true
        , true);
}
