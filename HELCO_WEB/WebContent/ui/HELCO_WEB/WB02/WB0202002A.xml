/********************************************************************************
* 기      능   : 조회
********************************************************************************/
function fn_query() {
	// 필수 입력사항 및 일자 체크
	if(gfn_isNull(cal_datet, true, st_datet.value)) return false;

	tit_clearActionInfo();
	tit_addSearchActionInfo("wb02:WB0202002A_S1");
	tit_callService(
        ""
        , ""
        , "ds_cond=ds_cond"
        , "ds_list=ds_list"
        , ""
        , "fn_afterQuery"
        , true
        , true);
}
/********************************************************************************
* 기      능   : 삽입 
********************************************************************************/
function fn_insert() {
}
/********************************************************************************
* 기      능   : 저장 
********************************************************************************/
function fn_save() {
	if (ds_list.RowCount() == 0) {
		gfn_showAlert("CW00010");
		return;
	}
	
	if(med_amt3.Value > 0 && chk_mflag.Value != '1')
	{
        var arr = [st_amt3.Value, st_mflag.Value];
		gfn_showMsg("CW00004",arr);
		return;
	}
	
	tit_clearActionInfo();
	tit_addMultiActionInfo("wb02:WB0202002A_U1");

	tit_callService(
		""
		, ""
		, "ds_list=ds_list"
		, ""
		, ""
		, "fn_afterSave");
}
/********************************************************************************
* 기      능   : 저장 Callback  
********************************************************************************/
function fn_afterSave(errCode, errMsg) {
	if ( errCode != 0 ) {
		gfn_showMsg("CE00001");
	} else {
		gfn_showMsg("CI00003");		
	}
	fn_query();
}
/********************************************************************************
* 기      능   : 닫기  
********************************************************************************/
function fn_close() {
	close();
}
