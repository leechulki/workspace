/********************************************************************************
* 기      능   : 그리드 헤더 클릭시 정렬 처리 
********************************************************************************/
function cb_gbn_OnChanged(obj,strCode,strText,nOldIndex,nNewIndex)
{
	if(cb_gbn.Value == '1')
	{
		st_num.Text = '주주번호';
		ed_num.MaxLength = 15;
	}	
	if(cb_gbn.Value == '2')
	{
		st_num.Text = '주민번호';
		ed_num.MaxLength = 5;
	}	
	if(cb_gbn.Value == '3')
	{
		st_num.Text = '주민번호';
		ed_num.MaxLength = 15;
	}	
}
/********************************************************************************
* 기      능   : print 40
********************************************************************************/
function fn_print() 
{
	if (ds_list.RowCount() == 0) {
		gfn_showAlert("CW00010");
		return;
	}

	if(length(cal_kidate.Value) != 8)
	{
		gfn_showMsg("CW00008",st_kidate.Value);
		cal_kidate.SetFocus();
		return;
	}
	if(trim(ed_kisu.text) == '' || trim(ed_kisu.Text) <= 0 || trim(ed_kisu.text) == null)
	{
		gfn_showMsg("CW00038",st_kisu.Value);
		ed_kisu.SetFocus();
		return;
	}

	ds_list_p.ClearData();
	ds_list_p.AddRow();
	ds_list_p.CopyRow(0, "ds_list", ParseInt(ds_list.row));

	if(ds_cond_copy.GetColumn(0,"GBN") == "3")
	{
		var qty = 0;
		for(i=0; i<ds_list.RowCount(); i++)
		{
			qty += parseInt(ds_list.GetColumn(i,"QTY"));
		}
		ds_list_p.SetColumn(0,"QTY",qty);
		
		if(ds_list.RowCount() > 1)
			ds_list_p.SetColumn(0,"NAM",ds_list.GetColumn(ParseInt(ds_list.row),"NAM2")+" 외");
		else
			ds_list_p.SetColumn(0,"NAM",ds_list.GetColumn(ParseInt(ds_list.row),"NAM2"));
	}
	else
	{
		ds_list_p.SetColumn(0,"NAM",ds_list.GetColumn(ParseInt(ds_list.row),"NAM2"));
	}
	
	ds_list_p.SetColumn(0,"YEAR",substr(cal_kidate.Value,0,4));
	ds_list_p.SetColumn(0,"MONTH",substr(cal_kidate.Value,4,2));
	ds_list_p.SetColumn(0,"DAY",substr(cal_kidate.Value,6,2));
	
	// UbiReport Call
	Dialog("WB02::WB0202002P.xml", "");
}
/********************************************************************************
* 기      능   : 그리드 헤더 클릭시 정렬 처리 
********************************************************************************/
function grd_list_OnHeadClick(obj,nCell,nX,nY,nPivotIndex)
{
	if ( nCell == 0 ) {
		// 그리드 표시할 컬럼 선택 
		gfn_selectGridCol(obj);
	} else {
		// 그리드 정렬 처리 
		gfn_sortGrid(obj,nCell);
	}
}
/********************************************************************************
* 기      능   : 실지급액 = 배당금* 주식수- 원천세
********************************************************************************/
function med_amt_OnCharChanged(obj,strPreText,strPostText)
{
	med_amt3.Value = ParseInt(med_amt1.Value) * ParseInt(med_qty.Value) - ParseInt(med_amt2.Value);
	
	if(med_amt3.Value > 0) chk_mflag.Value = '1';
	else chk_mflag.Value = '0';
	
}
