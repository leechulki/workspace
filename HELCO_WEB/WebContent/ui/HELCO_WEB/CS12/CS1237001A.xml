/********************************************************************************
* 기      능   : 그리드 헤더 클릭시 정렬 처리 
********************************************************************************/
function grd_list_OnHeadClick(obj,nCell,nX,nY,nPivotIndex) {
	if(nCell == 0) {
		// 전체 선택
		gfn_selectAllGrid2(obj,0,0,true);
	} else {
		// 그리드 정렬 처리
		gfn_sortGrid(obj,nCell);
	}
}

/********************************************************************************
* 기      능   : 액셀  
********************************************************************************/
function fn_excel() {
	gfn_saveToExcel(grd_list, true);
}

/********************************************************************************
* 기      능   : 저장
********************************************************************************/
function fn_save(){
	
	gfn_clearMsgBar();

    if(ds_list.FindRow("CHK", 1) == -1) {
		gfn_showMsg("CW00013");
		return;
	}

	ds_list2.ClearData();
	ds_save.ClearData();
	
	var j = 0;
	for(var i=0; i<ds_list.RowCount(); i++) {					
		if(ds_list.GetColumn(i, "CHK") == true) {		
			ds_list2.AddRow();
			ds_list2.SetColumn(j, "FLAG", "U");
			ds_list2.SetColumn(j, "GNO", ds_list.GetColumn(i, "GNO"));

			ds_save.AddRow();
			ds_save.SetColumn(j, "WBS", ds_list.GetColumn(i, "GNO"));
			ds_save.SetColumn(j, "CYN", ds_list.GetColumn(i, "CYN"));
			ds_save.SetColumn(j, "RMK", ds_list.GetColumn(i, "RMK"));
			j++;
		}
	}

	tit_clearActionInfo();
	// tit_addSaveActionInfo(
		// ""
		// , "cs12:CS1237001A_I1"
		// , ""
		// , "FLAG");
	tit_addMultiActionInfo("cs12:CS1237001A_I2"); 
	tit_callService(
		""
		, ""
		, "ds_list2=ds_list2 ds_save=ds_save"
		, ""
		, ""
		, "fn_afterSave"
		, true);
}

/********************************************************************************
* 기      능   : 저장 Callback  
********************************************************************************/
function fn_afterSave(errCode, errMsg) {
	if(errCode != 0) {
		gfn_showMsg("CE00001");
	} else {
		gfn_showMsg("CI00005");
		fn_query();
	}
}

/********************************************************************************
* 기      능   : 닫기  
********************************************************************************/
function fn_close() {
	close();
}

function btn_upn_OnClick(obj)
{
	var arg = "upn="+quote(ed_upn.Text);
	gfn_openPopup("CS9601001A",true,arg);
}




