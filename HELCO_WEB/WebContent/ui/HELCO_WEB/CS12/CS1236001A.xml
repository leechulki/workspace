	gfn_saveToExcel(grd_list, true);
}

function btn_pjt_OnClick(obj) {
	var arg = "pjt="+quote(ed_pjt.Text);
	gfn_openPopup("CS9601002A",true,arg);
}

/********************************************************************************
* 기      능   : 저장
********************************************************************************/
function fn_save(){
	
	gfn_clearMsgBar();

    if(ds_list.FindRow("CHK", 1) == -1) {
		gfn_showMsg("CW00013");
		return;
	}

	ds_list2.ClearData();
	
	var j = 0;
	for(var i=0; i<ds_list.RowCount(); i++) {					
		if(ds_list.GetColumn(i, "CHK") == true) {		
			ds_list2.AddRow();
			ds_list2.SetColumn(j, "FLAG", "U");
			ds_list2.SetColumn(j, "PJT", ds_list.GetColumn(i, "PJT"));
			ds_list2.SetColumn(j, "HNO", ds_list.GetColumn(i, "HNO"));
			ds_list2.SetColumn(j, "TEM", ds_list.GetColumn(i, "TEM"));
			ds_list2.SetColumn(j, "RDT", ds_list.GetColumn(i, "RDT"));
			ds_list2.SetColumn(j, "SEQ", ds_list.GetColumn(i, "SEQ"));
			ds_list2.SetColumn(j, "SIR", ds_list.GetColumn(i, "SIR"));
			ds_list2.SetColumn(j, "AYN", "Y");

			j++;
		}
	}

	tit_clearActionInfo();
	tit_addSaveActionInfo(
		""
		, "cs12:CS1236001A_I1"
		, ""
		, "FLAG");
	tit_callService(
		""
		, ""
		, "ds_list2=ds_list2"
		, ""
		, ""
		, "fn_afterSave"
		, true);
}

/********************************************************************************
* 기      능   : 저장 Callback  
********************************************************************************/
function fn_afterSave(errCode, errMsg) {
	if(errCode != 0) {
		gfn_showMsg("CE00001");
	} else {
		gfn_showMsg("CI00005");
		fn_query();
	}
}

/********************************************************************************
* 기      능   : 닫기  
********************************************************************************/
function fn_close() {
	close();
}
