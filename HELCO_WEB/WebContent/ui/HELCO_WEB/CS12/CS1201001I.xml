/********************************************************************************
* 기      능   : 계약사용 : 계약데이터로 사용
********************************************************************************/
function btn_set_cont_OnClick(obj)
{
	gfn_clearMsgBar();
	
	checkedRow = ds_list.FindRow("CHK", 1);
	
	if(checkedRow == -1) {
		gfn_showMsg("CW00013");
		return;
	} else {
		if (cb_jpgbn.value == "01" && ds_list.GetColumn(checkedRow,"GBU") == "3"){
			if (ds_list.GetColumn(checkedRow,"BDGBN") != "1"){
				alert("'분담이행방식'이 아닌 견적은 '계약생성' 할 수 없습니다.");
				return;
			}
		}
		
		
		//========================분담이행은 직영/자회사로 계약생성 불가===============================
		var tem, rdt, seq, sir;
		
		var tmps = Split(ds_list.GetColumn(checkedRow,"ETN"), "-");
		tem = tmps[0];
		rdt = tmps[1];
		seq = tmps[2];
		sir = ds_list.GetColumn(checkedRow,"SIR");
		
		ds_cond2.SetColumn(0, "TEM", tem);
		ds_cond2.SetColumn(0, "RDT", rdt);
		ds_cond2.SetColumn(0, "SEQ", seq);
		ds_cond2.SetColumn(0, "SIR", sir);
		
		tit_clearActionInfo();
		tit_addSearchActionInfo("cs12:CS1201001I_S3");
		tit_callService(
			""
			, ""
			, "ds_cond2=ds_cond2"
			, "ds_chk_cnt=ds_chk_cnt"
			, ""
			, ""
			, ""
			, ""
			, true);
			
		if ( ds_chk_cnt.GetColumn(0,"BD_CNT") > 0 ) {
			alert("'분담이행방식'으로 작성된 견적에 현재 '직영/자회사' 귀속된 호기가 존재합니다.");
			return;		
		}			
		//=============================================================================================
		
	}
	
	var etn = ds_list.GetColumn(checkedRow,"ETN");
	var cs301_sir = ds_list.GetColumn(checkedRow,"SIR");
	var cs301_upn = ds_list.GetColumn(checkedRow,"UPN");
	var cs301_cst = ds_list.GetColumn(checkedRow,"CST");
	
	//CS301_TEM || '-' || CS301_RDT || '-' || CS301_SEQ ETN	--견적번호
	//cs301_sir 차수
	
	var arrEtn = split(etn, "-");
	
	cs301_tem = arrEtn[0];
	cs301_rdt = arrEtn[1];
	cs301_seq = arrEtn[2];
	
	var arr = [cs301_tem, cs301_rdt, cs301_seq, cs301_sir, cs301_upn, cs301_cst];
	 
	close(arr);
}

function grd_list_OnCellClick(obj,nRow,nCell,nX,nY,nPivotIndex)
{
	if (nCell==0) {
	
		var dataset = object(obj.BindDataset);
		var colId = obj.GetCellProp("Body", nCell, "colId");
		for(row = 0; row < dataset.rowcount; row++) {
			if (nRow<> row) {
			 dataset.SetColumn(row, colId, "0");	
			}
		}
	}
}

