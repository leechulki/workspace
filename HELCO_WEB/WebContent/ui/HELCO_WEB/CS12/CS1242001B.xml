// Form Loading / Unloading 시 작업 정의
//=========================================================================================
function fn_OnLoadCompleted(obj) {
	gfn_initForm();

	var codeArr = ["CS184"];
	var dsObjArr = [ds_gbn_grd];
	var addTypeArr = [G_CODE_SELECT];
	gfn_searchCommCode(codeArr, dsObjArr, addTypeArr);

	ds_cond.SetColumn(0, "GNO", gno);
	ds_cond.SetColumn(0, "HIS", his);
	ds_cond.SetColumn(0, "UPN", upn);
	ds_cond.SetColumn(0, "CST", cst);
trace("1:"+frm);
	ufn_setInitGno();	
}

/********************************************************************************
* 기      능   : form UnloadCompleted 
********************************************************************************/
function fn_OnUnloadCompleted(obj) {
	return gfn_isFormUnload(obj);
}

function fn_completeInit(sCode, sDsObj, sCodeType) {

}

function ufn_setInitGno() {
	var t_sql = "";

	if(frm == "B") { // 상세버튼 클릭시
		ds_condGno.SetColumn(0, "GNO", gno);
		ds_condGno.SetColumn(0, "UPN", substr(gno,0,6));
		ds_condGno.SetColumn(0, "CST", substr(gno,12,3));

		t_sql = "cs12:CS1242001B_S4";	//계약번호 리스트
	} else { // 입력버튼 클릭시
		ds_condGno.SetColumn(0, "GNO", "");
		ds_condGno.SetColumn(0, "UPN", upn);
		ds_condGno.SetColumn(0, "CST", cst);

		t_sql = "cs12:CS1242001B_S5";	//계약번호 리스트(현재 계약이 만료되지 않은 건만)
	}

	tit_clearActionInfo();
	tit_addSearchActionInfo(t_sql);
	tit_callService(
        ""
        , ""
        , "ds_condGno=ds_condGno"
        , "ds_listGno=ds_listGno"
        , ""
        , "ufn_afterSetInitGno"
        , true
        , true
        , true);
}

function ufn_afterSetInitGno() {
	ed_chk.Text = "";

	if(frm == "B") {
		cb_gno.Value = gno;
	} else {
		cb_gno.Index = 0;
	}
	fn_query();
}

