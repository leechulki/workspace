function calc_Amt() {
	ds_list.FireEvent = false;

	for(var i=0; i<ds_list.RowCount(); i++) {
		var t_knd = ds_list.GetColumn(i, "KND");
		var t_abg = ds_list.GetColumn(i, "ABG");
		var t_hbn = ds_list.GetColumn(i, "HBN");
		var t_amt_a = ds_list.GetColumn(i, "AMT_A");
		var t_bam_a = 0;

/*
기성비 2015년도 POG 4.2% 인상 적용 201503
*/

		if(ds_list.GetColumn(i, "CHK") == true) {
			if (ds_list.GetColumn(i, "BDGBN") != "1"){
				if(t_hbn == "L") { // EL
					if(t_knd == "Y") { // FM
						t_bam_a = round((t_amt_a * 0.53),0);
						ds_list.SetColumn(i, "BAM_A", t_bam_a);
					} else { // POG
						if(t_abg == "A") { // 아파트
							t_bam_a = round((t_amt_a * 0.742),0);
							ds_list.SetColumn(i, "BAM_A", t_bam_a);
						} else { // 빌딩
							t_bam_a = round((t_amt_a * 0.642),0);
							ds_list.SetColumn(i, "BAM_A", t_bam_a);
						}
					}
				} else if(t_hbn == "S" || t_hbn == "W") { // ES, MW
					// 에스컬레이터 무빙워크 pog인 경우만 인상
					if(t_knd == "Y") { // FM
						t_bam_a = round((t_amt_a * 0.53),0);
						ds_list.SetColumn(i, "BAM_A", t_bam_a);
					} else {// POG
						t_bam_a = round((t_amt_a * 0.642),0);
						ds_list.SetColumn(i, "BAM_A", t_bam_a);
					}
				} else { // 주차
					t_bam_a = round((t_amt_a * 0.60),0);
					ds_list.SetColumn(i, "BAM_A", t_bam_a);
				}
			}
		}
	}

	ds_list.FireEvent = true;
}

function grd_list_OnKillFocus(obj) {
	calc_Amt();
}







