function fn_draft() {
	
	gfn_clearMsgBar();
	
	if(ds_List.FindRow("CHK", 1)==-1){
		gfn_showMsg('CW00046');
		return;
	}
	
	// 2018.12.12 매출계획 기안 시 계획년월이 최대 6자리 초과 시 기안하지 못하도록 예외로직 추가. 임유진SW 요청
	// Han J.H
	for(var i=0; i<ds_list.rowCount(); i++ ) {
		if(ds_list.getColumn(i, "CHK") == true) {
			if(length(trim(ds_list.getColumn(i, "YYM_A"))) > 6) {
				var idx = i + 1;
				gfn_showAlert(idx + "번째 행의 계획년월이 최대 6자리수를 초과합니다. \n" + "계획년월 값 변경 후 기안 바랍니다. \n" + "값: " + ds_list.getColumn(i, "YYM_A"));
				return;
			}
		}
	}
	
	// 변경 기안 입력 가능 여부 체크
	fn_inputCheck();
	
	if (ds_list_chk.RowCount() > 0) {
		var text = ds_cond.GetColumn(0, "GNO");
		gfn_showConfirm( text + " 계약번호로 현재 결재 진행 중인 문서가 존재합니다."); 
	} else {	
		fn_docgen();
	}
}

