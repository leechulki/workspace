// [ PART 3 ]
// Main Process Event 정의(조회,입력,삭제,저장,인쇄,form닫기)
//=========================================================================================
/********************************************************************************
* 기      능   : 조회 1
********************************************************************************/
function fn_query() {
trace("2:"+frm);
	if(frm == "B") {
		fn_query1();		//상세화면
	} else {
		fn_query2();		//입력화면
	}
}

function fn_query1() {	
	ds_cond.SetColumn(0, "GNO", cb_gno.Value);
	ds_cond.SetColumn(0, "HIS", his);
	ds_cond.SetColumn(0, "UPN", upn);
	ds_cond.SetColumn(0, "CST", cst);

	tit_clearActionInfo();
	tit_addSearchActionInfo("cs12:CS1242001B_S1");	//매출변경 등록한 자료 조회

	tit_callService(
        ""
        , ""
        , "ds_cond=ds_cond"
        , "ds_list=ds_list"
        , ""
        , "fn_afterQuery"
        , true
        , true
        , true);
}

function fn_query2() {
	ds_cond.SetColumn(0, "GNO", cb_gno.Value);
	ds_cond.SetColumn(0, "HIS", his);
	ds_cond.SetColumn(0, "UPN", upn);
