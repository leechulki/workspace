	tit_callService(
        ""
        , ""
        , "ds_cond=ds_cond"
        , "ds_list=ds_list"
        , ""
        , "fn_afterQuery2"
        , true
        , true
        , true);
}

function ufn_selData(obj,strCode,strText,nOldIndex,nNewIndex) {
	ds_cond.SetColumn(0, "GNO", cb_gno.Value);

	var t_sql = "";

	if(frm == "B") {
		t_sql = "cs12:CS1242001B_S7";
	} else {
		t_sql = "cs12:CS1242001B_S2";
	}

	tit_clearActionInfo();
	tit_addSearchActionInfo(t_sql);

	tit_callService(
        ""
        , ""
        , "ds_cond=ds_cond"
        , "ds_list=ds_list"
        , ""
        , "fn_afterQuery3"
        , true
        , true
        , true);
}

