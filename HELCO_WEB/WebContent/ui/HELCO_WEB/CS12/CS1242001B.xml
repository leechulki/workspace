function ufn_draftRe() {

	ds_list.fireevent = false;
	gfn_copyDs(ds_list, ds_list_copy, "CHK=1");
	ds_list.fireevent = true;
//	ds_list_copy.Sort("MYM", true, false, true);
	ds_list_copy.Sort("MYM", true, false, false);
	
	ds_list_copy2.ClearData();
	ds_list_copy.fireevent = false;	
	var k = 0;
	for (var i=0; i<ds_list_copy.RowCount(); i++) {
		var j = i+1;
		var m1 = ds_list_copy.GetColumn(i, "MYM") ;
		var m2 = ds_list_copy.GetColumn(j, "MYM") ;
		if ( m1 != m2 ) { 			
			var nrow = ds_list_copy2.AddRow();
			ds_list_copy2.SetColumn(nrow, "UPN", ds_list_copy.GetColumn(i, "UPN"));
			ds_list_copy2.SetColumn(nrow, "CST", ds_list_copy.GetColumn(i, "CST"));
			ds_list_copy2.SetColumn(nrow, "PJT", ds_list_copy.GetColumn(i, "PJT"));
			ds_list_copy2.SetColumn(nrow, "MYM", ds_list_copy.GetColumn(i, "MYM"));
			ds_list_copy2.SetColumn(nrow, "YYM", ds_list_copy.GetColumn(i, "YYM"));
			ds_list_copy2.SetColumn(nrow, "YDT", ds_list_copy.GetColumn(i, "YDT"));
			ds_list_copy2.SetColumn(nrow, "GNO", ds_list_copy.GetColumn(i, "GNO"));
			
			var cntc = ds_list_copy.Count(k, i-k+1);
			
			ds_list_copy2.SetColumn(nrow, "CNT", cntc);
			ds_list_copy2.SetColumn(nrow, "AMT", ds_list_copy.Sum("AMT",k,cntc));
			ds_list_copy2.SetColumn(nrow, "HMT", ds_list_copy.Sum("HMT",k,cntc));
			ds_list_copy2.SetColumn(nrow, "DMT", ds_list_copy.Sum("DMT",k,cntc));
			//===========================공기청정기 추가 20200515 Han.JH====================================
			ds_list_copy2.SetColumn(nrow, "ACMT", ds_list_copy.Sum("ACMT",k,cntc));
			//======================================================================================
			ds_list_copy2.SetColumn(nrow, "YYM_A", ds_list_copy.GetColumn(i, "YYM_A"));
			ds_list_copy2.SetColumn(nrow, "YDT_A", ds_list_copy.GetColumn(i, "YDT_A"));
			ds_list_copy2.SetColumn(nrow, "AMT_A", ds_list_copy.Sum("AMT_A",k,cntc));
			ds_list_copy2.SetColumn(nrow, "HMT_A", ds_list_copy.Sum("HMT_A",k,cntc));
			ds_list_copy2.SetColumn(nrow, "DMT_A", ds_list_copy.Sum("DMT_A",k,cntc));
			//===========================공기청정기 추가 20200515 Han.JH====================================
			ds_list_copy2.SetColumn(nrow, "ACMT_A", ds_list_copy.Sum("ACMT_A",k,cntc));
			//======================================================================================

			k = i+1;	
		}
	}	
	ds_list_copy.fireevent = true;

	var arg = "fa_gno=" + quote(cb_gno.value) 
			+ " fa_doc=" + quote(ds_docno_gen.GetColumn(0, "DOCNO"));
	gfn_openPopup("CS1242001D", true, arg);	
	
	div_btn.btn_draft.enable = false;
}

