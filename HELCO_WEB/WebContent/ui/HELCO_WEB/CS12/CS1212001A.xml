function grd_list_OnHeadClick(obj,nCell,nX,nY,nPivotIndex) {
	if(nCell == 0) {
		// 현재 처리할 format 기억
		gfn_selectGridCol("grd_list");
	} else {
		gfn_sortGrid(obj,nCell);
	}
}

/********************************************************************************
* 기      능   : 액셀  
********************************************************************************/
function fn_excel() {
	gfn_saveToExcel(grd_list, true, , , , true);
}

function cb_sel_OnChanged(obj,strCode,strText,nOldIndex,nNewIndex) {
	if(cb_sel.Value == "A") {
		ds_gkd_cd.Filter("CHK == " + quote(cb_sel.Value));
		ds_gkd_cd.InsertRow();
		ds_gkd_cd.SetColumn(0, "CODE", "");
		ds_gkd_cd.SetColumn(0, "CODE_NAME", "- 선택 -");
		ds_gkd_cd.SetColumn(0, "CHK", "");
		cb_gkd.Index = 0;
		cb_gkd.Enable = true;
	} else if(cb_sel.Value == "B") {
		ds_gkd_cd.Filter("CHK == " + quote(cb_sel.Value));
		ds_gkd_cd.InsertRow();
		ds_gkd_cd.SetColumn(0, "CODE", "");
		ds_gkd_cd.SetColumn(0, "CODE_NAME", "- 선택 -");
		ds_gkd_cd.SetColumn(0, "CHK", "");
		cb_gkd.Index = 0;
		cb_gkd.Enable = true;
	} else {
		cb_gkd.Index = 0;
		cb_gkd.Enable = false;
	}
}

function btn_pjt_OnClick(obj) {
	var arg = "pjt="+quote(ed_pjt.Text);
	gfn_openPopup("CS9601002A",true,arg);
}
