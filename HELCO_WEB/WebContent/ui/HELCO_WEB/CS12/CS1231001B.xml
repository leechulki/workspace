********************************************************************************/
function fn_copy() {
	gfn_clearMsgBar();

	var t_qtdat = cal_qtdat.value;
	var t_recost = med_recost.value;
	var t_dipbx = med_dipbx.value;
	var t_mamt = med_mamt.value;
	var t_knd = chk_knd.value;
	var t_yusd = cal_yusd.value;
	var t_yuhj = cal_yuhj.value;
	var t_eterm = cb_eterm.value;
	var t_ybstdk = cal_ybstdk.value;
	var t_bigo = ed_bigo.value;
	
	for(var i=0; i<parseInt(ed_rows.value); i++) {
		var t_num = parseInt(parent.ds_list.GetColumn(i, "NO").Text);

		if(parent.ds_list.GetColumn(i, "CHK") == true) {
			parent.ds_list.SetColumn(i, "QTDAT", t_qtdat);
			parent.ds_list.SetColumn(i, "RECOST", t_recost);
			parent.ds_list.SetColumn(i, "DIPBX", t_dipbx);
			parent.ds_list.SetColumn(i, "MAMT", t_mamt);
			parent.ds_list.SetColumn(i, "KND", t_knd);
			parent.ds_list.SetColumn(i, "YUSD", t_yusd);
			parent.ds_list.SetColumn(i, "YUHJ", t_yuhj);
			parent.ds_list.SetColumn(i, "ETERM", t_eterm);
			parent.ds_list.SetColumn(i, "YBSTDK", t_ybstdk);
			parent.ds_list.SetColumn(i, "BIGO", t_bigo);
		}
	}

	fn_close();
}
