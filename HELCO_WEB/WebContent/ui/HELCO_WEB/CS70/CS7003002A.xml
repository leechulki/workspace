/********************************************************************************
* 기      능   : Delete 
********************************************************************************/
function fn_delete() {
	if(ds_list2.rowcount == 0) {
		gfn_showMsg("CW00010");
		return;
	}

	if(ds_list2.GetColumn(0, "PST") == 'A5') {
		gfn_showAlert("이미 접수되어 삭제가 불가능 합니다.");
		return;
	}

	if(ds_list2.GetColumn(0, "PST") == 'A6') {
		gfn_showAlert("이미 승인되어 삭제가 불가능 합니다.");
		return;
	}

	if(ds_list2.GetColumn(0, "MID") <> G_USER_ID) {
		gfn_showAlert("본인이 작성한 내역만 삭제가 가능합니다.");
		return;
	}

	tit_clearActionInfo();
	tit_addSingleActionInfo("cs70:CS7003002A_U1");

	tit_callService(
		""
		, ""
		, "ds_cond2=ds_cond2"
		, ""
		, ""
		, "fn_afterDelete"
		, true);	
}

/********************************************************************************
* 기      능   : 삭제 Callback  
********************************************************************************/
function fn_afterDelete(errCode, errMsg) {
	if(errCode != 0) {
		gfn_showMsg("CE00001");
	} else {
		gfn_showMsg("CI00003");
		fn_close();
	}
}

function btn_pjt_OnClick(obj) {
	var arg = "pjt="+quote(Tab00.tab1.ed_pjt.Text)+" tmp_gbn=D";
	gfn_openPopup("CS9601002A",true,arg);
}
