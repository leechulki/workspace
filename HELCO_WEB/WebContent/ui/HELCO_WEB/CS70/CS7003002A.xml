#include "LIB::commons.js";	// 공통 스크립트 
#include "LIB::utility.js";	// 다중 선택 
//=========================================================================================
// [ PART 1 ]
// Form에서 사용될 전역변수를 선언
// FORM에서 사용되는 전역변수는 대문자로 작성하며, F_XXX 의 형태를 갖도록 한다. 
//=========================================================================================
var gRow = null;
//=========================================================================================
// [ PART 2 ]
// Form Loading / Unloading 시 작업 정의
//=========================================================================================
function fn_OnLoadCompleted(obj) {
	gfn_initForm();

	if(gubun == 'I') {
		ds_cond2.SetColumn(0,"SEQ","0");

		div_btn.btn_save.enable = true;
		div_btn.btn_delete.enable = false;
	} else if(gubun == 'U') {
		ds_cond2.SetColumn(0,"SEQ",seq);

		if(ds_list2.GetColumn(0,"PST") == 'A1') { //작성
			div_btn.btn_save.enable = true;
			div_btn.btn_delete.enable = true;
		} else if(ds_list2.GetColumn(0,"PST") >= 'A5') { //접수,승인
			div_btn.btn_save.enable = false;
			div_btn.btn_delete.enable = false;
		}
	}

	fn_query();
}

/********************************************************************************
* 기      능   : 공통코드가 조회된 후의 Callback 함수 
********************************************************************************/
function fn_completeInit(sCode, sDsObj, sCodeType) {

}

/********************************************************************************
* 기      능   : form UnloadCompleted 
********************************************************************************/
function fn_OnUnloadCompleted(obj) {
	return gfn_isFormUnload(obj);
}

//=========================================================================================
// [ PART 3 ]
// Main Process Event 정의(조회,입력,삭제,저장,인쇄,form닫기)
//=========================================================================================
/********************************************************************************
* 기      능   : 조회 
********************************************************************************/
function fn_query() {
	gRow = null;

	var t_sql = "";

	if(gubun == 'I') {
		t_sql = "cs70:CS7003002A_S1";
	} else {
		t_sql = "cs70:CS7003002A_S2";
	}

	tit_clearActionInfo();
	tit_addSearchActionInfo(t_sql);

	tit_callService(
        ""
        , ""
        , "ds_cond2=ds_cond2"
        , "ds_list2=ds_list2"
        , ""
        , "fn_afterQuery"
        , true
        , true);
}

/********************************************************************************
* 기      능   : 조회 Callback  
********************************************************************************/
function fn_afterQuery(errCode, errMsg) {

}

/********************************************************************************
* 기      능   : 저장
********************************************************************************/
function fn_save() {
	if(ds_list2.GetColumn(0, "PST") == 'A5' && G_USER_ID <> "1001396") {
		gfn_showAlert("이미 접수되어 저장이 불가능 합니다.");
		return;
	}

	if(ds_list2.GetColumn(0, "PST") == 'A6') {
		gfn_showAlert("이미 승인되어 저장이 불가능 합니다.");
		return;
	}

	if(gubun == 'U' && ds_list2.GetColumn(0, "MID") <> G_USER_ID && G_USER_ID <> "1001396") {
		gfn_showAlert("본인이 작성한 내역만 수정이 가능합니다.");
		return;
	}

	if(gfn_isNull(Tab00.tab1.ed_pjt, true, Tab00.tab1.st_pjt.Text)) return false; // 공사번호
	if(gfn_isNull(Tab00.tab1.ed_pnm, true, Tab00.tab1.st_pnm.Text)) return false; // 프로젝트명
	if(gfn_isNull(Tab00.tab1.ed_cnm, true, Tab00.tab1.st_cnm.Text)) return false; // 회사명
	if(gfn_isNull(Tab00.tab1.ed_nam, true, Tab00.tab1.st_nam.Text)) return false; // 요청자성명
	if(gfn_isNull(Tab00.tab1.cb_bgn, true, Tab00.tab1.st_bgn.Text)) return false; // 보수유형
	if(gfn_isNull(Tab00.tab1.cb_rst, true, Tab00.tab1.st_rst.Text)) return false; // 처리결과

	if(length(trim(Tab00.tab1.cal_bdy.Value)) == 0) { // 보수일자
		gfn_showMsg("보수일자는 필수입력 항목입니다. 확인하여 주십시오.");
		return false;
	}

	var lt01, lt02, lt03, lt04, lt05, lt06;

	lt01 = length(trim(Tab00.tab4.ed_t01.Text));
	lt02 = length(trim(Tab00.tab4.ed_t02.Text));
	lt03 = length(trim(Tab00.tab4.ed_t03.Text));
	lt04 = length(trim(Tab00.tab4.ed_t04.Text));
	lt05 = length(trim(Tab00.tab4.ed_t05.Text));
	lt06 = length(trim(Tab00.tab4.ed_t06.Text));

	if(lt01 > 0 || lt02 > 0 || lt03 > 0 || lt04 > 0 || lt05 > 0 || lt06 > 0) {
		if(G_USER_ID <> "1001396") { // 조용원SM 승인가능
			gfn_showAlert("결재지시사항은 관리자만 입력가능합니다.");
			return;
		}
	}

	tit_clearActionInfo();

	if(gubun == 'I') {
		tit_addSingleActionInfo("cs70:CS7003002A_I1");
	} else if(gubun == 'U') {
		tit_addSingleActionInfo("cs70:CS7003002A_U2");
	}

	tit_callService(
		""
		, ""
		, "ds_list2=ds_list2"
		, ""
		, ""
		, "fn_afterSave"
		, true);
}

/********************************************************************************
* 기      능   : 저장 Callback
********************************************************************************/
function fn_afterSave(errCode, errMsg) {
	if(errCode != 0) {
		gfn_showMsg("CE00001");
	} else {
		gfn_showMsg("CI00003");
		fn_close();
	}
}

/********************************************************************************
* 기      능   : 닫기  
********************************************************************************/
function fn_close() {
	close();
}

