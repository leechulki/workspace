#include "LIB::commons.js";	// 공통 스크립트 
#include "LIB::utility.js";	// 다중 선택 
//=========================================================================================
// [ PART 1 ]
// Form에서 사용될 전역변수를 선언
// FORM에서 사용되는 전역변수는 대문자로 작성하며, F_XXX 의 형태를 갖도록 한다. 
//=========================================================================================

//=========================================================================================
// [ PART 2 ]
// Form Loading / Unloading 시 작업 정의
//=========================================================================================
function fn_OnLoadCompleted(obj) {
	gfn_initForm(true);

	cal_fdt.Value = left(gfn_getCurrDate(),6)+"01";
	cal_tdt.Value = gfn_getCurrDate();

	if(G_USER_ID == "2010968") { // 나상교CJ 접수가능
		ufn_enableBtn("receipt", true);
		ufn_enableBtn("approval", false);
	} else if(G_USER_ID == "1001396") { // 조용원SM 승인가능
		ufn_enableBtn("receipt", false);
		ufn_enableBtn("approval", true);
	} else {
		ufn_enableBtn("receipt", false);
		ufn_enableBtn("approval", false);
	}

	cb_pst.Index = 0;
	cal_fdt.SetFocus();
}

/********************************************************************************
* 기      능   : 공통코드가 조회된 후의 Callback 함수 
********************************************************************************/
function fn_completeInit(sCode, sDsObj, sCodeType) {
}

/********************************************************************************
* 기      능   : form UnloadCompleted 
********************************************************************************/
function fn_OnUnloadCompleted(obj) {
	return gfn_isFormUnload(obj);
}

//=========================================================================================
// [ PART 3 ]
// Main Process Event 정의(조회,입력,삭제,저장,인쇄,form닫기)
//=========================================================================================
/********************************************************************************
* 기      능   : 조회 
********************************************************************************/
function fn_query() {
	// 필수 입력사항 및 일자 체크 
	if(!gfn_checkFromToObj(cal_fdt, cal_tdt, true, st_bdy.value)) {
		return;
	}

	// sort mark 삭제 
	gfn_clearGridSortMark(grd_list);
	ds_list.row = -1;
	grd_list.SetCellProp("head", 1, "Text", 0);

	tit_clearActionInfo();
	tit_addSearchActionInfo("cs70:CS7003001A_S1");
	tit_callService(
        ""
        , ""
        , "ds_cond=ds_cond"
        , "ds_list=ds_list"
        , ""
        , "fn_afterQuery"
        , true
        , true);
}

/********************************************************************************
* 기      능   : 조회 Callback  
********************************************************************************/
function fn_afterQuery(errCode, errMsg) {
	gfn_showMsg("CI00002", ds_list.RowCount()+"");
}

/********************************************************************************
* 기      능   : 닫기  
********************************************************************************/
function fn_close() {
	close();
}

//=========================================================================================
// [ PART 4]
// Callback 함수 정의 : callback 함수명은 메인 함수의 fn_xxxx() 에서 xxxx 부분은 fn_afterXXXX()
// 형태가 되도록 작성 
//=========================================================================================

/********************************************************************************
* 기      능   : 달력에서 일자 선택했을 경우 
********************************************************************************/
function cal_temp_OnDayClick(obj,strText) {
}

/********************************************************************************
* 기      능   : 달력에서 포커스 잃었을 경우 
********************************************************************************/
function cal_temp_OnKillFocus(obj) {
	obj.Visible = false;
}

/********************************************************************************
* 기      능   : 액셀  
********************************************************************************/
function fn_excel() {
	gfn_saveToExcel(grd_list, true);
}

/********************************************************************************
* 기      능   : 새로등록버튼 클릭시 팝업연동
********************************************************************************/
function btn_point_add_OnClick(obj) {
	var arg = "gubun='I'";
	gfn_openPopup("CS7003002A",true,arg);
	fn_query();
}

/********************************************************************************
* 기      능   : 그리드 더블 클릭시 팝업연동
********************************************************************************/
function grd_list_OnCellDblClick(obj,nRow,nCell,nX,nY,nPivotIndex) {
	var arg = "gubun='U' code='CS70' seq="+quote(ds_list.GetColumn(nRow, "SEQ"));
	gfn_openPopup("CS7003002A",true,arg);
	fn_query();
}

/********************************************************************************
* 기      능   : 그리드 헤더 클릭시 정렬 처리 
********************************************************************************/
function grd_list_OnHeadClick(obj,nCell,nX,nY,nPivotIndex) {
	if(nCell == 0) {
		gfn_selectGridCol("grd_list");
	} else if(nCell == 1) {
		gfn_selectAllGrid(obj,nCell,nCell,true);
	} else {
		gfn_sortGrid(obj,nCell);
	}
}

/********************************************************************************
* 기      능   : 접수
********************************************************************************/
function fn_receipt() {
	gfn_clearMsgBar();

	if(ds_list.rowCount() < 1) {
		// 승인할 정보 없음
		return;
	}

	if(!gfn_isChangeDs(ds_list, true)) {
		return;
	}

	if(fn_chkList("C") == false) {
		return false;
	} else {
		tit_clearActionInfo();

		tit_addSaveActionInfo(
			""
			, "cs70:CS7003001A_U2"
			, ""
			, "FLAG"
			, ""
			, -1
			, "");

		tit_callService(
			""
			, ""
			, "ds_list=ds_list:A"
			, ""
			, ""
			, "fn_afterReceipt");
	}
}

/********************************************************************************
* 기      능   : 승인
********************************************************************************/
function fn_approval() {
	gfn_clearMsgBar();

	if(ds_list.rowCount() < 1) {
		// 승인할 정보 없음
		return;
	}

	if(!gfn_isChangeDs(ds_list, true)) {
		return;
	}

	if(fn_chkList("D") == false) {
		return false;
	} else {
		tit_clearActionInfo();

		tit_addSaveActionInfo(
			""
			, "cs70:CS7003001A_U1"
			, ""
			, "FLAG"
			, ""
			, -1
			, "");

		tit_callService(
			""
			, ""
			, "ds_list=ds_list:A"
			, ""
			, ""
			, "fn_afterApproval");
	}
}

/********************************************************************************
* 기      능   : 출력
********************************************************************************/
function fn_print() {
	gfn_clearMsgBar();
	ds_list1.ClearData();

	if(ds_list.rowCount() < 1) {
		// 출력할 정보 없음
		return;
	}

	if(!gfn_isChangeDs(ds_list, true)) {
		return;
	}

	var k = 0;
	var z = 0;
	for(var i=0; i<ds_list.RowCount(); i++) {
		if(ds_list.GetColumn(i, "CHK") == true) { //체크박스가 true 일 경우
			z = i;
			k++;
		}
	}

	if(k == 0 || k > 1) {
		gfn_showMsg("견적서 출력은 한건씩 가능합니다.");
		return false;
	}

	ds_cond1.SetColumn(0, "SEQ", ds_list.GetColumn(z, "SEQ"));

	if(fn_chkList("E") == false) {
		return false;
	} else {
		tit_clearActionInfo();
		tit_addSearchActionInfo("cs70:CS7003001A_S2");
		tit_callService(
			""
			, ""
			, "ds_cond1=ds_cond1"
			, "ds_list1=ds_list1"
			, ""
			, "fn_afterPrint"
			, true
			, true);
	}
}

/********************************************************************************
* 기      능   : 접수 Callback
********************************************************************************/
function fn_afterReceipt(errCode, errMsg) {
	if(errCode != 0) {
		gfn_showMsg("CE00001");
	} else {
		gfn_showMsg("CI00003");
		fn_query();
	}
}

/********************************************************************************
* 기      능   : 승인 Callback
********************************************************************************/
function fn_afterApproval(errCode, errMsg) {
	if(errCode != 0) {
		gfn_showMsg("CE00001");
	} else {
		gfn_showMsg("CI00003");
		fn_query();
	}
}

/********************************************************************************
* 기      능   : 출력 Callback
********************************************************************************/
function fn_afterPrint(errCode, errMsg) {
	Dialog("CS70::CS7003001B.xml", "");
}

function fn_chkList(knd) {
	for(var i=0; i<ds_list.RowCount(); i++) {
		if(ds_list.GetColumn(i, "CHK") == true) { //체크박스가 true 일 경우
			if(knd == "C") { //접수
				if(ds_list.GetColumn(i, "PST") <> "A1") {
					var arr = [parseInt(i) + 1];
					gfn_showMsg("CW00042", arr);
					return false;
				}
			}

			if(knd == "D") { //승인
				if(ds_list.GetColumn(i, "PST") <> "A5") {
					var arr = [parseInt(i) + 1];
					gfn_showMsg("CW00042", arr);
					return false;
				}
			}

			if(knd == "E") { //출력
				if(ds_list.GetColumn(i, "PST") < "A5") {
					var arr = [parseInt(i) + 1];
					gfn_showMsg("CW00042", arr);
					return false;
				}
			}
		}
	}

	return true;
}

function ds_list_OnColumnChanged(obj,nRow,strColumnID,varOldValue,varNewValue,nPivotIndex) {
	obj.fireevent = false;
	switch(strColumnID) {
		case "CHK":
			if(varNewValue == '1') {
				ds_list.SetColumn(nRow,"CHK","1");
				ds_list.SetColumn(nRow,"FLAG","U");
			} else {
				ds_list.SetColumn(nRow,"CHK","0");
				ds_list.SetColumn(nRow,"FLAG","");
			}
			break;
	}
	obj.fireevent = true;
}

function btn_pjt_OnClick(obj) {
	var arg = "pjt="+quote(ed_pjt.Text)+" gbn=A";
	gfn_openPopup("CS9601002A",true,arg);
}
