#include "LIB::commons.js";	// 공통 스크립트
//=========================================================================================
// [ PART 1 ]
// Form에서 사용될 전역변수를 선언
// FORM에서 사용되는 전역변수는 대문자로 작성하며, F_XXX 의 형태를 갖도록 한다.
//=========================================================================================
var _P_GRUP = '';
var _P_ZZACTSS = '';
var _P_GBN = '';
//=========================================================================================
// [ PART 2 ]
// Form Loading / Unloading 시 작업 정의
//=========================================================================================
function fn_OnLoadCompleted(obj) {
	// Form 로드 이벤트 맨 첫줄에 기술
	gfn_initForm();

	trace('_P_GRUP : ' + _P_GRUP);
	trace('_P_INP_DT : ' + _P_INP_DT);
	trace('_P_ZZACTSS : ' + _P_ZZACTSS);
	trace('_P_GBN : ' + _P_GBN);
	
	trace('_P_GRUP2 : ' + _P_GRUP2);
	trace('_P_GUBUN : ' + _P_GUBUN);

	ds_cond.SetColumn(0, 'GRUP', _P_GRUP);
	ds_cond.SetColumn(0, 'INP_DT', _P_INP_DT);
	ds_cond.SetColumn(0, 'ZZACTSS', _P_ZZACTSS);
	ds_cond.SetColumn(0, 'GBN', _P_GBN);
	
	ds_cond.SetColumn(0, 'GRUP2', _P_GRUP2);
	ds_cond.SetColumn(0, 'GUBUN', _P_GUBUN);

	if (_P_GBN == '1') {
		div_btn.st_title.value = '지원' + this.title;
	} else {
		div_btn.st_title.value = '피지원' + this.title;
	}

	fn_query();
}
/********************************************************************************
* 기      능   : form UnloadCompleted
********************************************************************************/
function fn_OnUnloadCompleted(obj) {
	return gfn_isFormUnload(obj);
}
//=========================================================================================
// [ PART 3 ]
// Main Process Event 정의(조회,입력,삭제,저장,인쇄,form닫기)
//=========================================================================================
/********************************************************************************
* 기      능   : 조회 1
********************************************************************************/
function fn_query() {
	tit_clearActionInfo();
	
	if(_P_GBN == '1')	//지원
	{
		tit_addSearchActionInfo("ps17:PS1701001D_S1");
	}
	else	//피지원
	{
		tit_addSearchActionInfo("ps17:PS1701001D_S2");
	}
    
	tit_callService(
        ""
        , ""
        , "ds_cond=ds_cond"
        , "ds_list=ds_list"
        , ""
        , "fn_afterQuery"
        , true);
}

/********************************************************************************
* 기      능   : 닫기
********************************************************************************/
function fn_close() {
	close(null);
}

/********************************************************************************
* 기      능   : 조회 Callback
********************************************************************************/
function fn_afterQuery(errCode, errMsg) {
	// 메시지 처리
	gfn_showSearchMsg(grd_list);
}

function grd_list_OnCellDBLClick(obj,nRow,nCell,nX,nY,nPivotIndex) {
	var zzlifnr = ds_list.GetColumn(nRow, 'ZZLIFNR');
	var zzpmnum = ds_list.GetColumn(nRow, 'ZZPMNUM');
	if (Length(zzpmnum) > 0) {
		parent.ds_cond.SetColumn(0, 'ZZACTSS', _P_ZZACTSS);
		parent.ds_cond.SetColumn(0, 'ZZLIFNR', zzlifnr);
		parent.ds_cond.SetColumn(0, 'ZZPMNUM', zzpmnum);

		parent.fn_query();

		fn_close();
	}
}

function grd_list_OnCellClick(obj,nRow,nCell,nX,nY,nPivotIndex) {

}

function fn_excel()
{
	gfn_saveToExcel(grd_list, true);
}

