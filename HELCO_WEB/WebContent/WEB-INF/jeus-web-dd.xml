<?xml version="1.0" encoding="UTF-8"?>
<jeus-web-dd xmlns="http://www.tmaxsoft.com/xml/ns/jeus" version="6.0">
    <context-path>/</context-path>
    <enable-jsp>true</enable-jsp>
    <auto-reload>
        <enable-reload>false</enable-reload>
        <check-on-demand>false</check-on-demand>
    </auto-reload>
	<properties>
	           <property>
	               <key>jeus.servlet.login.manager.enable</key>
	               <value>true</value>
	           </property>
	           
	           <property>
			    <key>jeus.application.login.key</key>
			    <value>sessionkey</value>
			</property>
	</properties>        
</jeus-web-dd>
